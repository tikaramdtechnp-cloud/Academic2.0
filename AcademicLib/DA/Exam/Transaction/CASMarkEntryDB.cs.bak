        public ResponeValues SaveUpdate(int UserId,AcademicLib.RE.Exam.StudentForCASMarkEntryCollections dataColl)
        {
            ResponeValues resVal = new ResponeValues();

            dal.OpenConnection();
            System.Data.SqlClient.SqlCommand cmd = dal.GetCommand();
            cmd.CommandText = "truncate table tbl_TMP_CASMarkEntry;";
            cmd.ExecuteNonQuery();

            System.Data.DataTable dt = new System.Data.DataTable();
            dt.TableName = "tbl_TMP_CASMarkEntry";
            dt.Columns.Add(new System.Data.DataColumn("UserId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("StudentId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("EmployeeId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("SubjectId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("ExamTypeId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("CASTypeId", typeof(int)));
            dt.Columns.Add(new System.Data.DataColumn("Mark", typeof(double)));
            dt.Columns.Add(new System.Data.DataColumn("ExamDate", typeof(DateTime)));
            dt.Columns.Add(new System.Data.DataColumn("ObtainMark", typeof(double)));
            dt.Columns.Add(new System.Data.DataColumn("Remarks", typeof(string)));
            dt.Columns.Add(new System.Data.DataColumn("IsAbsent", typeof(bool)));

            foreach (var dc in dataColl)
            {
                System.Data.DataRow dr = dt.NewRow();
                dr["UserId"] = UserId;
                dr["StudentId"] = dc.StudentId;

                if(!dc.EmployeeId.HasValue || dc.EmployeeId==0)
                    dr["EmployeeId"] = DBNull.Value;
                else
                    dr["EmployeeId"] = dc.EmployeeId;

                dr["SubjectId"] = dc.SubjectId;


                if (!dc.ExamTypeId.HasValue || dc.ExamTypeId == 0)
                    dr["ExamTypeId"] = DBNull.Value;
                else
                    dr["ExamTypeId"] = dc.ExamTypeId;
                 
                dr["CASTypeId"] = dc.CASTypeId;
                dr["Mark"] = dc.Mark;
                dr["ExamDate"] = dc.ExamDate;
                dr["ObtainMark"] = dc.ObtainMark;
                dr["Remarks"] = dc.Remarks;
                dr["IsAbsent"] = dc.IsAbsent;

                dt.Rows.Add(dr);
            }
            System.Data.SqlClient.SqlBulkCopy objbulk = new System.Data.SqlClient.SqlBulkCopy(dal.Connection, System.Data.SqlClient.SqlBulkCopyOptions.Default, dal.Transaction);
            objbulk.DestinationTableName = "tbl_TMP_CASMarkEntry";
            foreach (System.Data.DataColumn column in dt.Columns)
            {
                objbulk.ColumnMappings.Add(column.ToString(), column.ToString());
            }
             
            objbulk.WriteToServer(dt, System.Data.DataRowState.Added);
             
            cmd.CommandType = System.Data.CommandType.StoredProcedure;
            cmd.Parameters.AddWithValue("@UserId", UserId);
            cmd.CommandText = "usp_UpdateCASMarkEntry";

            try
            {
                cmd.ExecuteNonQuery();
                resVal.IsSuccess = true;
                resVal.ResponseMSG ="MarkEntry "+ GLOBALMSG.SUCCESS;
            }
            catch (System.Data.SqlClient.SqlException ee)
            {
                resVal.IsSuccess = false;
                resVal.ResponseMSG = ee.Message;
            }
            catch (Exception ee)
            {
                resVal.IsSuccess = false;
                resVal.ResponseMSG = ee.Message;
            }
            finally
            {
                dal.CloseConnection();
            }
            return resVal;
        }

