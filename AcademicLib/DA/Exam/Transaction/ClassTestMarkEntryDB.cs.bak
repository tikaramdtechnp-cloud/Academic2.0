        public List<ResponeValues> SaveUpdate(int UserId, BE.Exam.Transaction.ClassTestMarkEntryCollections dataColl)
        {
            List<ResponeValues> responseList = new List<ResponeValues>();
            dal.OpenConnection();
            System.Data.SqlClient.SqlCommand cmd = dal.GetCommand();
            cmd.CommandType = CommandType.StoredProcedure;

            try
            {
                foreach (var beData in dataColl)
                {
                    cmd.Parameters.Clear();
                    cmd.Parameters.AddWithValue("@AcademicYearId", beData.AcademicYearId);
                    cmd.Parameters.AddWithValue("@ClassId", beData.ClassId);
                    cmd.Parameters.AddWithValue("@SectionId", beData.SectionId);
                    cmd.Parameters.AddWithValue("@EmployeeId", beData.EmployeeId);
                    cmd.Parameters.AddWithValue("@SubjectId", beData.SubjectId);
                    cmd.Parameters.AddWithValue("@LessonId", beData.LessonId);
                    cmd.Parameters.AddWithValue("@StudentId", beData.StudentId);
                    cmd.Parameters.AddWithValue("@BatchId", beData.BatchId);
                    cmd.Parameters.AddWithValue("@SemesterId", beData.SemesterId);
                    cmd.Parameters.AddWithValue("@ClassYearId", beData.ClassYearId);
                    cmd.Parameters.AddWithValue("@TestDate", beData.TestDate);
                    cmd.Parameters.AddWithValue("@FullMarks", beData.FullMarks);
                    cmd.Parameters.AddWithValue("@PassMarks", beData.PassMarks);
                    cmd.Parameters.AddWithValue("@ObtMarks", beData.ObtMarks);
                    cmd.Parameters.AddWithValue("@Remarks", beData.Remarks);
                    cmd.Parameters.AddWithValue("@CreateBy", UserId);
                    cmd.Parameters.AddWithValue("@ModifyBy", null);
                    cmd.Parameters.AddWithValue("@TranId", 0).Direction = ParameterDirection.Output;
                    cmd.Parameters.AddWithValue("@ResponseMSG", string.Empty).Direction = ParameterDirection.Output; 
                    cmd.Parameters.AddWithValue("@IsSuccess", false).Direction = ParameterDirection.Output;
                    cmd.Parameters.AddWithValue("@ErrorNumber", 0).Direction = ParameterDirection.Output;

                    cmd.CommandText = "usp_AddClassTestMarkEntry";
                    cmd.ExecuteNonQuery();

                    ResponeValues resVal = new ResponeValues();
                    resVal.RId = cmd.Parameters["@TranId"].Value == DBNull.Value ? 0 : Convert.ToInt32(cmd.Parameters["@TranId"].Value);
                    resVal.ResponseMSG = cmd.Parameters["@ResponseMSG"].Value == DBNull.Value ? string.Empty : Convert.ToString(cmd.Parameters["@ResponseMSG"].Value);
                    resVal.IsSuccess = cmd.Parameters["@IsSuccess"].Value == DBNull.Value ? false : Convert.ToBoolean(cmd.Parameters["@IsSuccess"].Value);
                    resVal.ErrorNumber = cmd.Parameters["@ErrorNumber"].Value == DBNull.Value ? 0 : Convert.ToInt32(cmd.Parameters["@ErrorNumber"].Value);

                    if (!resVal.IsSuccess && resVal.ErrorNumber > 0)
                        resVal.ResponseMSG += $"({resVal.ErrorNumber})";
                    else if (resVal.IsSuccess && resVal.ResponseMSG.Contains("Saved Successfully"))
                        resVal.ResponseMSG = "ClassTest MarkEntry successfully";

                    responseList.Add(resVal);
                }
            }
            catch (System.Data.SqlClient.SqlException ee)
            {
                dal.RollbackTransaction();
                ResponeValues resVal = new ResponeValues();
                resVal.IsSuccess = false;
                resVal.ResponseMSG = ee.Message;
                responseList.Add(resVal);
            }
            catch (Exception ee)
            {
                ResponeValues resVal = new ResponeValues();
                resVal.IsSuccess = false;
                resVal.ResponseMSG = ee.Message;
                responseList.Add(resVal);
            }
            finally
            {
                dal.CloseConnection();
            }

            return responseList;
        }



        public ResponeValues DeleteById(int UserId, int EntityId, int TranId)
{
	ResponeValues resVal = new ResponeValues();
	dal.OpenConnection();
	System.Data.SqlClient.SqlCommand cmd = dal.GetCommand();
	cmd.CommandType = System.Data.CommandType.StoredProcedure;
	cmd.Parameters.AddWithValue("@UserId", UserId);
	cmd.Parameters.AddWithValue("@EntityId", EntityId);
	cmd.Parameters.AddWithValue("@TranId", TranId);
	cmd.CommandText = "usp_DelClassTestMarkEntryById";
	cmd.Parameters.Add("@ResponseMSG", System.Data.SqlDbType.NVarChar, 254);
	cmd.Parameters.Add("@IsSuccess", System.Data.SqlDbType.Bit);
	cmd.Parameters.Add("@ErrorNumber", System.Data.SqlDbType.Int);
	cmd.Parameters[3].Direction = System.Data.ParameterDirection.Output;
	cmd.Parameters[4].Direction = System.Data.ParameterDirection.Output;
	cmd.Parameters[5].Direction = System.Data.ParameterDirection.Output;
	try
	{
		cmd.ExecuteNonQuery();

		if (!(cmd.Parameters[3].Value is DBNull))
			resVal.ResponseMSG = Convert.ToString(cmd.Parameters[3].Value);

		if (!(cmd.Parameters[4].Value is DBNull))
			resVal.IsSuccess = Convert.ToBoolean(cmd.Parameters[4].Value);

		if (!(cmd.Parameters[5].Value is DBNull))
			resVal.ErrorNumber = Convert.ToInt32(cmd.Parameters[5].Value);

		if (!resVal.IsSuccess && resVal.ErrorNumber > 0)
			resVal.ResponseMSG = resVal.ResponseMSG + "(" + resVal.ErrorNumber.ToString() + ")";

	}
	catch (System.Data.SqlClient.SqlException ee)
	{
		resVal.IsSuccess = false;
		resVal.ResponseMSG = ee.Message;
	}
	catch (Exception ee)
	{
		resVal.IsSuccess = false;
		resVal.ResponseMSG = ee.Message;
	}
	finally
	{
		dal.CloseConnection();
	}
	return resVal;
}
