                                            </tr>
                                        </tbody>
                                    </table>*@

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- New Hosting Class Card ends -->

                                                    <div class="card card-default">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Today's Missed Class List</h3>
                                                        </div>
                                                        <div class="card-body">
                                                            <div class="table-scroll-x-y sales-main-table table-scroll">
                                                                <table id="main-table" class="main-table table table-sm table-bordered bg-white">
                                                                    <thead>
                                                                        <tr>
                                                                            <th style="width: 60px">S.No</th>
                                                                            <th style="width: 90px">Class/Sec</th>
                                                                            <th style="width: 150px">Subject</th>
                                                                            <th style="width: 140px">Class Time</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="Missed in MissedClassesList">
                                                                            <td>{{$index+1}}</td>
                                                                            <td>{{Missed.ClassName}}-{{Missed.SectionName}}</td>
                                                                            <td>{{Missed.SubjectName}}</td>
                                                                            <td>{{Missed.StartTime}}-{{Missed.EndTime}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>


                                            </div>
                                        </div>
                                        <!-- Show Attendance of completed classes Section Starts -->
                                        <div id="attendance-completed-classes">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <label><b>Class/Sec : {{SelectedData.ClassName}}/{{SelectedData.SectionName}}</b></label>
                                                </div>
                                                <div class="col-md-3">
                                                    <label><b>Subject : {{SelectedData.SubjectName}}</b></label>
                                                </div>
                                                <div class="col-md-3">
                                                    <label><b>{{SelectedData.StartDateTime_AD| date:'HH:mm:ss'}}-{{SelectedData.EndDateTime_AD| date:'HH:mm:ss'}}={{SelectedData.Duration}}</b></label>
                                                </div>
                                                <div class="col-md-3">
                                                    <label><b>Total Student : {{SelectedData.NoOfStudent}}</b></label>
                                                </div>
                                            </div>
                                            <div class="card card-primary card-tabs mt-2">
                                                <div class="card-header with-gradient-bg p-0 pt-2">
                                                    <ul class="nav nav-tabs" id="custom-tabs-one-tab" role="tablist">
                                                        <li class="nav-item">
                                                            <a class="nav-link active" id="custom-tabs-one-home-tab"
                                                               data-toggle="pill" href="#custom-tabs-one-home" role="tab"
                                                               aria-controls="custom-tabs-one-home" aria-selected="true" style="padding-left: 3rem;
                                padding-right: 3rem;">Present({{SelectedData.NoOfPresent}})</a>
                                                        </li>
                                                        <li class="nav-item">
