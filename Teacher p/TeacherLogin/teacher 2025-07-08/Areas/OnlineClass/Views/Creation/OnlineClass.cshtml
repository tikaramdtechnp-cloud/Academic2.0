
@{
    ViewBag.Title = "OnlineClass";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/AngularJs/Creation/OnlineClass.js?dt=@(DateTime.Now.Ticks)"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="OnlineClassController" ng-init="LoadData()">
    <div class="content-wrapper">
        <section class="content">
            <div class="container-fluid">
                <section class="iframe-content-starts-here">
                    <div class="tab-content">
                        <div id="root">
                            <div class="card-primary card-tabs">
                                <header class="if-page-have-nav-tabs" style="top: 56px">
                                    <div class="row upper-tab">
                                        <div class="col-md-12">
                                            <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active"
                                                id="myTab"
                                                role="tablist">

                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="profile-tab"
                                                       data-toggle="tab"
                                                       href="#profile"
                                                       role="tab"
                                                       aria-controls="profile"
                                                       aria-selected="false">Host Class</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="contact-tab"
                                                       data-toggle="tab"
                                                       href="#contact"
                                                       role="tab"
                                                       aria-controls="contact"
                                                       aria-selected="false">Past Classes</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="SelfAttendence-tab"
                                                       data-toggle="tab"
                                                       href="#SelfAttendence"
                                                       role="tab"
                                                       aria-controls="SelfAttendence"
                                                       aria-selected="false">Self Attendence(Online Class)</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="class-tab"
                                                       data-toggle="tab"
                                                       href="#class"
                                                       role="tab"
                                                       aria-controls="contact"
                                                       aria-selected="false">Class Attendence</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="subject-tab"
                                                       data-toggle="tab"
                                                       href="#subject"
                                                       role="tab"
                                                       aria-controls="contact"
                                                       aria-selected="false">Subject Attendence</a>
                                                </li>
                                            </ul>
                                        </div>

                                    </div>
                                </header>
                                <div class="tab-content" id="myTabContent">
                                    <!-- Start class tab opens                    -->
                                    <div class="tab-pane fade main-section active show"
                                         id="profile"
                                         role="tabpanel"
                                         aria-labelledby="profile-tab">
                                        <div id="setup-form">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="row">
                                                        <div class="col-md-4 col-sm-4">
                                                            <img src="~/teacherloginFiles/dynamic/images/zoom.png" alt="" style="width: 40px;"><br>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" ng-value="1" type="radio" ng-model="newOnlineClass.PlatformType" ng-change="OnChangeplatformtype(1)" name="platformtype" />
                                                                <label class="form-check-label" for="zoom">Zoom</label>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4 col-sm-4">
                                                            <img src="~/teacherloginFiles/dynamic/images/meet.png" alt="" style="width: 40px;"><br>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio" ng-value="2" ng-model="newOnlineClass.PlatformType" ng-change="OnChangeplatformtype(2)" name="platformtype" />
                                                                <label class="form-check-label" for="google">Google</label>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4 col-sm-4">
                                                            <img src="~/teacherloginFiles/dynamic/images/team.png" alt="" style="width: 40px;"><br>
                                                            <div class="form-check form-check-inline">
                                                                <input class="form-check-input" type="radio" ng-value="3" ng-model="newOnlineClass.PlatformType" ng-change="OnChangeplatformtype(3)" name="platformtype" />
                                                                <label class="form-check-label" for="microsoft">Microsoft</label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>


                                            <div class="row mt-4">
                                                <div class="col-md-4">

                                                    <div class="card p-2">

                                                        <div class="form-group">
                                                            <label for="UserName">Username</label>
                                                            <input type="text" id="UserName" class="form-control form-control-sm ng-pristine ng-untouched ng-valid ng-empty" autocomplete="off" ng-model="newOnlineClass.UserName">
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="Pwd">Password</label>
                                                            <input type="password" id="Pwd" class="form-control form-control-sm ng-pristine ng-untouched ng-valid ng-empty" autocomplete="off" ng-model="newOnlineClass.Pwd">
                                                        </div>

                                                        <div class="form-group">
                                                            <label for="">Meeting URL</label>
                                                            <input type="text"
                                                                   class="form-control form-control-sm" ng-model="newOnlineClass.Link" />
                                                        </div>

                                                        <div class="time-label">
                                                            <span class="d-block border-0 p-3 text-center">

                                                                <button type="button" ng-click="SaveUpdateOnlineClass()" class="btn btn-sm btn-success" style="min-width: 90px;">Save</button>
                                                                <button type="button" id="next-btn" class="btn btn-sm btn-success" style="min-width: 90px;">Next</button>
                                                                <label class="mt-2">
                                                                    <strong>Don't have account? &nbsp; <a ng-href="{{newOnlineClass.Register}}" class="text-success" target="_blank">Register Now</a></strong>
                                                                </label>
                                                            </span>
                                                        </div>
                                                        @*<div class="text-center">
                                <label for="">Don't have an account? <a href="#">Register Now</a></label>
                            </div>*@
                                                    </div>

                                                </div>

                                            </div>
                                        </div>
                                        <div id="area-after-platform-setup">
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <!-- SELECT2 EXAMPLE -->
                                                    <div class="card card-default">
                                                        <div class="card-header">
                                                            <div class="row">
                                                                <div class="col-md-5">
                                                                    <h3 class="card-title">Create Class</h3>
                                                                </div>
                                                                <div class="col-md-7">
                                                                    <div class="custom-control custom-checkbox float-right">
                                                                        <input class="custom-control-input" type="checkbox" id="customCheckbox1" ng-model="GroupWiseHost" ng-click="Check()">
                                                                        <label for="customCheckbox1" class="custom-control-label">
                                                                            GroupWise Class
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- /.card-header -->
                                                        <div class="card-body">
                                                            <div class="row" ng-hide="GroupWiseHost==true">
                                                                <div class="col-md-12">
                                                                    <div class="form-group">
                                                                        <label>Class Shift</label>
                                                                        <select class="form-control form-control-sm" ng-model="createClass.ShiftId" ng-change="OnShiftChange()">
                                                                            <option ng-value="0" selected>** select shift **</option>
                                                                            <option ng-repeat="ct in ClassShiftColl  track by $index" ng-value="{{ct.ShiftId}}">
                                                                                {{ct.Name}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Select Class</label>
                                                                        <select class="form-control form-control-sm" ng-model="createClass.ClassId" ng-change="OnClassChange()">
                                                                            <option ng-value="0" selected>** select class **</option>
                                                                            <option ng-repeat="cls in ClassColl  track by $index" ng-value="{{ cls.ClassId }}">
                                                                                {{cls.Name}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <!-- /.form-group -->
                                                                    <div class="form-group">
                                                                        <label>Select Section</label>
                                                                        <select class="form-control form-control-sm select2" ng-model="createClass.SectionId" multiple ng-change="GetSubjectList()">
                                                                            <option ng-repeat="ct in SectionColl track by $index" ng-value="{{ct.SectionId}}">
                                                                                {{ct.SectionName}}
                                                                            </option>
                                                                        </select>

                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Select Subject</label>
                                                                        <select class="form-control form-control-sm" id="cboSubjectId" ng-model="createClass.SubjectId">
                                                                            <option ng-repeat="ct in SubjectColl track by $index" value="{{ ct.SubjectId }}">
                                                                                {{ct.Name}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Notes</label>
                                                                        <textarea class="form-control form-control-sm" ng-model="createClass.Notes"
                                                                                  rows="2"
                                                                                  placeholder=""></textarea>
                                                                    </div>
                                                                    <!-- /.form-group -->
                                                                    <div class="card-footer">
                                                                        <button type="button" class="btn btn-secondary btn-sm mb-1" ng-click="StartOnlineClass()">Host</button>
                                                                        <button type="button" class="btn btn-secondary btn-sm mb-1" id="back-to-platform">Back</button>

                                                                    </div>
                                                                </div>

                                                            </div>
                                                            <div id="div2" ng-show="GroupWiseHost==true">
                                                                <div class="form-group">
                                                                    <label for="">Select Group</label>
                                                                    <select class="form-control form-control-sm select2" ng-model="createClass.SelectedClassGroup">
                                                                        <option ng-value="cg" ng-repeat="cg in ClassGroupList">{{cg.Name}}</option>
                                                                    </select>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label for="">Subject</label>
                                                                    <input type="text" class="form-control form-control-sm" ng-model="createClass.SelectedClassGroup.SubjectName" readonly />
                                                                </div>
                                                                <div class="form-group">
                                                                    <label for="">Class</label>
                                                                    <input type="text" class="form-control form-control-sm" ng-model="createClass.SelectedClassGroup.ForClass" readonly />
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Notes</label>
                                                                    <textarea class="form-control form-control-sm" ng-model="createClass.Notes"
                                                                              rows="2"
                                                                              placeholder=""></textarea>
                                                                </div>
                                                                <div class="card-footer">
                                                                    <button type="button" class="btn btn-secondary btn-sm mb-1" ng-click="StartOnlineClassGroup()">Host</button>
                                                                    <button type="button" class="btn btn-secondary btn-sm mb-1" id="back-to-platform1">Back</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <!-- /.card-body -->
                                                    </div>
                                                    <!-- /.card -->

                                                </div>
                                                <div class="col-md-9">
                                                    <div class="card card-default">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Today's Upcoming Classes</h3>
                                                        </div>

                                                        <!-- /.card-header -->
                                                        <div class="card-body">
                                                            <div class="row" style="color: aliceblue; overflow-x: auto; white-space: nowrap;">
                                                                <div class="col-md-12">
                                                                    <div class="col-md-4 " ng-repeat="cs in ClassScheduleCollList" style="display: inline-block; float: none;">
                                                                        <div class="card mb-1" ng-click="GetHostDet(cs)" ng-style="{'background-color': colors[$index]}" style=" cursor:pointer">
                                                                            <div class="card-body" style="padding: 5px; height:140px">
                                                                                <div class="row">
                                                                                    <div class="col-md-7 col-sm-7">
                                                                                        <p>Class:-{{cs.ClassName}}-{{cs.SectionName}}</p>

                                                                                    </div>
                                                                                    <div class="col-md-5 col-sm-5">
                                                                                        <p class="float-right"><i class="fas fa-home icon-bg-green"></i></p>
                                                                                        <p class="text-break" style="font-size: 18px;  margin-top:38px; overflow-wrap: break-word"></p>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-md-12 col-sm-12">
                                                                                        <p style=" font-size: 18px;white-space: normal; overflow-wrap: break-word;">{{cs.SubjectName}}</p>
                                                                                    </div>

                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-md-12 col-sm-12">
                                                                                        <label style="font-size: 14px;">{{cs.StartTime}}-{{cs.EndTime}}</label>
                                                                                    </div>

                                                                                </div>
                                                                            </div>
                                                                        </div>


                                                                    </div>
                                                                </div>
                                                                <!-- /.card-body -->
                                                            </div>

                                                            <!-- Its time to host card starts -->
                                                            <div class="card card-default mt-3">
                                                                <div class="card-header" style="background-color: #23a2b8;color: aliceblue;">
                                                                    <h3 class="card-title"><i class="fas fa-star" style="color: #f9910c;"></i> It's time to host class now</h3>
                                                                </div>
                                                                <div class="card-body">
                                                                    <div class="" style="border: 2px solid #76be4e;padding: 8px;border-radius: 4px;">
                                                                        <div class="row">
                                                                            <div class="col-md-2">
                                                                                <label>Class : {{HostClass.ClassName}}</label>
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <label>Section :{{HostClass.SectionName}}</label>
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <label>Subject: {{HostClass.SubjectName}} </label>
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <label>Time Duration: {{HostClass.StartTime}}-{{HostClass.EndTime}}</label>
                                                                            </div>

                                                                            <div class="col-md-9 mt-3">
                                                                                <div class="form-group">
                                                                                    <label>Notes</label>
                                                                                    <textarea class="form-control form-control-sm" ng-model="createClass.Notes"
                                                                                              rows="1"
                                                                                              placeholder=""></textarea>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-3 mt-4 pt-3">
                                                                                <button type="button" class="btn btn-sm btn-success" ng-click="StartOnlineClass()" style="min-width: 90px">
                                                                                    Host
                                                                                </button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <!-- It's Time to host card ends -->

                                                        </div>

                                                    </div>
                                                </div>

                                            </div>
                                            <div class="row">
                                                <div class="col-md-3" ng-show="ColleaguesClassesColl.length!=0">
                                                    <!-- SELECT2 EXAMPLE -->
                                                    <div class="card card-default">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Colleagues Running Class</h3>
                                                        </div>
                                                        <!-- /.card-header -->
                                                        <div class="card-body">
                                                            <div class="row">
                                                                <div class="col-md-6">
                                                                    <ul class="list-group list-group-unbordered">

                                                                        <li class="list-group-item" ng-repeat="cc in ColleaguesClassesColl">
                                                                            <label class="pull-left" style="width:200px;">{{cc.TeacherName}}</label> <a class="pull-right">{{cc.StartDateTime_AD |timeFormat}}</a>

                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <ul class="list-group list-group-unbordered">

                                                                        <li class="list-group-item" ng-repeat="cc in ColleaguesClassesColl">
                                                                            <label class="pull-left" style="width:200px;">Class {{cc.ClassName}}</label> <a class="pull-right">{{cc.SubjectName}}</a>
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                            </div>
                                                            <!-- /.row -->
                                                        </div>
                                                        <!-- /.card-body -->
                                                    </div>
                                                    <!-- /.card -->

                                                </div>



                                                <div ng-class="getCollval()" style="margin-top:-10px">
                                                    <!-- Running Class card starts -->
                                                    <div class="card card-default" ng-show="RunningClassesColl.length!=0">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Running Class</h3>
                                                        </div>

                                                        <div class="card-body table-responsive p-2">
                                                            <table class="table table-head-fixed text-nowrap table-bordered">
                                                                <thead>
                                                                    <tr class="text-center">
                                                                        <th style="width:100px">Class</th>
                                                                        <th style="width:100px">Section</th>
                                                                        <th style="width:150px">Subject</th>
                                                                        <th style="width:250px">Notes</th>
                                                                        <th style="width:80px">Start Time</th>
                                                                        <th style="width:100px">Duration</th>
                                                                        <th style="width:100px">Action</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="rs in RunningClassesColl">
                                                                        <td class="align-middle text-left pl-1">{{rs.ClassName}}</td>
                                                                        <td class="align-middle text-left pl-1">{{rs.SectionName}}</td>
                                                                        <td class="align-middle text-left pl-1">{{rs.SubjectName}}</td>
                                                                        <td class="align-middle text-left pl-1">{{rs.Notes}}</td>
                                                                        <td class="align-middle text-left pl-1">{{rs.StartDateTime_AD| date : "h:mm a" }}</td>
                                                                        <td></td>
                                                                        <td class="p-1">
                                                                            <button class="btn btn-sm btn-secondary" style="min-width:90px" ng-click="EndOnlineClass(rs)">End Class</button>
                                                                        </td>

                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>

                                                    <div class="card card-default">
                                                        <div class="card-header">
                                                            <h3 class="card-title">Today's Completed Class List</h3>
                                                        </div>

                                                        <div class="card-body">
                                                            <div class="card-body table-responsive p-0" style="height: 300px;">
                                                                <table id="main-table" class="main-table table table-sm table-bordered bg-white">
                                                                    <thead>
                                                                        <tr>
                                                                            <th style="width: 60px">S.No</th>
                                                                            <th style="width: 90px">Class/Sec</th>
                                                                            <th style="width: 150px">Subject</th>
                                                                            <th style="width: 170px">Class Time</th>
                                                                            <th style="width: 110px">No.of Students</th>
                                                                            <th style="width: 70px">Present</th>
                                                                            <th style="width: 70px">Absent</th>
                                                                            <th style="width: 30px">Action</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="cl in PassClassesList">
                                                                            <td>{{$index+1}}</td>
                                                                            <td>{{cl.ClassName}}/{{cl.SectionName}}</td>
                                                                            <td>{{cl.SubjectName}}</td>
                                                                            <td>{{cl.StartDateTime_AD| date:'HH:mm:ss'}}-{{cl.EndDateTime_AD| date:'HH:mm:ss'}}={{cl.Duration}}</td>
                                                                            <td>{{cl.NoOfStudent}}</td>
                                                                            <td>{{cl.NoOfPresent}}</td>
                                                                            <td>{{cl.absentNo}}</td>
                                                                            <td><button class="btn btn-sm btn-info py-0 px-1" ng-click="TodaysAttendence(cl)" @*id="todays-attendance"*@>View</button></td>
                                                                        </tr>
                                                                    </tbody>
                                                                    <tfoot>
                                                                        <tr>
                                                                            <td></td>
                                                                            <td colspan="3" style="text-align:right">TOTAL </td>
                                                                            <td>{{ PassClassesList | sumByColumn: 'NoOfStudent' }}</td>
                                                                            <td>{{ PassClassesList | sumByColumn: 'NoOfPresent' }}</td>
                                                                            <td>{{ PassClassesList | sumByColumn: 'absentNo' }}</td>
                                                                            <td></td>
                                                                        </tr>
                                                                    </tfoot>
                                                                </table>

                                                                @*<table class="table table-head-fixed text-nowrap table-bordered">
                                        <thead class="text-center">
                                            <tr>
                                                <th style="width:50px">S.NO. sddsdsdsddds</th>
                                                <th style="width:100px">Class/Sec</th>
                                                <th style="width:80px">Total Students</th>
                                                <th style="width:50px">Present</th>
                                                <th style="width:50px">Absent</th>
                                                <th style="width:50px">Late</th>
                                                <th style="width:50px"> </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="cl in PassClassesList" >
                                                <td class="align-middle">{{$index+1}}</td>
                                                <td class="align-middle">{{cl.ClassName}}/{{cl.SectionName}}</td>
                                                <td class="align-middle">{{cl.NoOfStudent}}</td>
                                                <td class="align-middle">{{cl.NoOfPresent}}</td>
                                                <td class="align-middle">5</td>
