
@{
    ViewBag.Title = "StudentRecord";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/AngularJs/Creation/StudentRecords.js?dt=@(DateTime.Now.Ticks)"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="StudentRecordController" ng-init="LoadData()">
    <div class="content-wrapper">
        <!-- Main content -->
        <section class="content">
            <div class="container-fluid">
                <section class="iframe-content-starts-here">
                    <div class="tab-content">
                        <!-- Third Tab Content -->
                        <div id="root">
                            <div class="card-primary card-tabs">
                                <header class="if-page-have-nav-tabs">
                                    <div class="row upper-tab">
                                        <div class="col-md-12">
                                            <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active"
                                                id="myTab"
                                                role="tablist">
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link active"
                                                       id="home-tab"
                                                       data-toggle="tab"
                                                       href="#home"
                                                       role="tab"
                                                       aria-controls="home"
                                                       aria-selected="true">Record</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="profile-tab"
                                                       data-toggle="tab"
                                                       href="#profile"
                                                       role="tab"
                                                       aria-controls="profile"
                                                       aria-selected="false">Student Remarks</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </header>
                                <div class="tab-content" id="myTabContent">
                                    <!-- Record Start -->
                                    <div class="tab-pane fade show active main-section"
                                         id="home"
                                         role="tabpanel"
                                         aria-labelledby="home-tab">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label>Class</label>
                                                    <select class="form-control form-control-sm select2"  ng-model="StudentRecord.ClassId" ng-change="GetStudentRecord()">
                                                        <option ng-repeat="ct in ClassColl  track by $index" ng-value="{{ct.ClassId}}" ng-selected="ct.ClassId==V">
                                                            {{ct.Name}}
                                                        </option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label>Section</label>
                                                    <select class="form-control form-control-sm select2"  multiple ng-model="StudentRecord.SectionIdColl" ng-change="GetStudentRecord()">
                                                        <option ng-value="se.SectionId" ng-repeat="se in SectionClassColl |  filter: { ClassId: StudentRecord.ClassId }:true">{{se.SectionName}}</option>                                                        
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-md-3 mt-4">
                                                <div class="form-group">
                                                    <label>Total Students :{{StudentRecordList.length}} </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card p-2">
                                            <header class="row mb-2">
                                                <div class="col-md-7">
                                                    <div class="clearfix">
                                                        <form class="form-inline">
                                                            <div class="input-group input-group-sm mr-sm-2">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text cstm">Show</span>
                                                                </div>
                                                                <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.Record">
                                                                    <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.Record">{{pp.text}}</option>
                                                                </select>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text bg-white"><small>entries</small></span>
                                                                </div>
                                                            </div>
                                                            <div class="input-group input-group-sm table-search-filed">
                                                                <input class="form-control form-control-navbar"
                                                                       type="search"
                                                                       placeholder="Search"
                                                                       aria-label="Search" ng-model="searchData.Record" />
                                                                <div class="input-group-append">
                                                                    <button class="btn btn-default border-left-0"
                                                                            type="submit">
                                                                        <i class="fas fa-search"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                                <!-- end col-md-8 -->
                                                <div class="col-md-5 text-right">
                                                    <div class="dropdown dnld-file"
                                                         data-toggle="tooltip"
                                                         data-placement="top"
                                                         title="Download File">
                                                        <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                                                type="button"
                                                                id="dropdownMenuButton"
                                                                data-toggle="dropdown"
                                                                aria-haspopup="true"
                                                                aria-expanded="false">
                                                            <i class="fas fa-download"></i>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-right"
                                                             aria-labelledby="dropdownMenuButton">
                                                            <span class="dropdown-item">Choose an option:</span>
                                                            <a class="dropdown-item" ng-click="exportToExcel('#StudentRecord')"><img src="~/teacherloginFiles/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                            <a class="dropdown-item" ng-click="ExportCSV('#StudentRecord')"><img src="~/teacherloginFiles/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                            <a class="dropdown-item" ng-click="Exportpdf('StudentRecord')"><img src="~/teacherloginFiles/dynamic/images/flaticons/pdf.png" class="img-fluid exports-icon" alt="Excel image"> PDF</a>
                                                            <a class="dropdown-item" ng-click="ExportWord('#StudentRecord')"><img src="~/teacherloginFiles/dynamic/images/flaticons/word.png" class="img-fluid exports-icon" alt="Excel image"> Word</a>

                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/gmail.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Mail
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <a href="#"
                                                       class="btn btn-secondary btn-light btn-sm"
                                                       data-toggle="tooltip"
                                                       data-placement="bottom"
                                                       title="Print">
                                                        <i class="fas fa-print"></i>
                                                    </a>

                                                    <a class="btn btn-success btn-sm" ng-show="(StudentRecordList|filter:{Full:true}).length>0" data-toggle="modal" data-target="#modal-xl" style="color:#fff;">
                                                        <i class="fas fa-bell w-t">
                                                        </i>
                                                        Send Notice
                                                    </a>
                                                    <a class="btn btn-success btn-sm" ng-show="(StudentRecordList|filter:{Full:true}).length>0" data-toggle="modal" data-target="#modal-xl2" style="color:#fff;">
                                                        <i class="fas fa-bell w-t">
                                                        </i>
                                                        Remarks
                                                    </a>
                                                </div>
                                            </header>
                                            <div class="table-scroll-x-y sales-main-table table-scroll" style="height: calc(100vh - 22rem) ;overflow-x:auto">
                                                <table id="StudentRecord"
                                                       class="main-table table table-sm table-bordered bg-white table-head-fixed">
                                                    <thead>
                                                        <tr>
                                                            <th style="width: 50px;">
                                                                <div class="custom-control custom-checkbox">
                                                                    <input type="checkbox" class="custom-control-input" id="customCheckbox3" ng-model="chkFull" ng-change="checkFull()" @*onclick="toggle(this);"*@ />
                                                                    <label for="customCheckbox3"
                                                                           class="custom-control-label">All </label>
                                                                </div>
                                                            </th>
                                                            <th class="align-middle" style="width: 50px">ID</th>
                                                            <th class="align-middle" style="width: 150px">Name</th>
                                                            <th class="align-middle" style="width: 80px">Roll No.</th>
                                                            <th class="align-middle" style="width: 55px">Gender</th>
                                                            <th class="align-middle" style="width: 90px">Class/Sec</th>
                                                            <th class="align-middle" style="width: 100px">DOB</th>
                                                            <th class="align-middle" style="width: 150px">Father Name</th>
                                                            <th class="align-middle" style="width: 100px">FATHER'S CONTACT NO</th>
                                                            <th class="align-middle" style="width: 100px">Address</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr @*ng-repeat="rl in StudentRecordList"*@ dir-paginate="rl in StudentRecordList | filter:searchData.Record | itemsPerPage: perPage.Record" current-page="currentPages.Record" pagination-id="AddStudentRecordPagination">
                                                            <td class="text-center">
                                                                <div class="custom-control custom-checkbox">
                                                                    <input class="custom-control-input" type="checkbox" ng-value="1" @*ng-change="Checkedbox()"*@
                                                                           id="customCheckbox2{{$index+1}}" ng-model="rl.Full">
                                                                    <label for="customCheckbox2{{$index+1}}"
                                                                           class="custom-control-label"></label>
                                                                </div>
                                                            </td>
                                                            <td>{{$index+1}}</td>
                                                            <td class="text-left pl-1">{{rl.Name}}</td>
                                                            <td>{{rl.RollNo}}</td>
                                                            <td class="text-left pl-1">{{rl.Gender}}</td>
                                                            <td class="text-left pl-1">{{rl.ClassName}}/{{rl.SectionName}}</td>
                                                            <td>{{rl.DOB_BS}}</td>
                                                            <td class="text-left pl-1">{{rl.FatherName}}</td>
                                                            <td>{{rl.ContactNo}}</td>
                                                            <td class="text-left pl-1">{{rl.Address}}</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                            <footer class="row align-items-center">
                                                <div class="col-md-12">
                                                    <dir-pagination-controls pagination-id="AddStudentRecordPagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/Scripts/dirPagination.tpl.html">
                                                    </dir-pagination-controls>
                                                </div>
                                            </footer>

                                        </div>
                                    </div>
                                    <!-- Record Ends -->
                                    <!-- Student Remarks Start -->
                                    <div class="tab-pane fade main-section"
                                         id="profile"
                                         role="tabpanel"
                                         aria-labelledby="profile-tab">
                                        <div id="remarks-list">
                                            <div class="card p-2">
                                                <header class="row mb-2">
                                                    <div class="col-md-7">
                                                        <div class="clearfix">
                                                            <form class="form-inline">
                                                                <div class="input-group input-group-sm mr-sm-2">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text cstm">Show</span>
                                                                    </div>
                                                                    <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                                                    <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.StudentRemarks">
                                                                        <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.StudentRemarks">{{pp.text}}</option>
                                                                    </select>
                                                                    <div class="input-group-append">
                                                                        <span class="input-group-text bg-white"><small>entries</small></span>
                                                                    </div>
                                                                </div>
                                                                <div class="input-group input-group-sm table-search-filed">
                                                                    <input class="form-control form-control-navbar"
                                                                           type="search"
                                                                           placeholder="Search"
                                                                           aria-label="Search" />
                                                                    <div class="input-group-append">
                                                                        <button class="btn btn-default border-left-0"
                                                                                type="submit">
                                                                            <i class="fas fa-search"></i>
                                                                        </button>
                                                                    </div>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                    <!-- end col-md-8 -->
                                                    <div class="col-md-5 text-right">
                                                        <div class="dropdown dnld-file"
                                                             data-toggle="tooltip"
                                                             data-placement="top"
                                                             title="Download File">
                                                            <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                                                    type="button"
                                                                    id="dropdownMenuButton"
                                                                    data-toggle="dropdown"
                                                                    aria-haspopup="true"
                                                                    aria-expanded="false">
                                                                <i class="fas fa-download"></i>
                                                            </button>
                                                            <div class="dropdown-menu dropdown-menu-right"
                                                                 aria-labelledby="dropdownMenuButton">
                                                                <span class="dropdown-item">Choose an option:</span>
                                                                <a class="dropdown-item" ng-click="exportToExcel('#StudentRemarks')"><img src="~/teacherloginFiles/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                                <a class="dropdown-item" ng-click="ExportCSV('#StudentRemarks')"><img src="~/teacherloginFiles/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                                <a class="dropdown-item" ng-click="Exportpdf('StudentRemarks')"><img src="~/teacherloginFiles/dynamic/images/flaticons/pdf.png" class="img-fluid exports-icon" alt="Excel image"> PDF</a>
                                                                <a class="dropdown-item" ng-click="ExportWord('#StudentRemarks')"><img src="~/teacherloginFiles/dynamic/images/flaticons/word.png" class="img-fluid exports-icon" alt="Excel image"> Word</a>

                                                                <a class="dropdown-item" href="#">
                                                                    <img src="~/teacherloginFiles/dynamic/images/flaticons/gmail.png"
                                                                         class="img-fluid exports-icon"
                                                                         alt="Excel image" />
                                                                    Mail
                                                                </a>
                                                            </div>
                                                        </div>
                                                        <a href="#"
                                                           class="btn btn-secondary btn-light btn-sm"
                                                           data-toggle="tooltip"
                                                           data-placement="bottom"
                                                           title="Print">
                                                            <i class="fas fa-print"></i>
                                                        </a>


                                                        <button type="button" class="btn btn-sm btn-success" id="add-remarks" style="min-width: 90px;">
                                                            <i class="fas fa-plus w-t"></i> Add Remarks
                                                        </button>
                                                    </div>
                                                </header>

                                                <div class="table-scroll-x-y sales-main-table table-scroll" style="height: calc(100vh - 22rem) ;overflow-x:auto">
                                                    <table id="StudentRemarks"
                                                           class="main-table table table-sm table-bordered bg-white table-head-fixed">
                                                        <thead>
                                                            <tr>
                                                                <th style="width: 50px">ID  </th>
                                                                <th style="width: 80px">Regd No.</th>
                                                                <th style="width: 80px">Roll No.</th>
                                                                <th style="width: 150px">Name</th>
                                                                <th style="width: 100px">Class/Sec</th>
                                                                <th style="width: 100px">Date</th>
                                                                <th style="width: 150px">Extra Activities</th>
                                                                <th style="width: 200px">Remark</th>
                                                                <th style="width: 80px">Action</th>
                                                                <th style="width: 50px">Send</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="sr in newStudentRemarks.StudentRemarksDetailsColl">
                                                                <td>{{$index+1}}</td>
                                                                <td>{{sr.RegdNo}}</td>
                                                                <td>{{sr.RollNo}}</td>
                                                                <td class="text-left pl-1">{{sr.Name}}</td>
                                                                <td class="text-left pl-1">{{sr.ClassSection}}</td>
                                                                <td>{{sr.Date}}</td>
                                                                <td class="text-left pl-1">{{sr.ExtraActivities}}</td>
                                                                <td class="text-left pl-1">{{sr.Remark}}</td>
                                                                <td>
                                                                    <a class="p-1" href="#">
                                                                        <i class="fas fa-edit text-info"></i>
                                                                    </a>
                                                                </td>
                                                                <td>
                                                                    <a class="p-1" href="#">
                                                                        <i class="fas fa-envelope text-primary"></i>
                                                                    </a>
                                                                    <a class="p-1" href="#">
                                                                        <i class="fas fa-bell text-info"></i>
                                                                    </a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                        <div id="add-remarks-form">

                                            <div class="row">
                                                <div class="col-sm-4 col-lg-4 col-md-4">
                                                    <label>Class</label>
                                                    <div class="form-group">
                                                        <select class="form-control form-control-sm select2" ng-model="SelectedClass" id="ClassSectionId">
                                                            <option ng-repeat="ct in SectionClassColl track by $index" value="{{ ct }}">
                                                                {{ct.ClassName}}-{{ct.SectionName}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-8 col-lg-8 col-md-8 mt-4">
                                                    <div class="input-group">
                                                        <div class="input-group-prepend dropdown">
                                                            <select class="input-group-text dropdown-toggle p-0"
                                                                    id="dropdownMenuButton"
                                                                    data-toggle="dropdown"
                                                                    aria-haspopup="true"
                                                                    aria-expanded="false"
                                                                    style="outline: none; font-size: 14px">
                                                                <div class="dropdown-menu"
                                                                     aria-labelledby="dropdownMenuButton">
                                                                    <option class="dropdown-item">
                                                                        Name
                                                                    </option>
                                                                    <option class="dropdown-item">
                                                                        RegdNo.
                                                                    </option>
                                                                    <option class="dropdown-item">
                                                                        Roll No.
                                                                    </option>
                                                                    <option class="dropdown-item">Id</option>
                                                                </div>
                                                            </select>
                                                        </div>
                                                        <select class="form-control form-control-sm " ng-model="newRemarks.StudentId" id="StudentId">
                                                            <option ng-repeat="ct in StudentColl track by $index" value="{{ ct.StudentId }}">
                                                                {{ct.Name}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>


                                            <div class="table-scroll-x-y mt-2 card p-2"  style="height: calc(100vh - 22rem) ;overflow-x:auto">
                                                <table id="main-table"
                                                       class="main-table table table-sm table-bordered bg-white table-head-fixed">
                                                    <thead>
                                                        <tr class="text-center text-nowrap">
                                                            <th class="text-center align-middle cstm-sticky"
                                                                style="width: 50px">
                                                                S.No.
                                                            </th>

                                                            <th style="width: 150px">Date</th>
                                                            <th style="width: 200px">Remarks Type</th>
                                                            <th class="d-flex justify-content-center">
                                                                Remarks &nbsp;
                                                                <div class="form-check"
                                                                     style="margin-top: -2px">
                                                                    <input class="form-check-input"
                                                                           type="checkbox"
                                                                           value=""
                                                                           id="flexCheckDefault" />
                                                                    (<label class="form-check-label"
                                                                            for="flexCheckDefault"
                                                                            style="font-size: 10px">
                                                                        Unicode
                                                                    </label>
                                                                </div>
                                                                )
                                                            </th>

                                                            <th style="width: 180px">Attach Document</th>
                                                            <th style="width: 90px">Action</th>
                                                            <th style="width: 60px">Send</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td class="align-middle cstm-sticky">1</td>
                                                            <td>
                                                                <input type="text"
                                                                       class="form-control form-control-sm" date-picker ng-model="newRemarks.forDate" date-detail="newRemarks.forDateDet">
                                                            </td>
                                                            <td>
                                                                <select class="form-control form-control-sm select2" id="CboRemarksTypeId" ng-model="newRemarks.RemarksTypeId">

                                                                    <option ng-repeat="ct in RemarkTypeColl" value="{{ ct.RemarksTypeId }}" ng-selected="ct.ClassId==newRemarks.RemarksTypeId">
                                                                        {{ct.Name}}
                                                                    </option>
                                                                  
                                                                </select>
                                                            </td>
                                                            <td>
                                                                <textarea name=""
                                                                          id=""
                                                                          cols="50"
                                                                          rows="1" ng-model="newRemarks.description"
                                                                          class="form-control form-control-sm"></textarea>
                                                            </td>
                                                            <td>
                                                                @*<input type="file" onchange="angular.element(this).scope().uploadFiles(this)"
                                                                       class="form-control form-control-sm"
                                                                       ng-model="newRemarks.File"
                                                                       style="padding: 1px" />*@
                                                            </td>
                                                            <td class="text-center">
                                                                <a class="p-1" href="#">
                                                                    <i class="fas fa-save text-primary"></i>
                                                                </a>
                                                                <a class="p-1" href="#">
                                                                    <i class="fas fa-plus text-info"></i>
                                                                </a>
                                                                <a class="p-1" href="#">
                                                                    <i class="fas fa-trash-alt text-danger"></i>
                                                                </a>
                                                            </td>
                                                            <td class="text-center">
                                                                <a class="p-1" href="#">
                                                                    <i class="fas fa-envelope text-primary"></i>
                                                                </a>
                                                                <a class="p-1" href="#">
                                                                    <i class="fas fa-bell text-info"></i>
                                                                </a>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <!-- table end -->
                                            </div>


                                            <div class="time-label">
                                                <span class="d-block border-0 p-3 text-center">
                                                    <button type="button"
                                                            class="btn btn-sm btn-info"
                                                            id="back-btn"
                                                            style="min-width: 90px">
                                                        Back to List
                                                    </button>
                                                    <button type="button"
                                                            class="btn btn-sm btn-default" ng-click="ClearnewRemarks()"
                                                            style="min-width: 90px">
                                                        Reset
                                                    </button>
                                                    <button type="button"
                                                            class="btn btn-sm btn-success" ng-click="SaveRemarksToStudent()"
                                                            style="min-width: 90px">
                                                        Save
                                                    </button>
                                                </span>
                                            </div>
                                        </div>


                                    </div>
                                </div>
                                <!-- Student Remarks Ends -->
                            </div>
                            <!-- /.card -->
                        </div>
                    </div>
                </section>
            </div>
        </section>
        <!-- /.row -->
    </div>
    <!--/. container-fluid -->

    <div class="modal fade" id="modal-xl" style="margin-top: 0px;">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title">Send Notification</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <label for="">Title</label>
                        <input type="text"
                               class="form-control form-control-sm" placeholder="Title" ng-model="newNotice.title" />
                    </div>

                    <div class="form-group">
                        <label>Description</label>
                        <textarea class="form-control form-control-sm" rows="2" ng-model="newNotice.description" placeholder="Description"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="">Attach File</label>
                        <input type="file" file-model-resize ng-model="Notice_Files_TMP" after-selected="AfterChooseFile1()" fileread="Notice_Files_Data" accept="image/png, image/gif, image/jpeg, image/jpg" />
                    </div>

                    <div class="custom-control custom-checkbox">
                        <input class="custom-control-input" type="checkbox"
                               id="alert-to-parents">
                        <label for="alert-to-parents"
                               class="custom-control-label">Alert to Parents</label>
                    </div>

                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-3 text-center">
                        <button ng-disabled="loadingstatus=='running'" type="button" class="btn btn-sm btn-success" ng-click="SendNoticeToStudent()" style="min-width: 90px;">Send</button>
                    </span>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <div class="modal fade" id="modal-xl2" style="margin-top: 0px;">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title">Remarks</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">

                    <div class="form-group">
                        <label for="">Remarks Type</label>
                        <select class="form-control form-control-sm select2" ng-model="newRemarks.RemarksTypeId">
                            <option ng-repeat="ct in RemarkTypeColl" value="{{ ct.RemarksTypeId }}" ng-selected="ct.ClassId==newRemarks.RemarksTypeId">
                                {{ct.Name}}
                            </option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>Date</label>
                        <input type="text"
                               class="form-control form-control-sm" date-picker ng-model="newRemarks.forDate_TMP" date-detail="newRemarks.forDateDet">
                    </div>
                    <div class="form-group">
                        <label>Description</label>
                        <textarea class="form-control form-control-sm" rows="2" ng-model="newRemarks.description" placeholder="Description"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="">Attach File</label>
                        <input  type="file" file-model ng-model="Remarks_Files_TMP" after-selected="AfterChooseFile()" fileread="Remarks_Files_Data" accept="image/png, image/gif, image/jpeg, image/jpg" />
                    </div>
                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-3 text-center">
                        <button ng-disabled="loadingstatus=='running'" type="button" class="btn btn-sm btn-success" ng-click="SaveRemarksToStudent()" style="min-width: 90px;">Send</button>
                    </span>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.content -->
</div>


