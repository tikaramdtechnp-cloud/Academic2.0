
@{ Layout = "~/Views/Shared/_Layout.cshtml"; }
<link href="~/teacherloginFiles/dynamic/css/teacher-profile.css" rel="stylesheet" />

<script src="~/AngularJs/Creation/TeacherProfile.js"></script>
<script>
    var WEBURL="@ViewBag.WebURL";
</script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="TeacherProfileController" ng-init="LoadData()">
    <div class="content-wrapper">
        <!-- Main content -->
        <section class="content">
            <div class="container-fluid">
                <section class="iframe-content-starts-here">
                    <div class="tab-content" id="pills-tabContent">
                        <div class="tab-pane fade show active" id="pills-second" role="tabpanel"
                             aria-labelledby="pills-second-tab">
                            <!-- if-page-have-nav-tabs -->
                            <div class="col-sm-12 col-lg-12 col-md-12">
                                <div class="row jus_space">
                                    <span class="user_img">

                                        <img ng-src="{{PhotoPathData || MyProfile.PhotoPath}}" alt="user_auth" class="user-auth-img img-circle">
                                        <label for="choose-file" class="p-1">
                                            <span class="user-online-status_cam"></span>
                                        </label>
                                        <input type="file" id="choose-file" accept=".jpg,.jpeg,.png" file-model ng-model="PhotoPath_TMP" after-selected="UploadTeacherPhoto()" fileread="PhotoPathData" style="display: none" />

                                    </span>
                                    <div class="per_profile">
                                        <table>
                                            <tr>
                                                <td colspan="3">
                                                    <h1>{{MyProfile.Name}}</h1>
                                                    <h4>Employee Code: {{MyProfile.Code}}</h4>
                                                </td>
                                                @*<td><a href="#">Update Profile</a></td>*@
                                            </tr>
                                            <tr>
                                                <td>
                                                    <h5>Department</h5>
                                                    <h4>{{MyProfile.Department}}</h4>
                                                </td>
                                                <td>
                                                    <h5>Designation</h5>
                                                    <h4>{{MyProfile.Designation}}</h4>
                                                </td>
                                                <td>
                                                    <h5>Category</h5>
                                                    <h4>{{MyProfile.Category}}</h4>
                                                </td>
                                                <td><img ng-src="{{ MyProfile.SignaturePath}}"></td>
                                            </tr>
                                        </table>
                                    </div>
                                    <div class="down_img">
                                        <img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/Teaching.svg">
                                    </div>
                                </div>
                                <div class="green_stripe">
                                    <p>
                                        <i class="far fa-envelope"></i>
                                        <a href="mailto:{{MyProfile.EmailId}}" target="_blank" style="color: white !important;">{{MyProfile.OfficeEmailId}} </a>
                                    </p>
                                    <p style="color: white !important;">
                                        <i class="fa fa-phone"></i>
                                        <a href="tel:{{MyProfile.ContactNo}}" style="color: white !important;">{{MyProfile.ContactNo}}</a>
                                    </p>
                                    <i class="fas fa-map-marker-alt"></i><p>{{MyProfile.PermanentAddress}} </p>
                                </div>
                            </div>
                            <div class="col-sm-12 col-lg-12 col-md-12 mt-4">
                                <div class="row">
                                    <div class="col-sm-12 col-lg-12 col-md-12 col-xl-9">
                                        <!-- <div class="col-sm-12 col-lg-12 col-md-12"> -->
                                        <div class="per_info">
                                            <label class="horizontal_text">
                                                <span class="d-flex">
                                                    <img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/profile.svg">
                                                    <h5>Personal Information</h5>
                                                </span>
                                                <span>
                                                    <span id="toggle_tst" style="display:none; color:white!important;">
                                                        <a class="btn btn-info btn-sm" ng-click="BackProfile()">
                                                            Back To View
                                                        </a>
                                                        <a class="btn btn-info btn-sm" id="btnEdit" ng-click="EditProfile()">
                                                            <i class="far fa-edit w-t"></i> Edit
                                                        </a>
                                                        <a class="btn btn-success btn-sm" id="btnUpdate" style="display:none;" ng-click="UpdateProfile()">
                                                            <i class="far fa-save w-t"></i> Update
                                                        </a>
                                                    </span>
                                                    <span id="show_hide"><i class="fas fa-ellipsis-v"></i></span>
                                                </span>


                                            </label>

                                            <table>
                                                <tr>
                                                    <td>
                                                        <h5>Employee Id</h5>
                                                        <h4>{{MyProfile.EmployeeId}}</h4>
                                                        @*<input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.EmployeeId">*@
                                                    </td>
                                                    <td>
                                                        <h5>Employee Code</h5>
                                                        <h4>{{MyProfile.Code}}</h4>
                                                        @*<input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.Code">*@
                                                    </td>
                                                    <td>
                                                        <h5>Enroll Number</h5>
                                                        <h4>{{MyProfile.EnrollNumber}}</h4>
                                                        @*<input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.EnrollNumber">*@
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Gender</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.Gender}}</h4>
                                                        <select class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.Gender">
                                                            <option ng-value="gen.text" ng-repeat="gen in GenderColl" ng-selected="gen.text==MyProfile.Gender">{{gen.text}}</option>
                                                        </select>
                                                    </td>
                                                    <td>
                                                        <h5>DOB</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.DOB_BS}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" date-picker ng-model="MyProfile.DOB_AD_TMP" date-detail="MyProfile.DOB_ADDet">
                                                    </td>
                                                    <td>
                                                        <h5>Blood group</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.BloodGroup}}</h4>
                                                        <input type="text" class="form-control-sm form-control" ng-show="CanEdit.PerfornalInfo==true" data-ng-model="MyProfile.BloodGroup" list="lstBloodGroup">
                                                        <datalist id="lstBloodGroup">
                                                            <option data-ng-repeat="ttl in BloodGroupList" value="{{ttl}}">
                                                        </datalist>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Religion</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.Religion}}</h4>
                                                        <input type="text" class="form-control-sm form-control" ng-show="CanEdit.PerfornalInfo==true" data-ng-model="MyProfile.Religion" list="lstReligion">
                                                        <datalist id="lstReligion">
                                                            <option data-ng-repeat="ttl in ReligionList" value="{{ttl}}">
                                                        </datalist>
                                                    </td>
                                                    <td>
                                                        <h5>Caste</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.CasteName}}</h4>
                                                        <select class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.CasteId" ng-change="ChangeCasteName()">
                                                            <option ng-value="gen.id" ng-repeat="gen in CasteList" ng-selected="gen.id==MyProfile.CasteId">{{gen.text}}</option>
                                                        </select>
                                                    </td>
                                                    <td>
                                                        <h5>Nationality</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.Nationality}}</h4>
                                                        <input type="text" class="form-control-sm form-control" ng-show="CanEdit.PerfornalInfo==true" data-ng-model="MyProfile.Nationality" list="lstNationality">
                                                        <datalist id="lstNationality">
                                                            <option data-ng-repeat="ttl in CountryList" value="{{ttl}}">
                                                        </datalist>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Marital Status</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.MaritalStatus}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.MaritalStatus">
                                                    </td>

                                                    <td>
                                                        <h5>Spouse Name</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.SpouseName}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.SpouseName">
                                                    </td>
                                                    <td>
                                                        <h5>Anniversary Date</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.AnniversaryDateBS}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" date-picker ng-model="MyProfile.AnniversaryDateAD_TMP" date-detail="MyProfile.AnniversaryDateDet">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Father Name</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.FatherName}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.FatherName">
                                                    </td>
                                                    <td>
                                                        <h5>Mother Name</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.MotherName}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.MotherName">
                                                    </td>
                                                    <td>
                                                        <h5>Grand Father</h5>
                                                        <h4 ng-hide="CanEdit.PerfornalInfo==true">{{MyProfile.GrandFather}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PerfornalInfo==true" ng-model="MyProfile.GrandFather">
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                        <!-- </div> -->
                                        <div class="col-md-12 mt-4">
                                            <div class="row academic_sec">
                                                <!-- Academic Qualifications -->
                                                <div class="col-md-6">
                                                    <div class="per_info aca_info">
                                                        <label class="horizontal_text">
                                                            <span class="d-flex"><img src="~/teacherloginFiles/dynamic/images/certificate.png" class="w-15"><h5>Academic Qualifications</h5></span>
                                                            <span hidden>
                                                                <span id="toggle_academic" style="display:none;">
                                                                    <a class="btn btn-info btn-sm text-center text-white mb-1">
                                                                        <i class="far fa-edit w-t">
                                                                        </i>
                                                                        Edit
                                                                    </a>
                                                                    <a class="btn btn-success btn-sm text-center text-white mb-1">
                                                                        <i class="far fa-save w-t">
                                                                        </i>
                                                                        Update
                                                                    </a>
                                                                </span>
                                                                <span id="show_hide_academic"><i class="fas fa-ellipsis-v"></i></span>
                                                            </span>
                                                        </label>
                                                        <div class="borderRadius p-4">
                                                            <div class="yellow_box" ng-repeat="Q in MyProfile.QualificationColl">
                                                                <div>
                                                                    <h4>{{Q.DepartmentName}}</h4>
                                                                    <div class="d-flex">
                                                                        <img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/univ.svg" class="mr-2">
                                                                        <h5>{{Q.University}}, {{Q.PassedYear}}</h5>
                                                                    </div>
                                                                </div>
                                                                <div class="yellow_box">
                                                                    <h4>{{Q.GraduatePercentage}}%</h4>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- Work Experience -->
                                                <div class="col-md-6">
                                                    <div class="per_info work_info">
                                                        <label href="#" class="horizontal_text">
                                                            <span class="d-flex">
                                                                <img src="~/teacherloginFiles/dynamic/images/working-hours.png" class="w-15">
                                                                <h5>Work Experience</h5>
                                                            </span>
                                                            <span hidden>
                                                                <span id="toggle_work" style="display:none;">
                                                                    <a class="btn btn-info btn-sm text-center text-white">
                                                                        <i class="far fa-edit w-t">
                                                                        </i>
                                                                        Edit
                                                                    </a>
                                                                    <a class="btn btn-success btn-sm text-center text-white">
                                                                        <i class="far fa-save w-t">
                                                                        </i>
                                                                        Update
                                                                    </a>
                                                                </span>
                                                                <span id="show_hide_work"><i class="fas fa-ellipsis-v"></i></span>
                                                            </span>
                                                        </label>
                                                        <div class="borderRadius eme_info p-4" ng-repeat="W in MyProfile.WorkExperienceColl">
                                                            <h4>{{W.Organization}}</h4>
                                                            <h5 class="pb-3">
                                                                {{W.JobTitle}}<br>
                                                                {{W.Department}}<br>
                                                                {{W.StartMiti}} - {{W.EndMiti}}<br>
                                                                {{W.Remarks}}
                                                            </h5>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- permanent address -->
                                        <div class="per_info mt-4">
                                            <label class="horizontal_text">
                                                <span class="d-flex"><img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/location.png"><h5>Permanent Address</h5></span>


                                                <span>
                                                    <span id="toggle_paddress" style="display:none; color:white!important;">
                                                        <a class="btn btn-info btn-sm" ng-click="BackPAddress()">
                                                            Back To View
                                                        </a>
                                                        <a class="btn btn-info btn-sm" id="btnEditPAdd" ng-click="EditPAddress()">
                                                            <i class="far fa-edit w-t"></i> Edit
                                                        </a>
                                                        <a class="btn btn-success btn-sm" id="btnUpdatePAdd" style="display:none;" ng-click="UpdatePAddress()">
                                                            <i class="far fa-save w-t"></i> Update
                                                        </a>
                                                    </span>
                                                    <span id="show_hide_paddress"><i class="fas fa-ellipsis-v"></i></span>
                                                </span>
                                            </label>
                                            <table>
                                                <tr>
                                                    <td>
                                                        <h5>Country</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_Country}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_Country">
                                                    </td>
                                                    <td>
                                                        <h5>State</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_State}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_State">
                                                    </td>
                                                    <td>
                                                        <h5>zone</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_Zone}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_Zone">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>District</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_District}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_District">
                                                    </td>
                                                    <td>
                                                        <h5>City</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_City}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_City">
                                                    </td>
                                                    <td>
                                                        <h5>Municipality/VDC</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_Municipality}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_Municipality">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Ward</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_Ward}}</h4>
                                                        <input type="number" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_Ward">
                                                    </td>
                                                    <td>
                                                        <h5>Street</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_Street}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_Street">
                                                    </td>
                                                    <td>
                                                        <h5>House No</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_HouseNo}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_HouseNo">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Full Address</h5>
                                                        <h4 ng-hide="CanEdit.PAddress==true">{{MyProfile.PA_FullAddress}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.PAddress==true" ng-model="MyProfile.PA_FullAddress">
                                                    </td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                            </table>
                                        </div>
                                        <!-- temporary address -->
                                        <div class="per_info temp_info mt-4">
                                            <label class="horizontal_text">
                                                <span class="d-flex"><img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/location.png"><h5>Temporary Address</h5></span>


                                                <span>
                                                    <span id="toggle_taddress" style="display:none; color:white!important;">
                                                        <a class="btn btn-info btn-sm" ng-click="BackTAddress()">
                                                            Back To View
                                                        </a>
                                                        <a class="btn btn-info btn-sm" id="btnEditTAdd" ng-click="EditTAddress()">
                                                            <i class="far fa-edit w-t"></i> Edit
                                                        </a>
                                                        <a class="btn btn-success btn-sm" id="btnUpdateTAdd" style="display:none;" ng-click="UpdateTAddress()">
                                                            <i class="far fa-save w-t"></i> Update
                                                        </a>
                                                    </span>
                                                    <span id="show_hide_taddress"><i class="fas fa-ellipsis-v"></i></span>
                                                </span>

                                            </label>
                                            <table>
                                                <tr>
                                                    <td>
                                                        <h5>Country</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_Country}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_Country">
                                                    </td>
                                                    <td>
                                                        <h5>State</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_State}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_State">
                                                    </td>
                                                    <td>
                                                        <h5>zone</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_Zone}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_Zone">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>District</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_District}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_District">
                                                    </td>
                                                    <td>
                                                        <h5>City</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_City}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_City">
                                                    </td>
                                                    <td>
                                                        <h5>Municipality/VDC</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_Municipality}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_Municipality">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Ward</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_Ward}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_Ward">
                                                    </td>
                                                    <td>
                                                        <h5>Street</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_Street}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_Street">
                                                    </td>
                                                    <td>
                                                        <h5>House No</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_HouseNo}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_HouseNo">
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <h5>Full Address</h5>
                                                        <h4 ng-hide="CanEdit.TAddress==true">{{MyProfile.TA_FullAddress}}</h4>
                                                        <input type="text" class="form-control form-control-sm" ng-show="CanEdit.TAddress==true" ng-model="MyProfile.TA_FullAddress">
                                                    </td>
                                                    <td></td>
                                                    <td></td>
                                                </tr>
                                            </table>
                                        </div>
                                        <div class="col-sm-12 col-lg-12 col-md-12 mt-4">
                                            <div class="row">
                                                <!-- citizenship -->
                                                <div class="col-sm-4 col-lg-4 col-md-4">
                                                    <div class="per_info eme_info">
                                                        <label class="horizontal_text">
                                                            <span class="d-flex">
                                                                <img src="~/teacherloginFiles/dynamic/images/Teacher_Profile/world.png">
                                                                <h5 class="mr-2">Citizenship</h5>
                                                            </span>
                                                            <span id="toggle_citizenship" style="display:none;">
                                                                <a class="btn btn-info btn-sm mb-1 text-white" ng-click="BackCitizen()">
                                                                    <i class="far fa-arrow-alt-circle-left"></i>
                                                                </a>
                                                                <a class="btn btn-info btn-sm mb-1 text-white" id="btnEditCitizen" ng-click="EditCitizenship()">
                                                                    <i class="far fa-edit">
                                                                    </i>
                                                                </a>
                                                                <a class="btn btn-success btn-sm mb-1 text-white" id="btnUpdateCitizen" ng-click="UpdateCitizenship()">
                                                                    <i class="far fa-save">
                                                                    </i>
                                                                </a>
                                                            </span>
                                                            <span id="show_hide_citizenship"><i class="fas fa-ellipsis-v"></i></span>

                                                        </label>
                                                        <table>
                                                            <tr>
                                                                <td>
                                                                    <h5>PAN Id</h5>
                                                                    <h4 ng-hide="CanEdit.Citizenship==true">{{MyProfile.PanId}}</h4>
                                                                    <input type="text" class="form-control form-control-sm" ng-show="CanEdit.Citizenship==true" ng-model="MyProfile.PanId">
                                                                </td>
                                                            </tr>
                                                            <tr>
