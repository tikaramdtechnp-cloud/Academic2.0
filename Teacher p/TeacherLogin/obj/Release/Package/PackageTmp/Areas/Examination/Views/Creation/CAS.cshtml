
@{
    ViewBag.Title = "CAS";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/AngularJs/Creation/cas.js"></script>

<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="CasController" ng-init="LoadData()">
    <div class="content-wrapper">
        <!-- Main content -->
        <section class="content">
            <div class="container-fluid">
                <section class="iframe-content-starts-here">
                    <div class="tab-content">
                        <!-- Third Tab Content -->
                        <div id="root">
                            <div class="card-primary card-tabs">
                                <header class="if-page-have-nav-tabs">
                                    <div class="row upper-tab">
                                        <div class="col-md-12">
                                            <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active"
                                                id="myTab"
                                                role="tablist">
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link active"
                                                       id="home-tab"
                                                       data-toggle="tab"
                                                       href="#home"
                                                       role="tab"
                                                       aria-controls="home"
                                                       aria-selected="true">Marks Entry</a>
                                                </li>
                                                <li class="nav-item" role="presentation">
                                                    <a class="nav-link"
                                                       id="profile-tab"
                                                       data-toggle="tab"
                                                       href="#profile"
                                                       role="tab"
                                                       aria-controls="profile"
                                                       aria-selected="false">Summary</a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </header>
                                <div class="tab-content" id="myTabContent">
                                    <!-- Marks Entry Start -->
                                    <div class="tab-pane fade show active main-section"
                                         id="home"
                                         role="tabpanel"
                                         aria-labelledby="home-tab">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label for="">Class</label>
                                                    <select class="form-control form-control-sm select2" ng-model="newMarksEntry.ClassId">
                                                        <option ng-value="0">**Select Class**</option>
                                                        <option ng-value="cl.id" ng-repeat="cl in ClassList" ng-selected="cl.id==newMarksEntry.ClassId">{{cl.text}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label for="">Subject</label>
                                                    <select class="select2 form-control-sm"
                                                            ng-model="newMarksEntry.SubjectId"
                                                            style="width: 100%">
                                                        <option>S1</option>
                                                        <option>S2</option>
                                                        <option>S3</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label for="">Date</label>
                                                    <input type="text"
                                                           class="form-control form-control-sm" date-picker ng-model="newMarksEntry.Date_TMP" date-detail="newMarksEntry.DateDet">
                                                </div>
                                            </div>
                                            <div class="col-md-3 mt-4">
                                                <div class="form-group">
                                                    <label>Total Students : </label>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="card p-2">
                                            <header class="row mb-2">
                                                <div class="col-md-7">
                                                    <div class="clearfix">
                                                        <form class="form-inline">
                                                            <div class="input-group input-group-sm mr-sm-2">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text cstm">Show</span>
                                                                </div>
                                                                <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.MarksEntry">
                                                                    <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.MarksEntry">{{pp.text}}</option>
                                                                </select>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text bg-white"><small>entries</small></span>
                                                                </div>
                                                            </div>
                                                            <div class="input-group input-group-sm table-search-filed">
                                                                <input class="form-control form-control-navbar"
                                                                       type="search" placeholder="Search"
                                                                       aria-label="Search" ng-model="searchData.MarksEntry">
                                                                <div class="input-group-append">
                                                                    <button class="btn btn-default border-left-0"
                                                                            type="submit">
                                                                        <i class="fas fa-search"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                                <!-- end col-md-8 -->
                                                <div class="col-md-5 text-right">
                                                    <div class="dropdown dnld-file"
                                                         data-toggle="tooltip"
                                                         data-placement="top"
                                                         title="Download File">
                                                        <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                                                type="button"
                                                                id="dropdownMenuButton"
                                                                data-toggle="dropdown"
                                                                aria-haspopup="true"
                                                                aria-expanded="false">
                                                            <i class="fas fa-download"></i>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-right"
                                                             aria-labelledby="dropdownMenuButton">
                                                            <span class="dropdown-item">Choose an option:</span>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/excel.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Excel
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/csv.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                CSV
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/pdf.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                PDF
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/word.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Word
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/gmail.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Mail
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <a href="#"
                                                       class="btn btn-secondary btn-light btn-sm"
                                                       data-toggle="tooltip"
                                                       data-placement="bottom"
                                                       title="Print">
                                                        <i class="fas fa-print"></i>
                                                    </a>
                                                </div>
                                            </header>
                                            <div class="table-scroll-x-y sales-main-table table-scroll">
                                                <table id="main-table"
                                                       class="main-table table table-sm table-bordered bg-white">
                                                    <thead>
                                                        <tr>
                                                            <th class="text-center align-middle" style="width: 120px">Roll No.</th>
                                                            <th class="text-center align-middle" style="width: 120px">Symbol No.</th>
                                                            <th class="text-center align-middle" style="width: 120px">Regd No.</th>
                                                            <th class="text-center align-middle" style="width: 120px">Student Name</th>
                                                            <th class="text-center align-middle" style="width: 120px">CAS Type Name</th>
                                                            <th class="text-center align-middle" style="width: 120px">Remarks</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>1</td>
                                                            <td>1</td>
                                                            <td></td>
                                                            <td></td>
                                                            <td class="text-center">
                                                                <div class="custom-control custom-checkbox">
                                                                    <input class="custom-control-input" type="checkbox"
                                                                           id="customCheckbox1" value="option1">
                                                                    <label for="customCheckbox1"
                                                                           class="custom-control-label"></label>
                                                                </div>
                                                            </td>
                                                            <td></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>

                                            <div class="time-label">
                                                <span class="d-block border-0 p-3 text-center">
                                                    <button type="button"
                                                            class="btn btn-sm btn-default"
                                                            style="min-width: 90px">
                                                        Reset
                                                    </button>
                                                    <button type="button"
                                                            class="btn btn-sm btn-danger"
                                                            style="min-width: 90px">
                                                        Delete
                                                    </button>
                                                    <button type="button"
                                                            class="btn btn-sm btn-success"
                                                            style="min-width: 90px">
                                                        Save
                                                    </button>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Marks Entry Ends -->
                                    <!-- Summary Start -->
                                    <div class="tab-pane fade main-section"
                                         id="profile"
                                         role="tabpanel"
                                         aria-labelledby="profile-tab">
                                        <div class="row">
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="">Class</label>
                                                    <select class="form-control form-control-sm select2" ng-model="newSummary.ClassId">
                                                        <option ng-value="0">**Select Class**</option>
                                                        <option ng-value="cl.id" ng-repeat="cl in ClassList" ng-selected="cl.id==newSummary.ClassId">{{cl.text}}</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="">From Date</label>
                                                    <input type="text"
                                                           class="form-control form-control-sm" date-picker ng-model="newSummary.FromDate_TMP" date-detail="newSummary.FromDateDet">
                                                </div>
                                            </div>

                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label for="">To Date</label>
                                                    <input type="text"
                                                           class="form-control form-control-sm" date-picker ng-model="newSummary.ToDate_TMP" date-detail="newSummary.ToDateDet">
                                                </div>
                                            </div>
                                            <div class="col-md-1 col-sm-1 mt-4">
                                                <label>FTH :</label>
                                            </div>
                                            <div class="col-md-1 col-sm-1 mt-4">
                                                <label>FPR :</label>
                                            </div>
                                            <div class="col-md-1 col-sm-1 mt-4">
                                                <label>PTH :</label>
                                            </div>
                                            <div class="col-md-1 col-sm-1 mt-4">
                                                <label>PPR :</label>
                                            </div>
                                        </div>
                                        <div class="card p-2">
                                            <header class="row mb-2">
                                                <div class="col-md-7">
                                                    <div class="clearfix">
                                                        <form class="form-inline">
                                                            <div class="input-group input-group-sm mr-sm-2">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text cstm">Show</span>
                                                                </div>
                                                                <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                                                <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.Summary">
                                                                    <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.Summary">{{pp.text}}</option>
                                                                </select>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text bg-white"><small>entries</small></span>
                                                                </div>
                                                            </div>
                                                            <div class="input-group input-group-sm table-search-filed">
                                                                <input class="form-control form-control-navbar"
                                                                       type="search" placeholder="Search"
                                                                       aria-label="Search" ng-model="searchData.Summary">
                                                                <div class="input-group-append">
                                                                    <button class="btn btn-default border-left-0"
                                                                            type="submit">
                                                                        <i class="fas fa-search"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                                <!-- end col-md-8 -->
                                                <div class="col-md-5 text-right">
                                                    <div class="dropdown dnld-file"
                                                         data-toggle="tooltip"
                                                         data-placement="top"
                                                         title="Download File">
                                                        <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                                                type="button"
                                                                id="dropdownMenuButton"
                                                                data-toggle="dropdown"
                                                                aria-haspopup="true"
                                                                aria-expanded="false">
                                                            <i class="fas fa-download"></i>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-right"
                                                             aria-labelledby="dropdownMenuButton">
                                                            <span class="dropdown-item">Choose an option:</span>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/excel.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Excel
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/csv.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                CSV
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/pdf.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                PDF
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/word.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Word
                                                            </a>
                                                            <a class="dropdown-item" href="#">
                                                                <img src="~/teacherloginFiles/dynamic/images/flaticons/gmail.png"
                                                                     class="img-fluid exports-icon"
                                                                     alt="Excel image" />
                                                                Mail
                                                            </a>
                                                        </div>
                                                    </div>
                                                    <a href="#"
                                                       class="btn btn-secondary btn-light btn-sm"
                                                       data-toggle="tooltip"
                                                       data-placement="bottom"
                                                       title="Print">
                                                        <i class="fas fa-print"></i>
                                                    </a>
                                                </div>
                                            </header>
                                            <div class="table-scroll-x-y sales-main-table table-scroll">
                                                <table id="main-table"
                                                       class="main-table table table-sm table-bordered bg-white">
                                                    <thead>
                                                        <tr>
                                                            <th class="text-center align-middle" style="width: 90px">S.No.</th>
                                                            <th class="text-center align-middle" style="width: 100px">Regd No.</th>
                                                            <th class="text-center align-middle" style="width: 100px">Roll No.</th>
                                                            <th class="text-center align-middle" style="width: 200px">Name</th>
                                                            <th class="text-center align-middle" style="width: 120px">Class/Sec</th>
                                                            <th class="text-center align-middle" style="width: 120px">Count</th>
                                                            <th class="text-center align-middle" style="width: 150px">Total Obtain Marks</th>
                                                            <th class="text-center align-middle" style="width: 150px">Avg Obtain Marks</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr dir-paginate="cl in SummaryList | filter:searchData.Summary | itemsPerPage: perPage.Summary" current-page="currentPages.Summary" pagination-id="SummaryPagination">
                                                            <td>{{$index+1}}</td>
                                                            <td>{{cl.RegdNo}}</td>
                                                            <td>{{cl.RollNo}}</td>
                                                            <td>{{cl.Name}}</td>
                                                            <td>{{cl.ClassSec}}</td>
                                                            <td>{{cl.Count}}</td>
                                                            <td>{{cl.TotalObtainMarks}}</td>
                                                            <td>{{cl.AvgObtainMark}}</td>                                                            
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>

                                            <div class="time-label">
                                                <span class="d-block border-0 p-3 text-center">



                                                    <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearSummary()">Reset</button>
                                                    <button type="button"
                                                            class="btn btn-sm btn-danger"
                                                            style="min-width: 90px">
                                                        Delete
                                                    </button>
                                                    <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateSummary()">{{newSummary.Mode}}</button>
                                                </span>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                                <!-- /.card -->
                            </div>
                        </div>
                    </div>
                </section>
                <!-- /.row -->
            </div>
            <!--/. container-fluid -->
        </section>
        <!-- /.content -->
    </div>
</div>