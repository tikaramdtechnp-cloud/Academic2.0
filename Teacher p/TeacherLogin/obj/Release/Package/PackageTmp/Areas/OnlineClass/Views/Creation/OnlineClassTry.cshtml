                                                                                    <label>Notes</label>
                                                                                    <textarea class="form-control form-control-sm" ng-model="HostClass.Notes"
                                                                                              rows="1"
                                                                                              placeholder=""></textarea>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-3 mt-4 pt-3">
                                                                                <button type="button" class="btn btn-sm btn-success" ng-click="StartOnlineClass()" style="min-width: 90px">
                                                                                    Host
                                                                                </button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <!-- It's Time to host card ends -->
                                                        </div>
                                                    </div>
                                                </div>

