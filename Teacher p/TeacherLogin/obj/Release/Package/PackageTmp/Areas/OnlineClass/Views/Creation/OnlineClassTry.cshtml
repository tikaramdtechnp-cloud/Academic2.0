                                                                        <select class="form-control form-control-sm select2" id="cboClassShift" ng-model="ct">
                                                                            <option ng-repeat="ct in ClassShiftList  track by $index" value="{{ ct }}" ng-selected="ct.ClassShiftId==ct">
                                                                                {{ct.Name}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Select Class</label>
                                                                        <select class="form-control form-control-sm select2" id="cboClassId" ng-model="V">
                                                                            <option ng-repeat="ct in ClassColl  track by $index" value="{{ ct.ClassId }}" ng-selected="ct.ClassId==V">
                                                                                {{ct.Name}}
                                                                            </option>
                                                                            @*<option ng-repeat="ct in ClassListColl  track by $index" value="{{ ct.ClassId }}" ng-selected="ct.ClassId==V">
                                                                                    {{ct.Name}}
                                                                                </option>*@
                                                                        </select>
                                                                    </div>
                                                                    <!-- /.form-group -->
                                                                    <div class="form-group">
                                                                        <label>Select Section</label>
                                                                        <select class="form-control form-control-sm select2" id="cboSectionId" ng-model="SectionId" multiple>
                                                                            <option ng-repeat="ct in SectionList  " value="{{ ct.SectionId }}" ng-selected="ct.SectionId==SectionId">
                                                                                {{ct.SectionName}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Select Subject</label>
                                                                        <select class="form-control form-control-sm select2" id="cboSubjectId" ng-model="newOnlineClass.SubjectId">
                                                                            <option ng-repeat="ct in SubjectColl track by $index" value="{{ ct.SubjectId }}" ng-selected="ct.SubjectId==newOnlineClass.SubjectId">
                                                                                {{ct.Name}}
                                                                            </option>
                                                                        </select>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label>Notes</label>
                                                                        <textarea class="form-control form-control-sm" ng-model="newOnlineClass.Notes"
                                                                                  rows="2"
                                                                                  placeholder=""></textarea>
                                                                    </div>
                                                                    <!-- /.form-group -->
                                                                    <div class="card-footer">
                                                                        <button type="button" class="btn btn-secondary btn-sm mb-1" ng-click="StartOnlineClass()">Host</button>
                                                                        <button type="button" class="btn btn-secondary btn-sm mb-1" id="back-to-platform">Back</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <!-- /.row -->
                                                        </div>
                                                        <!-- /.card-body -->
                                                    </div>
                                                    <!-- /.card -->
                                                </div>
