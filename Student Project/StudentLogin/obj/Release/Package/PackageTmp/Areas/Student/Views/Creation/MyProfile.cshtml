<script type="text/javascript">
    Chart.pluginService.register({
        beforeDraw: function (chart) {
            var width = chart.chart.width,
                height = chart.chart.height,
                ctx = chart.chart.ctx;
            ctx.restore();
            var fontSize = (height / 114).toFixed(2);
            ctx.font = fontSize + "em sans-serif";
            ctx.textBaseline = "middle";
            var text = chart.config.options.elements.center.text,
                textX = Math.round((width - ctx.measureText(text).width) / 2),
                textY = height / 2;
            ctx.fillText(text, textX, textY);
            ctx.save();
        }
    });

    var chartData = [{ "Attendance": 90, "visits": 1 }, { "Attendance": 68, "visits": 2 }, { "Attendance": 85, "visits": 3 }, { "Attendance": 95, "visits": 4 }]

    var AttendanceData = [],
        sum = 0,
        visitsData = [];

    for (var i = 0; i < chartData.length; i++) {
        AttendanceData.push(chartData[i]['Attendance'])
        visitsData.push(chartData[i]['visits'])

        sum += chartData[i]['Attendance'];
    }

    var textInside = sum.toString();

    var myChart = new Chart(document.getElementById('classChart'), {
        type: 'doughnut',
        animation: {
            animateScale: true
        },
        data: {
            labels: visitsData,
            datasets: [{
                label: 'Attendance',
                data: AttendanceData,
                backgroundColor: [
                    "#F4933C",
                    "#F4C546",
                    "#1D92DF",
                    "#76BE4E"
                ]
            }]
        },
        options: {
            elements: {
                center: {
                    text: textInside
                }
            },
            responsive: true,
            legend: false,
