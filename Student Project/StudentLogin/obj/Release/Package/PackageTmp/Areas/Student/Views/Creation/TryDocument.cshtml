
@{
    ViewBag.Title = "Homework";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Jslib/Test/Assignment.js"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="HomeworkController" ng-init="LoadData()">
    <div class="content-wrapper" ng-controller="MyCtrl">

        <!-- Main content -->
        <section class="content">
            <div class="container-fluid">
                <section class="iframe-content-starts-here">
                    <div class="tab-content" id="pills-tabContent">
                        <!-- First Tab Content -->
                        <!-- Second Tab Content -->
                        <div class="tab-pane fade show active" id="pills-second" role="tabpanel"
                             aria-labelledby="pills-second-tab">
                            <!-- if-page-have-nav-tabs -->
                            <header class="if-page-have-nav-tabs">
                                <div class="row upper-tab">

                                    <div class="col-md-12">
                                        <!-- <h3 class="menu-title">Class Setup</h3> -->
                                        <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active" id="myTab"
                                            role="tablist">
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link active" id="pills-four-tab" data-toggle="tab"
                                                   href="#pills-four" role="tab" aria-controls="pills-four"
                                                   aria-selected="true">Homework List  </a>
                                            </li>
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link" id="profile-tab" data-toggle="tab"
                                                   href="#profile" role="tab" aria-controls="profile"
                                                   aria-selected="false">Submitted Homework</a>
                                            </li>
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link" id="profile-tab1" data-toggle="tab"
                                                   href="#profile1" role="tab" aria-controls="profile"
                                                   aria-selected="false">Not Submitted</a>
                                            </li>

                                        </ul>
                                    </div>

                                </div>
                            </header>
                            <!-- end if-page-have-nav-tabs -->
                            <div class="tab-content " id="myTabContent">
                                <div class="tab-pane fade show active main-section" id="pills-four" role="tabpanel" aria-labelledby="pills-four-tab">
                                    <div class="homework-list" id="homework-list">

                                        <div class="row">
                                            <div class="col-md-3">
                                                <label for="">From Date </label>
                                                <input type="text" class="form-control form-control-sm" ng-change="ToSubmittSelectDate()" ng-model="newToSubmitHomework.FromDate" date-picker date-detail="newSubmittedHomework.FromDateDet">
                                            </div>

                                            <div class="col-md-3">
                                                <label for="">To Date </label>
                                                <input type="text" class="form-control form-control-sm" ng-change="ToSubmittSelectDate()" ng-model="newToSubmitHomework.ToDate" date-picker date-detail="newSubmittedHomework.ToDateDet">
                                            </div>
                                            <div class="col-md-3">
                                                <label for="">Homework Type</label>
                                                <select class="form-control form-control-sm " ng-change="onSelectHomewoskType(cl)" style="width:100%" ng-model="cl">
                                                    <option>-- Select ExamType --</option>
                                                    <option value="{{cl}}" ng-repeat="cl in HomeworkColl" @*ng-selected="cl.Name==HomewoskTypeName"*@>{{cl.Name}}</option>
                                                </select>

                                            </div>
                                        </div>

                                        <div class="card p-2 mt-2">
                                            <header class="row mb-2">
                                                <div class="col-md-7">
                                                    <div class="clearfix">
                                                        <form class="form-inline">

                                                            <div class="input-group input-group-sm mr-sm-2">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text cstm">Show</span>
                                                                </div>
                                                                <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                                                <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.Homework">
                                                                    <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.Homework">{{pp.text}}</option>
                                                                </select>
                                                                <div class="input-group-append">
                                                                    <span class="input-group-text bg-white"><small>entries</small></span>
                                                                </div>
                                                            </div>
                                                            <div class="input-group input-group-sm table-search-filed">
                                                                <input class="form-control form-control-navbar" type="search"
                                                                       placeholder="Search" aria-label="Search" ng-model="searchData.Homework">
                                                                <div class="input-group-append">
                                                                    <button class="btn btn-default border-left-0" type="submit">
                                                                        <i class="fas fa-search"></i>
                                                                    </button>
                                                                </div>
                                                            </div>

                                                        </form>
                                                    </div>
                                                </div>
                                                <!-- end col-md-8 -->
                                                <div class="col-md-5 text-right">
                                                    <div class="dropdown no-caret d-inline-block">
                                                        <button type="button" class="btn btn-secondary btn-light btn-sm dropdown-toggle" data-toggle="dropdown">
                                                            <i class="fas fa-columns"></i>
                                                        </button>
                                                        <ul class="dropdown-menu cstm-column-visibility dropdown-menu-right">
                                                            <span class="dropdown-item">Column Visibility:</span>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option1" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        Date
                                                                        <input type="checkbox" ng-model="DateCol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option2" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        SUBJECT NAME
                                                                        <input type="checkbox" ng-model="SUBJECTNAMECol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option3" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        DESCRIPTION
                                                                        <input type="checkbox" ng-model="DESCRIPTIONCol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option3" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        TEACHER
                                                                        <input type="checkbox" ng-model="TEACHERCol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option3" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        DEADLINEDATE
                                                                        <input type="checkbox" ng-model="DEADLINEDATECol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <a href="#" class="small dropdown-item" data-value="option3" tabIndex="-1">
                                                                    <label class="checkbox-wrap">
                                                                        STATUS
                                                                        <input type="checkbox" ng-model="STATUSCol">
                                                                        <span class="checkmark"></span>
                                                                    </label>
                                                                </a>
                                                            </li>

                                                            @*<li class="btns-wrap">
                                                            <a href="#" class="btn btn-link btn-sm">Restore</a>
                                                            <a href="#" class="btn btn-secondary btn-sm">Appy</a>
                                                        </li>*@
                                                        </ul>
                                                    </div>
                                                    <div class="dropdown dnld-file" data-toggle="tooltip"
                                                         data-placement="top" title="Download File">
                                                        <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button"
                                                                id="dropdownMenuButton" data-toggle="dropdown"
                                                                aria-haspopup="true" aria-expanded="false">
                                                            <i class="fas fa-download"></i>
                                                        </button>


                                                        <button class="btn btn-link" ng-click="exportToExcel('#main-table')">
                                                            <span class="glyphicon glyphicon-share"></span> Export to Excel
                                                        </button>

                                                        <button ng-click="ExportCSV('#main-table')">DownloadCSV</button>
                                                        <button ng-click="Exportpdf('main-table')">DownloadPDF</button>
                                                        <button ng-click="ExportWord('#main-table')">DownloadWord</button>
                                                        <button ng-click="createPDF()">createPDF</button>


                                                        <div class="dropdown-menu dropdown-menu-right"
                                                             aria-labelledby="dropdownMenuButton">
                                                            <span class="dropdown-item">Choose an option:</span>
                                                            <a class="dropdown-item" ng-click="exportToExcel('#main-table')"><img src="~/StudentLoginFiles/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                            <a class="dropdown-item" ng-click="ExportCSV('#main-table')"><img src="~/StudentLoginFiles/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                            <a class="dropdown-item" ng-click="Exportpdf('main-table')"><img src="~/StudentLoginFiles/dynamic/images/flaticons/pdf.png" class="img-fluid exports-icon" alt="Excel image"> PDF</a>
                                                            <a class="dropdown-item" ng-click="ExportWord('#main-table')"><img src="~/StudentLoginFiles/dynamic/images/flaticons/word.png" class="img-fluid exports-icon" alt="Excel image"> Word</a>
                                                            <a class="dropdown-item" href="#"><img src="~/StudentLoginFiles/dynamic/images/flaticons/gmail.png" class="img-fluid exports-icon" alt="Excel image"> Mail</a>
                                                        </div>
                                                    </div>
                                                    <a href="#" class="btn btn-secondary btn-light btn-sm" data-toggle="tooltip"
                                                       data-placement="bottom" title="Print">
                                                        <i class="fas fa-print"></i>
                                                    </a>

                                                </div>
                                            </header>
                                            <div>
                                                <div class="table-scroll-x-y sales-main-table table-scroll" id="tab">
                                                    <table id="main-table" class="main-table table table-sm table-bordered bg-white mb-2">
                                                        <thead class=" text-nowrap">
                                                            <tr>
                                                                <th style="width: 40px;text-align:center;">
                                                                    S.No.
                                                                </th>
                                                                <th ng-hide="DateCol" style="width: 80px;text-align:center;" ng-click="sort('AsignDateTime_BS')">Date</th>
                                                                <th ng-hide="SUBJECTNAMECol" style="width: 150px;text-align:center" ng-click="sort('SubjectName')">Subject Name</th>

                                                                <th ng-hide="DESCRIPTIONCol" style="width: 200px;text-align:center" ng-click="sort('Description')">Description</th>
                                                                <th ng-hide="TEACHERCol" style="width: 150px;text-align:center" ng-click="sort('TeacherName')">Teacher</th>

                                                                <th ng-hide="DEADLINEDATECol" style="width: 150px;text-align:center" ng-click="sort('DeadlineDate_BS')">DeadlineDate </th>
                                                                <th ng-hide="STATUSCol" style="width: 80px;text-align:center" ng-click="sort('HomeWorkStatus')">Status</th>
                                                                <th style="width: 90px;text-align:center">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr dir-paginate="V in ToSubmitHomework | filter:searchData.Homework| filter:HomewoskTypeName | itemsPerPage: perPage.Homework|orderBy:sortKey:reverse" current-page="currentPages.Homework" pagination-id="HomeworkPagination">
                                                                <td>{{$index+1}}</td>
                                                                <td ng-hide="DateCol">{{V.AsignDateTime_BS}}</td>
                                                                <td ng-hide="SUBJECTNAMECol">{{V.SubjectName}} </td>
                                                                <td ng-hide="DESCRIPTIONCol" ng-bind-html="V.Description| unsafe">{{V.Description}}<br /></td>

                                                                <td ng-hide="TEACHERCol">{{V.TeacherName}}</td>
                                                                <td ng-hide="DEADLINEDATECol" ng-style="{'color': ((V.HomeWorkStatus == 'NOT DONE') ?  '#FF6969' : '') }">{{V.DeadlineDate_BS}}-{{V.DeadlineTime}}</td>
                                                                <td ng-hide="STATUSCol">{{V.HomeWorkStatus}}</td>

                                                                <td class="text-white justify-content-center" style="font-size: 8px;">

                                                                    <button type="button" class="btn btn-sm btn-success py-0" style="min-width :70px" ng-click="Submitted(V)">Submit</button>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>

                                                <!-- table end -->
                                            </div>
                                            <footer class="row align-items-center">
                                                <div class="col-md-12">
                                                    <dir-pagination-controls pagination-id="HomeworkPagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/Scripts/dirPagination.tpl.html"></dir-pagination-controls>
                                                </div>
                                            </footer>

                                        </div>
                                    </div>

                                    <div id="submitted">
                                        <div class="card card-default">
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="img-div">
                                                            <style>
                                                                span.circle {
                                                                    background: #e3e3e3;
                                                                    border-radius: 50%;
                                                                    -moz-border-radius: 50%;
                                                                    -webkit-border-radius: 50%;
                                                                    color: #6e6e6e;
                                                                    display: inline-block;
                                                                    font-weight: bold;
                                                                    line-height: 40px;
                                                                    margin-right: 5px;
                                                                    margin-left: 5px;
                                                                    text-align: center;
                                                                    width: 40px;
                                                                }
                                                            </style>
                                                            <span class="circle">{{(HomeWork.TeacherName).substring(0, 1)}}</span>
                                                            @*<img src="~/StudentLoginFiles/dynamic/images/aryan-shrestha.png" alt="" class="rounded-circle" style="width: 50px;" />*@
                                                            <span style="font-weight: bold"> {{HomeWork.TeacherName}} </span> <br>


                                                        </div>

                                                    </div>
                                                    <div class="col-md-6 mt-2">
                                                        <span class="float-right" style="font-weight: 500">{{HomeWork.AsignDateTime_AD| date : "fullDate"}} -{{HomeWork.DeadlineTime}} </span>
                                                    </div>
                                                </div>
                                                <div class="row mt-2" style="font-weight: 500">
                                                    <div class="col-md-4">
                                                        <span>Subject : {{HomeWork.SubjectName}}</span>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <span>Lesson : {{HomeWork.Lession}}</span>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <span>Topic :  {{HomeWork.Topic}}</span>
                                                    </div>
                                                </div>
                                                <hr>
                                                <div class="card homework-description p-2" style="margin-bottom: 10px !important;" ng-bind-html="HomeWork.Description| unsafe">
                                                    {{HomeWork.Description}}
                                                </div>
                                                <div class="teacher-file-attach d-flex bg-white p-2" style="margin-top: -22px;">
                                                    <span style="font-size: 16px;"><i class="fas fa-folder-open text-success"></i> &nbsp; {{HomeWork.AttachmentColl.length}} File(s) Attachment </span>


                                                    <span style="font-size: 16px;" class="ml-auto">
                                                        <a ng-click="AllDownload(HomeWork.AttachmentColl)"> <i class="fas fa-download text-success" title="Download All"></i></a>
                                                    </span>
                                                </div>

                                                <!-- teacher file preview -->
                                                <div class="container">
                                                    <div class="row">

                                                        <div class="col-md-3" ng-repeat="item in HomeWork.AttachmentColl">
                                                            <a href="#" data-toggle="modal" class="color-gray-darker c6 td-hover-none">
                                                                <div class="ba-0 ds-1" ng-click="modalIMG(item)">
                                                                    <img alt="Homework_{{(item).slice(-10)}}" class="img-fluid" src="{{item}}" style="width:60%;" />
                                                                </div>
                                                            </a>
                                                            <span style="font-size: 16px;" class="ml-auto">
                                                                <a href="{{item}}" download> <i class="fas fa-download text-success" title="Download All"></i></a>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div aria-hidden="true" aria-labelledby="myModalLabel" class="modal fade" id="modalIMG" role="dialog" tabindex="-1">
                                                    <div class="modal-dialog modal-lg" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-body mb-0 p-0">
                                                                <iframe id="frmTeacherDoc" style='width: 100%;height:calc(100vh - 200px);background-color: white;visibility: visible;overflow:hidden ' frameborder='0' scrolling='yes' @*ng-src="https://localhost:44374/Student/Creation/Homework" style="width:100%; height: 70vh;"*@></iframe>
                                                            </div>
                                                            <div class="modal-footer">
                                                                <div><a href="{{TeacherDoc}}" target="_blank" download>Download</a></div>
                                                                <button class="btn btn-success btn-rounded btn-sm ml-4 text-center" data-dismiss="modal" type="button" style="min-width: 90px;">Close</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <!-- teacher file preview end -->
                                                <hr>
                                                <div class="student-attach-file card-default">
                                                    <label style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'">Teacher's Remarks </label>
                                                    <div class="p-2" ng-show="HomeWork.HomeWorkStatus=='RE-DO'" style="background-color: #f5f5f9; border: 1px solid #ccc;">
                                                        <label>{{HomeWork.CheckedRemarks}}</label>
                                                    </div>
                                                    <label style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='RE-DO'" class="mt-2"><i class="fas fa-folder-open text-success"></i> &nbsp; Checked Files </label>
                                                    <span ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'" style="font-size: 16px;" class="ml-auto  float-right ">
                                                        <a ng-click="AllDownload(HomeWork.StudentAttachmentsColl)"> <i class="fas fa-download text-success" title="Download All"></i></a>
                                                    </span>
                                                    <div class="container" ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'">
                                                        <div class="row">
                                                            <div class="col-md-2 m-2" ng-repeat="item in HomeWork.StudentAttachmentsColl">
                                                                <a href="#" ng-click="ResubmitToggel(item)" data-toggle="modal" class="color-gray-darker c6 td-hover-none">
                                                                    <div class="ba-0 ds-1">
                                                                        <img alt="Homework_{{(item).slice(-10)}}" class="img-fluid" src="{{item}}" style="width:60%;" />
                                                                    </div>
                                                                </a>
                                                                <span style="font-size: 16px;" class="ml-auto">
                                                                    <a href="{{item}}" download> <i class="fas fa-download text-success" title="Download All"></i></a>
                                                                </span>
                                                            </div>
                                                        </div>

                                                    </div>
                                                    <div ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'" aria-hidden="true" aria-labelledby="myModalLabel" class="modal fade" id="ResubmitToggel" role="dialog" tabindex="-1">
                                                        <div class="modal-dialog modal-lg" role="document">
                                                            <div class="modal-content">
                                                                <div class="modal-body mb-0 p-0">
                                                                    @*<iframe id="CheckedAttach_Iframe" style='width: 100%;height:calc(100vh - 200px);background-color: white;visibility: visible;' frameborder='0' scrolling='yes' ></iframe>*@
                                                                    <img ng-src="{{CheckedDoc}}" alt="" style="width:100%; height: 70vh;">
                                                                </div>
                                                                <div class="modal-footer">
                                                                    <div><a href="{{CheckedDoc}}" target="_blank" download>Download</a></div>
                                                                    <button class="btn btn-success btn-rounded btn-sm ml-4 text-center" data-dismiss="modal" type="button" style="min-width: 90px;">Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <br ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'">
                                                    <hr ng-show="HomeWork.HomeWorkStatus=='RE-DO'||HomeWork.HomeWorkStatus=='DONE'">
                                                    <div class="py-1" style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='NEW'"><strong>Submit Homework</strong> </div>
                                                    <div class="py-1" style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='PENDING'"><strong>You Submitted </strong> </div>
                                                    <div class="py-1" style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='NOT DONE'"><strong>Submit Homework</strong> </div>
                                                    <div class="py-1" style="font-size: 16px;" ng-show="HomeWork.HomeWorkStatus=='RE-DO'"><strong>RE-Submit Homework</strong> </div>

                                                    <label style="font-size: 16px;">Description</label>
                                                    <textarea class="form-control form-control-sm" ng-model="HomeWork.Notes" cols="30" rows="1"></textarea>


                                                    <label for="fileupload" style="font-size: 16px;" class="mt-3"><i class="fas fa-folder-open text-success"></i> &nbsp; Attach Your files </label>
                                                    <!-- attach file  -->
                                                    <div class="card-body" style="margin-top: -10px;">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <div class="row">

                                                                    <div class="bg-white">
                                                                        <div class="button-green">
                                                                            <input class="file-upload" type="file" multiple file-model-resize ng-model="Files_TMP" after-selected="AfterChooseFile()" fileread="Files_Data" />
                                                                            <span><i class="fas fa-plus-circle"></i></span>
                                                                        </div>
                                                                        <style>

                                                                            .button-green {
                                                                                position: relative;
                                                                                border-radius: 5px;
                                                                                border: 2px dotted #28a745;
                                                                                width: 82px;
                                                                                height: 82px;
                                                                                padding: 30px 30px;
                                                                            }

                                                                            .file-upload {
                                                                                border-radius: 5px;
                                                                                border: 2px dotted #28a745;
                                                                                opacity: 0;
                                                                                top: 0;
                                                                                left: 0;
                                                                                width: 82px;
                                                                                height: 35px;
                                                                                position: absolute;
                                                                                padding: 30px 30px;
                                                                            }

                                                                            .button-green:hover {
                                                                                background-color: #ccc;
                                                                            }
                                                                        </style>


                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <c class="m-1" ng-show="(newHomework.uploadFiles).length != 0">
                                                            <div class=" row">
                                                                <div class="col-sm-2 " ng-repeat="FileImg in newHomework.uploadFiles">
                                                                    <div class="ba-0 ds-1">

                                                                        <span style="margin: 3px ">
                                                                            <a ng-click="DeleteAlbumPhoto($index)"><i class="fas fa-times"></i></a>
                                                                        </span>

                                                                        <span style="display:block;width:365%;">
                                                                            <a class="lightbox"
                                                                               href="{{ FileImg.uploadFiles}}">
                                                                                <img ng-src="{{ FileImg.uploadFiles}}"
                                                                                     id="FileImg{{$index+1}}" alt="" style=" height:10%;width:20%;object-fit:cover ;">
                                                                            </a>
                                                                        </span>
                                                                        <label for="FileImg{{$index + 1}}" style=" max-width: 150px; word-wrap: break-word;">
                                                                            {{$index + 1}}-{{FileImg.FileName}}
                                                                        </label>

                                                                        <br>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </c>

                                                    </div>
                                                    <!-- attach file end -->
                                                </div>

                                            </div>
                                            <div class="time-label">
                                                <span class="d-block border-0 p-3 text-center">
                                                    <button type="button" class="btn btn-sm btn-info" id="back-to-homeworklist" style="min-width: 90px;">Back to List</button>

                                                    <button type="button" ng-disabled="" class="btn btn-sm btn-success" ng-click="SubmitHomeWork()" style="min-width: 90px;">Submit</button>

                                                </span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="tab-pane fade main-section" id="profile" role="tabpanel"
                                     aria-labelledby="profile-tab">

                                    <div id="submitted-homework">
                                        <form action="" style="margin-top: -10px;">
                                            <div class="row mb-2">
                                                <div class="col-md-3 ">
                                                    <label for="">From Date </label>
                                                    <input type="text" class="form-control form-control-sm" ng-change="SelectDate()" ng-model="newSubmittedHomework.FromDate" date-picker date-detail="newSubmittedHomework.FromDateDet">
