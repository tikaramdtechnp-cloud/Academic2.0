  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select LAC.LedgerAllocationId,LAC.TranId,LAC.LedgerId,L.Name ,LAC.AgentId,LAC.DrCr,LAC.DrAmount,LAC.CrAmount,LAC.Narration,A.Name as AgentName  from tbl_LedgerAllocationContra LAC left join tbl_Ledger L on LAC.LedgerId=L.LedgerId left join tbl_Agent A on LAC.AgentId=A.AgentId where LAC.TranId=@TranId and LAC.DrCr=2 
</CommandText>
        <QueryParameters>
