  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>with tblItem
as
(
select P.ProductCategoriesId,Sum(IAS.ActualQty) as Qty from tbl_ItemAllocationSalesInvoice IAS 
inner join tbl_Product P on P.ProductId=IAS.ProductId
where IAS.TranId=@TranId
group by P.ProductCategoriesId
)
select PC.Name,I.Qty from tblItem I inner join tbl_ProductCategories PC on PC.ProductCategoriesId=I.ProductCategoriesId
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Qty">
          <DataField>Qty</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
