  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select SIDSJ.TranId,SIDSJ.ProductId,P.Name as ProductName,SIDSJ.GodownId,G.Name as GodownName,SIDSJ.Batch,SIDSJ.MFGDate,SIDSJ.EXPDate,SIDSJ.ActualQty,SIDSJ.BilledQty,SIDSJ.Rate,SIDSJ.UnitId,U.Name as UnitName,SIDSJ.Amount,SIDSJ.DiscountPer,SIDSJ.DiscountAmt from tbl_SourceItemDetailsStockJournal SIDSJ
left join tbl_Product P on SIDSJ.ProductId=P.ProductId
left join TBL_GODOWN G on SIDSJ.GodownId=G.GodownId
left join TBL_UNIT U on SIDSJ.UnitId=U.UnitId where SIDSJ.TranId=@TranId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
