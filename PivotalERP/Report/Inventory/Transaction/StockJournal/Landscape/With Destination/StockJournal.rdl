  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select TSJ.TranId,TSJ.AutoVoucherNo,TSJ.ManualVoucherNO, TSJ.VoucherId,V.VoucherName,TSJ.CostClassId,CC.Name as CostClassName,TSJ.VoucherDate,TSJ.Narration,TSJ.No,TSJ.IsCancel,TSJ.IsPost,TSJ.PostDateTime,TSJ.PostBy,TSJ.PostUserId,U.UserName as PostUserName,TSJ.NY,TSJ.NM,TSJ.ND,TSJ.CurrencyId,C.Name as CurrencyName,TSJ.CurRate,TSJ.AutoManualNo,TSJ.CreateBy,TSJ.ModifyBy from tbl_TransactionStockJournal TSJ
left join tbl_Voucher V on TSJ.VoucherId=V.VoucherId
left join tbl_CostClass CC on  TSJ.CostClassId=CC.CostClassId
left join TBL_USER U on TSJ.PostUserId=U.UserId
left join tbl_Currency C on TSJ.CurrencyId=C.CurrencyId where TSJ.TranId=@TranId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
