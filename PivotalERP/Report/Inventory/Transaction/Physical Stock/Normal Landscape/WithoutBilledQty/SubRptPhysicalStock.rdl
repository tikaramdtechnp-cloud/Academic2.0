  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select G.Name as GodownName,P.Name as ProductName,U.Name as UnitName,IDPS.TranId,IDPS.ProductId,IDPS.GodownId,IDPS.Batch,IDPS.MFGDate,IDPS.EXPDate,IDPS.ActualQty,IDPS.BilledQty,IDPS.Rate,IDPS.UnitId,IDPS.Amount,IDPS.DiscountPer,IDPS.DiscountAmt from tbl_ItemDetailsPhysicalStock IDPS left join tbl_Product P on IDPS.ProductId=P.ProductId left join TBL_UNIT U on IDPS.UnitId=U.UnitId left join TBL_GODOWN G on IDPS.GodownId=G.GodownId where IDPS.TranId=@TranId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
