  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select IGP.PurchaseInvoiceTranId,IG.AutoManualNo as IGNo,TP.VoucherDate,TP.AutoManualNo as RefNo,API.Suppliers,API.Address,API.SalesTaxNo,TP.No,API.PurchaseOrderTranId from tbl_IGPurchaseInvoiceDetails IGP inner join tbl_TransactionPurchaseInvoice TP on TP.TranId=IGP.PurchaseInvoiceTranId left join tbl_AditionalDetailsPurchaseInvoice API on TP.TranId=API.TranId left join tbl_TransactionPurchaseOrder PO on PO.TranId=API.PurchaseOrderTranId inner join tbl_TransactionIG IG on IG.TranId=IGP.TranId   where IGP.TranId=@TranId
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
