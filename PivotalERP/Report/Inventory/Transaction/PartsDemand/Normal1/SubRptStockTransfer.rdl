  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select P.Name,P.Alias,P.Code,P.[Description],P.PartNo,P.PartNoAlias,P.Remarks,SI.ActualQty as Qty,SI.Rate,SI.Amount,U.Name as Unit,U.NOOfDecimalPlaces
from tbl_SourceItemDetailsPartsDemand SI
inner join tbl_Product P on P.ProductId=SI.ProductId
left join tbl_Unit U on U.UnitId=SI.UnitId
where TranId=@TranId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
