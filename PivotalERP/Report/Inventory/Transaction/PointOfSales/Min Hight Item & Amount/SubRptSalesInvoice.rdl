  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select IASI.ItemAllocationId,IASI.TranId,IASI.ProductId,IASI.ActualQty,IASI.BilledQty,IASI.Rate,IASI.UnitId,U.Name as UnitName,P.Name as ProductName,IASI.Amount,IASI.DiscountPer,IASI.DiscountAmt from tbl_ItemAllocationSalesInvoice IASI left join tbl_Product P on IASI.ProductId=P.ProductId left join TBL_UNIT U on P.BaseUnitId=U.UnitId where IASI.TranId=@TranId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
