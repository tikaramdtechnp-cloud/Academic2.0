  <Code >Public Shared Function GradeStr(ByVal gpa As Double) As String
    if gpa&gt;=3.61 and gpa&lt;=4 then
	return "A+"
    else if gpa&gt;=3.21 and gpa&lt;=3.60 then
	return "A"
    else if gpa&gt;=2.81 and gpa&lt;=3.2 then
	return "B+"
    else if gpa&gt;=2.41 and gpa&lt;=2.8 then
	return "B"
    else if gpa&gt;=2.01 and gpa&lt;=2.4 then
	return "C+"
    else if gpa&gt;=1.61 and gpa&lt;=2 then
	return "C"
    else if gpa&gt;=1.21 and gpa&lt;=1.6 then
	return "D+"
    else if gpa&gt;=0.81 and gpa&lt;=1.2 then
	return "D"
    else if gpa&gt;=0 and gpa&lt;=0.8 then
	return "E"
    end if

    return ""
End Function

Public Shared Function GradeStrFromPer(ByVal gpa As Double) As String
    if gpa&gt;=90 and gpa&lt;=100 then
	return "A+"
    else if gpa&gt;=80 and gpa&lt;=89.99 then
	return "A"
    else if gpa&gt;=70 and gpa&lt;=79.99 then
	return "B+"
    else if gpa&gt;=60 and gpa&lt;=69.99 then
	return "B"
    else if gpa&gt;=50 and gpa&lt;=59.99 then
	return "C+"
    else if gpa&gt;=40 and gpa&lt;=49.99 then
	return "C"
    else if gpa&gt;=30 and gpa&lt;=39.99 then
	return "D+"
    else if gpa&gt;=20 and gpa&lt;=29.99 then
	return "D"
    else if gpa&gt;=1 and gpa&lt;=19.99 then
	return "E"
    end if

    return ""
End Function</Code>
