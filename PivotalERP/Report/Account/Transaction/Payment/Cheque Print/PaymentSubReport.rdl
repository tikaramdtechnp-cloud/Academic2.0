  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select LAP.LedgerAllocationId,LAP.TranId,LAP.LedgerId,L.Name ,LAP.AgentId,LAP.DrCr,LAP.DrAmount,LAP.CrAmount,LAP.Narration,A.Name as AgentName,CC.Name as CostCenter  from tbl_LedgerAllocationPayment LAP left join tbl_Ledger L on LAP.LedgerId=L.LedgerId left join tbl_Agent A on LAP.AgentId=A.AgentId left join tbl_CostCenterDetailPayment CCDP on LAP.LedgerAllocationId=CCDP.LedgerAllocationId left join tbl_costcenter CC on CCDP.CostCenterId=CC.CostCenterId  where LAP.TranId=@TranId and LAP.DrCr=1 
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
