  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select TP.TranId,TP.AutoVoucherNo,LAP.LedgerId,L.Name as LedgerName,TP.CostClassId,TP.VoucherDate,TP.Narration,TP.IsCancel,TP.RefNo,TP.IsPost,TP.PostDateTime,TP.PostBy,TP.PostUserId,TP.NY,TP.NM,TP.ND,TP.CurrencyId,TP.CurRate,TP.AutoManualNo from tbl_TransactionPayment TP left join tbl_LedgerAllocationPayment LAP on TP.TranId=LAP.TranId left join tbl_Ledger L on LAP.LedgerId=L.LedgerId where LAP.TranId=@TranId and  LAP.DrCr=2
</CommandText>
        <QueryParameters>
