  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select LAJ.LedgerAllocationId,LAJ.TranId,LAJ.LedgerId,L.Name ,LAJ.AgentId,LAJ.LFNO,LAJ.DrCr,LAJ.DrAmount,LAJ.CrAmount,LAJ.Narration,A.Name as AgentName  from tbl_LedgerAllocationJournal LAJ left join tbl_Ledger L on LAJ.LedgerId=L.LedgerId left join tbl_Agent A on LAJ.AgentId=A.AgentId where LAJ.TranId=@TranId  order by LAJ.LedgerAllocationId
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TranId">
            <Value>=Parameters!TranId</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
