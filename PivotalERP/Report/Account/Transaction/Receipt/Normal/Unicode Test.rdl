      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select TR.TranId,TR.AutoVoucherNo,LAR.LedgerId,L.Name as LedgerName,TR.CostClassId,TR.VoucherDate,TR.Narration,TR.IsCancel,TR.RefNo,TR.IsPost,TR.PostDateTime,TR.PostBy,TR.PostUserId,TR.NY,TR.NM,TR.ND,TR.CurrencyId,TR.CurRate,TR.AutoManualNo from tbl_TransactionReceipt TR left join tbl_LedgerAllocationReceipt LAR on TR.TranId=LAR.TranId left join tbl_Ledger L on LAR.LedgerId=L.LedgerId where LAR.TranId=@TranId and  LAR.DrCr=1
</CommandText>
        <QueryParameters>
