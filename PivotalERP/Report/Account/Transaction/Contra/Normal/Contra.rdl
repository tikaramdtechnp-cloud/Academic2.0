        <CommandText>select TC.TranId,TC.AutoVoucherNo,LAC.LedgerId,L.Name as LedgerName,TC.CostClassId,TC.VoucherDate,TC.Narration,TC.IsCancel,TC.RefNo,TC.IsPost,TC.PostDateTime,TC.PostBy,TC.PostUserId,TC.NY,TC.NM,TC.ND,TC.CurrencyId,TC.CurRate,TC.AutoManualNo from tbl_TransactionContra TC left join tbl_LedgerAllocationContra LAC on TC.TranId=LAC.TranId left join tbl_Ledger L on LAC.LedgerId=L.LedgerId where LAC.TranId=@TranId and LAC.DrCr=1
</CommandText>
