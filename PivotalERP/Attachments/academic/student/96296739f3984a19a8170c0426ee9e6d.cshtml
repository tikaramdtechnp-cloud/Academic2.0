@{

    ViewBag.Title = "";
    Layout = null;
}
@RenderPage("~/Views/Global/_GlobalLayout.cshtml")
<script src="~/jslib/inventory/transaction/StockTransfor.js?v=20230718"></script>
<script>
    var VoucherType = @ViewBag.VoucherType;
    var EntityId = @ViewBag.EntityId;
</script>

<div ng-app="myApp" ng-controller="StockTransfor">
    <div class="sale-page-wrap">
        <div class="overflow-hidden">
            @RenderPage("~/Views/Global/_WaitDialog.cshtml")
            @RenderPage("~/Views/Global/_LedgerDetails.cshtml")

            <!-- Page Content  -->
            <div class="content-wrapper-new">
                <div class="card mt-2 bodypart">
                    <div class="demo-container">
                        <div class="demo-header">
                            <div class="card-header py-1 header-corner-triangle overflow-hidden" ng-hide="HideShow.CostClass==true && HideShow.VoucherType==true">
                                <h3 class="card-title mb-0">{{SelectedVoucher && SelectedVoucher.VoucherName ? SelectedVoucher.VoucherName : 'Stock Journal'}}</h3>
                            </div>
                            <div class="card-body py-0 px-4">
                                <div class="row mt-1 ">
                                    <div class="col-sm-4" ng-hide="HideShow.VoucherType==true">
                                        <div class="form-group row">
                                            <label for="inputEmail3" class="col-sm-4 col-form-label">Voucher Type<span style="color:red">*</span></label>
                                            <div class="col-sm-8">
                                                <select class="form-control form-control-sm hideSideBar" style="width: 100%;" ng-model="beData.VoucherId" ng-change="getVoucherNo()">
                                                    <option ng-repeat="vt in VoucherTypeColl track by vt.VoucherId" ng-value="vt.VoucherId" ng-selected="vt.VoucherId == beData.VoucherId">
                                                        {{vt.VoucherName}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group d-flex">
                                            <label style="min-width:70px;">Ref No<span style="color:red">*</span></label>
                                            <input type="text" class="form-control form-control-sm" />
                                        </div>
                                    </div>
                                    <div class="col-sm-2" ng-hide="HideShow.VoucherType==true">
                                        <div class="form-group row">
                                            <div class="custom-control custom-checkbox">
                                                <input class="custom-control-input" type="checkbox"
                                                       id="customCheckbox1" value="option1">
                                                <label for="customCheckbox1"
                                                       class="custom-control-label">
                                                    Is Fixed Product
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3" @*ng-hide="HideShow.CostClass==true"*@>
                                        <div class="form-group row">
                                            <label for="inputEmail3" class="col-sm-5 col-form-label">For Year<span style="color:red">*</span></label>
                                            <div class="col-sm-7">
                                                <select class="form-control form-control-sm hideSideBar" style="width: 100%;" ng-model="beData.CostClassId" ng-change="getVoucherNo()">
                                                    <option ng-repeat="cc in CostClassColl track by cc.CostClassId" ng-value="cc.CostClassId" ng-selected="cc.CostClassId == beData.CostClassId">
                                                        {{cc.Name}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-2">
                                        <div class="form-group row">
                                            <label>Voucher No.</label>
                                            <input type="text" class="form-control form-control-sm" placeholder="" ng-model="beData.AutoManualNo" readonly ng-hide="HideShow.AutoVoucherNo==true">

                                        </div>
                                    </div>

                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label>From Godown</label>
                                            <select class="form-control form-control-sm select2" ng-model="beData.SourceGodownId">
                                                <option ng-value="at.GodownId" ng-repeat="at in GodownColl" ng-selected="at.GodownId == beData.SourceGodownId">
                                                    {{at.Name}}
                                                </option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="form-group">
                                            <label>To Godown</label>
                                            <select class="form-control form-control-sm select2" ng-model="beData.TargetGodownId" @*ng-change="GodownSelectionChange()"*@>
                                                <option ng-value="at.GodownId" ng-repeat="at in ToGodownColl" ng-selected="at.GodownId == beData.TargetGodownId">
                                                    {{at.Name}}
                                                </option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label>Brand</label>
                                            <select class="select2 form-control form-control-sm" ng-model="beData.ProductBrandId" data-placeholder="**Select Brand**">
                                                <option ng-value="at.ProductBrandId" ng-repeat="at in ProductBrandColl" ng-selected="at.ProductBrandId == beData.ProductBrandId">
                                                    {{at.Name}}
                                                </option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label>{{SelectedVoucher.VoucherDateLabel ? SelectedVoucher.VoucherDateLabel : 'Transfer Date'}}</label>
                                            <input type="text" class="form-control form-control-sm float-right serial hideSideBar" name="VoucherDate" date-picker ng-model="beData.VoucherDate_TMP" date-detail="beData.VoucherDateDet" />
                                        </div>
                                    </div>

                                    @*<div class="col-sm-2">
                                            <div class="form-group">
                                                <label>Code</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="beData.Code" />
                                            </div>
                                        </div>*@

                                </div>
                            </div>
                        </div>
                        <div class="table-scroll-x-y  table-container">
                            <table id="main-table" class="main-table table table-sm table-bordered">
                                <thead>
                                    <tr>
                                        <th rowspan="2" class="text-center align-middle cstm-sticky" style="width: 50px; left: 0; z-index: 3;background-color:#ececec!important;">S.N.</th>
                                        <th rowspan="2" class="text-center align-middle cstm-sticky" style="width: 300px; left: 50px; z-index: 3; background-color: #ececec!important;">Particulars</th>
                                        <th ng-hide="HideShow.ProductDescription==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2;background-color:#ececec!important;">Description</th>
                                        <th ng-hide="HideShow.ProductLedger==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2;background-color:#ececec!important;">Sales Ledger</th>
                                        <th ng-hide="HideShow.Batch==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">Batch No.</th>
                                        <th ng-hide="HideShow.MFGDate==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">MFG.Date</th>
                                        <th ng-hide="HideShow.EXPDate==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">EXP.Date</th>

                                        <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 80px; z-index: 2;background-color:#ececec!important;">Weight<br />(gm)</th>
                                        <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 80px; z-index: 2;background-color:#ececec!important;">Loss<br />(gm)</th>

                                        <th colspan="{{(HideShow.BilledQty==true ? 2 : 3 )}}" scope="colgroup" class="text-center" style="width: 250px; z-index: 2;background-color:#ececec!important;">{{comDet.Maintain==2 ? 'Total' :'Quantity' }}</th>
                                        <th ng-hide="HideShow.AlternetUnit1==true" colspan="2" scope="colgroup" class="text-center" style="width: 170px; z-index: 2;background-color:#ececec!important;">Alt.Unit 1</th>
                                        <th ng-hide="HideShow.AlternetUnit2==true" colspan="2" scope="colgroup" class="text-center" style="width: 170px; z-index: 2;background-color:#ececec!important;">Alt.Unit 2</th>
                                        <th rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Rate</th>
                                        <th ng-hide="HideShow.Discount==true" colspan="2" scope="colgroup" class="text-center" style="width: 180px; z-index: 2;background-color:#ececec!important;">Discount</th>
                                        <th ng-hide="HideShow.Amount==true" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Amount</th>

                                        <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Making</th>
                                        <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Stone/Extra</th>
                                        <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Total</th>

                                        <th ng-hide="HideShow.ExciseDuty==true" rowspan="2" class="text-center align-middle" style="width: 100px;background-color:#ececec!important;">Excise Duty</th>
                                        <th ng-hide="HideShow.Vat==true" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Taxable</th>
                                        <th ng-hide="HideShow.Vat==true" rowspan="2" class="text-center align-middle" style="width: 80px;background-color:#ececec!important;">VAT</th>
                                        <th ng-hide="HideShow.EachNarration==true" rowspan="2" class="text-center align-middle" style="width: 250px;background-color:#ececec!important;">Narration</th>
                                        <th rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Action</th>
                                    </tr>
                                    <tr>
                                        <th scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">{{comDet.Maintain==2 ? 'Weight' :'Actual' }}</th>
                                        <th ng-hide="HideShow.BilledQty==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Billed</th>
                                        <th scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                        <th ng-hide="HideShow.AlternetUnit1==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Qty.</th>
                                        <th ng-hide="HideShow.AlternetUnit1==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                        <th ng-hide="HideShow.AlternetUnit2==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Qty.</th>
                                        <th ng-hide="HideShow.AlternetUnit2==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                        <th ng-hide="HideShow.Discount==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Per(%)</th>
                                        <th ng-hide="HideShow.Discount==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Amount</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="itemDet in beData.SourceItemDetailsColl">
                                        <th class="align-middle cstm-sticky" style="width: 50px; left: 0; z-index: 2; background-color: #fff">{{$index+1}}</th>
                                        <td style="width: 250px; z-index: 2; left: 50px; background-color: #fff">
                                            <select autoShowHide="true" class="form-control form-control-sm select2 serial" all-product ng-model="itemDet.ProductId" style="width:100%"
                                                    placeholder="** Select Product  **" showdetails="true" confirm-action="ProductSelectionChange(itemDet,$index)" product-detail="itemDet.productDetail" side-bar-data="itemDet.sideBarData" voucher-id="SelectedVoucher.VoucherId" vouchertype="19" godown-id="beData.SourceGodownId"></select>
                                        </td>
                                        <td ng-hide="HideShow.ProductDescription==true"><input type="text" class="form-control form-control-sm" ng-model="itemDet.Description"></td>
                                        <td style="z-index: 1;" ng-hide="HideShow.ProductLedger==true">
                                            <select ng-disabled="itemDet.AutoCharge==true" class="form-control form-control-sm select2bs4" active-select2 style="width: 100%; background-color: #fff" ng-model="itemDet.LedgerId" data-placeholder="**Select Sales Ledger**">
                                                <option ng-value="sl.LedgerId" ng-repeat="sl in SalesLedgerColl" ng-selected="itemDet.LedgerId==sl.LedgerId">{{sl.Name}}</option>
                                            </select>
                                        </td>
                                        <td ng-hide="HideShow.Batch==true">
                                            <select class="form-control form-control-sm select2bs4" active-select2 style="width: 100%; background-color: #fff" ng-model="itemDet.Batch" data-placeholder="**Select Batch**" ng-change="ChangeBatch(itemDet)">
                                                <option ng-value="bc.BatchNo" ng-repeat="bc in itemDet.productDetail.BatchWiseColl" ng-selected="itemDet.Batch==bc.BatchNo">{{bc.BatchNo}}-ED:{{bc.EXPDate |expFormat }}({{bc.BalQty |number:2}})</option>
                                            </select>
                                        </td>
                                        <td ng-hide="HideShow.MFGDate==true">
                                            <input exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.MFGDate" date-detail="itemDet.MFGDateDet" readonly>
                                        </td>
                                        <td ng-hide="HideShow.EXPDate==true">
                                            <input exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.EXPDate" date-detail="itemDet.EXPDateDet" readonly>
                                        </td>
                                        <td ng-show="comDet.Maintain==2">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="NetWeight" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'netWeight')" positive="true" noofdecimal="itemDet.QtyDecimal" ng-model="itemDet.NetWeight">
                                        </td>
                                        <td ng-show="comDet.Maintain==2">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="LossWeight" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'lossWeight')" positive="true" noofdecimal="itemDet.QtyDecimal" ng-model="itemDet.LossWeight">
                                        </td>
                                        <td>
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="ActualQty" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'aQty')" positive="true" noofdecimal="itemDet.QtyDecimal" ng-model="itemDet.ActualQty" ng-style="itemDet.IsNegativeQty==true && { 'color' : 'red' }" ng-disabled="itemDet.ReadOnlyQty==true">
                                        </td>
                                        <td ng-hide="HideShow.BilledQty==true">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="ActualQty" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'bQty')" positive="true" noofdecimal="itemDet.QtyDecimal" ng-model="itemDet.BilledQty" ng-style="itemDet.IsNegativeQty==true && { 'color' : 'red' }" ng-disabled="itemDet.ReadOnlyQty==true">
                                        </td>
                                        <td><input type="text" class="form-control  form-control-sm" ng-model="itemDet.UnitName" disabled></td>
                                        <td ng-hide="HideShow.AlternetUnit1==true">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" name="Rate" onfocus="this.select();" ng-model="itemDet.ALValue1" ng-change="ChangeItemAlternetQty(itemDet,'unit1')" ng-disabled="itemDet.ReadOnlyQty==true">
                                        </td>
                                        <td ng-hide="HideShow.AlternetUnit1==true">{{itemDet.UnitName1}}</td>
                                        <td ng-hide="HideShow.AlternetUnit2==true">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" name="Rate" onfocus="this.select();" ng-model="itemDet.ALValue2" ng-change="ChangeItemAlternetQty(itemDet,'unit2')" ng-disabled="itemDet.ReadOnlyQty==true">
                                        </td>
                                        <td ng-hide="HideShow.AlternetUnit2==true">{{itemDet.UnitName2}}</td>

                                        <td><input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" name="Rate" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'rate')" noofdecimal="itemDet.RateDecimal" ng-model="itemDet.Rate" ng-readonly="itemDet.CanEditRate==false" positive="true"></td>

                                        <td ng-hide="HideShow.Discount==true"><input type="text" class="form-control  form-control-sm" style="text-align:center" number-input ng-model="itemDet.DiscountPer" ng-change="ChangeItemRowValue(itemDet,'disPer')" positive="true"></td>
                                        <td ng-hide="HideShow.Discount==true"><input type="text" class="form-control form-control-sm" style="text-align:right" number-input ng-model="itemDet.DiscountAmt" ng-change="ChangeItemRowValue(itemDet,'disAmt')" positive="true"></td>
                                        <td ng-hide="HideShow.Amount==true"><input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" onfocus="this.select();" name="Amount" ng-model="itemDet.Amount" noofdecimal="itemDet.AmountDecimal" ng-disabled="SelectedVoucher.CanEditProductAmount==false" ng-change="ChangeItemRowValue(itemDet,'amt')"></td>

                                        <td ng-show="comDet.Maintain==2">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="LossWeight" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'makeing')" positive="true" noofdecimal="itemDet.AmountDecimal" ng-model="itemDet.Makeing">
                                        </td>
                                        <td ng-show="comDet.Maintain==2">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="LossWeight" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'stone')" positive="true" noofdecimal="itemDet.AmountDecimal" ng-model="itemDet.Stone">
                                        </td>
                                        <td ng-show="comDet.Maintain==2">
                                            <input type="text" number-input class="form-control form-control-sm serial hideSideBar" name="LossWeight" style="text-align:center" onfocus="this.select();" positive="true" noofdecimal="itemDet.AmountDecimal" ng-model="itemDet.TotalAmount" disabled>
                                        </td>

                                        <td ng-hide="HideShow.ExciseDuty==true"><input type="text" class="form-control form-control-sm" number-input ng-model="itemDet.ExDutyAmount" positive="true"></td>
                                        <td ng-hide="HideShow.Vat==true"><input type="text" class="form-control form-control-sm" number-input ng-model="itemDet.TaxableAmt" positive="true"></td>
                                        <td ng-hide="HideShow.Vat==true"><input type="text" class="form-control form-control-sm" number-input ng-model="itemDet.VatAmount" positive="true"></td>
                                        <td ng-hide="HideShow.EachNarration==true"><input type="text" class="form-control form-control-sm" ng-model="itemDet.Narration"></td>
                                        <td class="project-actions no-print text-center">
                                            <div class="btn-group-sm">
                                                <a class="btn btn-info" ng-click="AddRowInSourceItemDetails($index,true)"><i class="fas fa-plus"></i></a>
                                                <a class="btn btn-danger" ng-click="delRowFromSourceItemDetails($index)"><i class="fas fa-trash"></i></a>
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!-- /.container-fluid -->
    </div>

    <footer class="main-footer control-btns pt-0">
        <div class="secondary-footer mt-2">
            <div class="row">
                <div class="col-md-7 col-xl-8">
                    <div class="form-group">
                        <input type="text" list="lstNarration" class="form-control form-control-sm hideSideBar" name="" id="" rows="3" placeholder="Narration" ng-model="beData.Narration">
                        <datalist id="lstNarration">
                            <option ng-repeat="x1 in NarrationList track by $index" value="{{x1}}">
                        </datalist>
                    </div>
                </div>
                <div class="col-md-5 col-xl-4">
                    <div class="form-group row">
                        <label for="inputEmail3" class="col-5 col-sm-6 col-form-label font-weight-bold text-dark text-right">Grand Total:</label>
                        <div class="col-7 col-sm-6 col-md-6">
                            <input type="text" number-input class="form-control hideSideBar" style="text-align:right;" placeholder="" readonly ng-model="beData.TotalAmount">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-12">
                <form class="form-inline justify-content-end">
                    @*<button type="button" class="btn btn-default ml-1" data-toggle="modal" data-target="#stocktransferdetails">test</button>*@
                    <button type="button" class="btn btn-default ml-1" ng-click="ClearData()">Clear</button>
                    <button type="button" class="btn btn-secondary ml-1" ng-click="SearchData()">Search</button>
                    <button type="button" class="btn btn-primary ml-1" ng-click="SaveStockTransfor()" ng-disabled="loadingstatus=='running'">Save</button>
                </form>
            </div>
        </div>
    </footer>


    <!-- Sales Quotation Details Modal demo 2 -->
    <div class="modal fade bd-example-modal-xl sales-quotation-page-modal" id="frmSalesQuotationDetailsModel" @*tabindex="-1"*@ role="dialog" aria-labelledby="exampleModalCenter2Title" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title" id="exampleModalCenterTitle">Sales Quotation Details</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body gradient-bg">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-lg-5 col-form-label">Ref. Voucher</label>
                                <div class="col-lg-7">
                                    <select class="form-control select2 form-control-sm" style="width: 100%;" ng-model="beData.RefVoucherType" ng-change="RefVoucherChange(beData.RefVoucherType)">
                                        <option selected="selected" value="1">StockTransfer</option>

                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-lg-4 offset-lg-1 col-form-label">Voucher No</label>
                                <div class="col-lg-7">
                                    @*<select ng-model="SelectedRefVoucher" class="form-control input-sm" ng-options="x.text for x in RefVoucherNoColl" id="cboRefVoucherNo" multiple></select>*@
                                    <select ng-model="SelectedRefVoucher" class="form-control input-sm" id="cboRefVoucherNo" multiple>
                                        <option ng-repeat="et in RefVoucherNoColl track by et.id" value="{{et.id}}">
                                            {{et.text}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-body gradient-bg">
                    <div id="table-scroll">
                        <div id="datatable" style="height: calc(55vh - 200px);width: 100%;" class="ag-theme-balham"></div>
                    </div>
                    <div class="sec3">
                        <article>
                            <h6 class="title">Dispatch Details</h6>
                            <div class="row">
