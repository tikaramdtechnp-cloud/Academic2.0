<!--Fixed Product Modal Starts-->
<div class="modal fade bd-example-modal-xl sales-invoice-page-modal" id="frmImportMultipleCopy">
    <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title"> {{MulData.Title}} </h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body py-2">
                <div class="table-scroll-x-y table-scroll" style="height:300px!important;overflow-y:auto">
                    <table class="main-table table table-sm table-bordered bg-white">
                        <thead>
                            <tr>
                                <th class="cstm-sticky" style="left:0px;width: 60px;z-index:3;background-color:#eee!important">S.No.</th>                                
                                <th style="width: 180px; z-index: 2;background-color:#eee!important" ng-repeat="cl in MulData.ColColl">{{cl.label}}</th>                                
                                <th class="text-center align-middle" style="width: 50px; z-index: 2;background-color:#eee!important">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="mdc in MulData.DataColl">
                                <td class="cstm-sticky text-center" style="left:0px;background-color:#ffffff!important;z-index:2">
                                    {{$index+1}}
                                </td>                               
                                <td ng-repeat="cn in MulData.ColColl">                                                                        
                                    <input ng-show="cn.dataType=='Int32' || cn.dataType=='double' || cn.dataType=='Double' " type="text" number-input class="form-control form-control-sm" ng-model="mdc[cn.name]" ng-paste="PasteData(cn.name,$parent.$index)" onpaste="return false" ng-change="CalculateCopyPasteData()" />
                                    <input ng-show="cn.dataType=='String'" type="text" class="form-control form-control-sm" ng-model="mdc[cn.name]" ng-paste="PasteMData(cn.name,$parent.$index)" onpaste="return false" />
                                </td>                                 
                                <td>
                                    <a class="p-1" href="#" data-original-title="Add Row" ng-click="addRowInMD($index)">
                                        <i class="fas fa-plus text-info"></i>
                                    </a>
                                    <a class="p-1" href="#" data-original-title="Delete Row." ng-click="delRowInMD($index)">
                                        <i class="fas fa-trash-alt text-danger"></i>
                                    </a>
                                </td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <th class="cstm-sticky" style="left:0px;width: 60px;z-index:3;background-color:#eee!important"></th>
                                <th style="width: 180px; z-index: 2;background-color:#eee!important" ng-repeat="cl in MulData.ColColl">{{cl.Total}}</th>
                                <th class="text-center align-middle" style="width: 50px; z-index: 2;background-color:#eee!important">Action</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-2 float-right">                        
                        <button type="button" class="btn btn-sm btn-danger" style="min-width: 70px;"  ng-click="SaveMultipleData()">Ok</button>
                    </span>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!--TDS Modal Ends-->