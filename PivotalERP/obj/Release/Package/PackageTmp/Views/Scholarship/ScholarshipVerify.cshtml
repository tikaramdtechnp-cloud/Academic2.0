    <style>

        .stepwizard-step p {
            margin-top: 0px;
            font-size: 20px;
        }

        .stepwizard-row {
            display: table-row;
            width: 100%
        }

        .stepwizard {
            display: table;
            width: 100%;
            position: relative;
        }

        .stepwizard-step button[disabled] {
            opacity: 1 !important; /* For modern browsers */
            filter: none !important; /* For older versions of IE */
        }


        .status-row:before {
            top: 21px;
            bottom: 0;
            position: absolute;
            content: " ";
            width: 70%;
            right: 15%;
            height: 3px;
            background-color: #dee2e6;
            z-index: 0;
        }

        .stepwizard-step {
            display: table-cell;
            text-align: center;
            position: relative;
        }

        .btn-circle {
            width: 39px;
            height: 39px;
            text-align: center;
            padding: 6px 6px;
            font-size: 20px;
            line-height: 1.228571;
            border-radius: 50%;
        }

        /*.btn-primary:not(:disabled):not(.disabled):active, .btn-primary:not(:disabled):not(.disabled).active, .show > .btn-primary.dropdown-toggle {
            color: #ffffff;
            background-color: #28a745 !important;
            border-color: #28a745;
        }*/

        .btn-green {
            background-color: #077A56;
            border-radius: 77px;
            color: #ffffff;
            min-Width: 130px
        }

        .btn-light-green {
            background-color: #EEFFFA;
            border-radius: 77px;
            color: #077A56;
            min-Width: 130px;
            border: 1px solid #077A5633
        }

        .btn-light-red {
            background-color: #FFAAADCC;
            color: #AA0B12;
            border-radius: 77px;
        }


        .input-field.input-outlined > input {
            border: 2px solid #eee;
            padding: 12px 16px 14px;
            width: calc(100% - 2.5rem);
            float: none;
            display: block;
            border-radius: 4px;
            transition: box-shadow, border-color 0.15s;
        }

            .input-field.input-outlined > input:focus:not([readonly]) {
                border-color: #26A69B;
                box-shadow: 0 1px 0 0px #26A69B;
            }

        .input-field.input-outlined > label {
            left: 27px;
            display: inline-flex;
            width: auto !important;
        }

            .input-field.input-outlined > label.active {
                background: white;
                border-left: 4px solid white;
                border-right: 4px solid white;
                transform: translateY(-3.35rem);
                top: 1rem;
            }

        .table td, .table th {
            Padding: 0.2rem 0.3rem !important;
        }

        .rotingtxt {
            -webkit-transform: rotate(331deg);
            -moz-transform: rotate(331deg);
            -o-transform: rotate(331deg);
            transform: rotate(331deg);
            font-size: 6em;
            color: #6cba4429;
            position: absolute;
            font-family: 'Denk One', sans-serif;
            text-transform: uppercase;
            padding: 10%;
            padding-top: 10%;
        }

        .marksheet td, .marksheet th {
            padding: 0.35rem;
            border: none !important;
            vertical-align: top;
            border-right: 2px solid #0000005e !important;
            border-left: 2px solid #0000005e !important;
        }

        @@media (max-width: 577px) {
            .status-row:before {
                top: auto;
                bottom: auto;
                position: static;
                content: none;
                width: auto;
                right: auto;
                height: auto;
                background-color: transparent;
                z-index: auto;
            }

            h3 {
                font-size: 20px
            }

            h5 {
                font-size: 1.2rem;
            }
        }
    </style>
