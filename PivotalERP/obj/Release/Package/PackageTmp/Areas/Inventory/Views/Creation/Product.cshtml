                                                    <th ng-show="FixedProductConfig.ShowChassisNo==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.ChassisNo}}</th>
                                                    <th ng-show="FixedProductConfig.ShowRegdNo==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.RegdNo}}</th>

                                                    <th ng-show="FixedProductConfig.ShowModel==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.Model}}</th>
                                                    <th ng-show="FixedProductConfig.ShowType==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.Type}}</th>
                                                    <th ng-show="FixedProductConfig.ShowColor==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.Color}}</th>
                                                    <th ng-show="FixedProductConfig.ShowKeyNo==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.KeyNo}}</th>
                                                    <th ng-show="FixedProductConfig.ShowCodeNo==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.CodeNo}}</th>
                                                    <th ng-show="FixedProductConfig.ShowMFGYear==true && newProduct.IsFixedProduct==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">{{FixedProductConfig.MFGYear}}</th>

                                                    <th ng-show="newProduct.MaintainBatchWise==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">Batch No.</th>
                                                    <th ng-show="newProduct.UseMfgDate==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">MFg. Date</th>
                                                    <th ng-show="newProduct.UseExpDate==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;">Exp. Date</th>

                                                    <th style="width: 100px">Quantity</th>
                                                    <th style="width: 90px">Unit</th>
                                                    <th style="width: 90px">Rate</th>
                                                    <th style="width: 90px">Amount</th>
                                                    <th class="text-center" style="width: 80px">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="op in newProduct.OpeningColl">
                                                    <td>
                                                        <select class="form-control form-control-sm select2" ng-model="op.GodownId">
                                                            <option ng-value="g.GodownId" ng-repeat="g in GodownList" ng-selected="op.GodownId==g.GodownId">{{g.Name}}</option>
                                                        </select>
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowEngineNo==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.EngineNo">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowChassisNo==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.ChassisNo">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowRegdNo==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.RegdNo">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowModel=true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.ShowModel">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowType==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.Type">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowColor==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.Color">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowKeyNo==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.KeyNo">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowCodeNo==true && newProduct.IsFixedProduct==true">
                                                        <input type="text" class="form-control form-control-sm" ng-model="op.CodeNo">
                                                    </td>
                                                    <td ng-show="FixedProductConfig.ShowMFGYear==true && newProduct.IsFixedProduct==true">
                                                        <input type="number" class="form-control form-control-sm" ng-model="op.MFGYear">
                                                    </td>
                                                    <td ng-show="newProduct.MaintainBatchWise==true">
                                                        <input type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="op.Batch">
                                                    </td>
                                                    <td ng-show="newProduct.UseMfgDate==true">
                                                        <input exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="op.MFGDate_TMP" date-detail="op.MFGDateDet">
                                                    </td>
                                                    <td ng-show="newProduct.UseExpDate==true">
                                                        <input exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="op.EXPDate_TMP" date-detail="op.EXPDateDet">
                                                    </td>

                                                    <td><input type="text" number-input class="form-control form-control-sm" ng-model="op.Quantity" noofdecimal="newProduct.QtyDecimal" ng-change="ChangeOpeningQty(1,op)" positive="true"></td>
                                                    <td>
                                                        <input type="text" class="form-control form-control-sm" ng-model="newProduct.BaseUnitName" readonly>
                                                    </td>
                                                    <td><input type="text" number-input class="form-control form-control-sm" ng-model="op.Rate" noofdecimal="newProduct.RateDecimal" ng-change="ChangeOpeningQty(2,op)" positive="true"></td>
                                                    <td><input type="text" number-input class="form-control form-control-sm" ng-model="op.Amount" noofdecimal="newProduct.AmountDecimal" ng-change="ChangeOpeningQty(3,op)" positive="true"></td>
                                                    <td>
                                                        <a class="" ng-click="AddOpening(op,$index)">
                                                            <i class="fas fa-plus text-info"></i>
                                                        </a>
                                                        <a class="p-1" ng-click="delOpening($index)">
                                                            <i class="fas fa-trash-alt text-danger"></i>
                                                        </a>
                                                    </td>
                                                </tr>

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Set GodownWise Opening Ends-->
                    </div>
                </div>

                <footer class="text-center mb-4">
                    <div class="control-btns">
                        <a href="~/Inventory/Reporting/Product" class="btn btn-info btn-sm my-1" style="min-width: 90px;">List</a>
                        <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="SearchData()">Search</button>
                        <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearProduct()">Cancel</button>
                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateProduct()">Save</button>
                    </div>
                    
                </footer>
            </section>
            <!-- Main Footer -->


            <div class="modal fade sales-quotation-page-modal document-attach-modal" id="searVoucherRightBtn" tabindex="-1" role="dialog" aria-labelledby="partyAccRightBtn" aria-hidden="true">
                <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h6 class="modal-title" id="exampleModalCenterTitle">Search Product</h6>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body gradient-bg">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group row">
                                        <label for="" class="col-sm-2 col-form-label col-form-label-sm text-center">Search By</label>
                                        <div class="col-sm-10">
                                            <div class="input-group input-group-sm mb-3">
                                                <div class="input-group-prepend">
                                                    <select class="form-control form-control-sm select2bs4" style="width: 100%;" ng-model="paginationOptions.SearchColDet">
                                                        <option ng-value="asp" ng-repeat="asp in VoucherSearchOptions" ng-selected="asp.text==paginationOptions.SearchColDet.text">{{asp.text}}</option>
                                                    </select>
                                                </div>
                                                <input type="text" class="form-control" placeholder="" ng-model="paginationOptions.SearchVal" ng-change="ReSearchData(-1)">
                                                <div class="input-group-append">
                                                    <div class="input-group-text">
                                                        <span class="fas fa-search" ng-click="ReSearchData(-1)"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <form class="float-right">
                                        <div class="form-row align-items-center">
                                            <div class="d-flex align-items-center">
                                                <label class="mr-sm-2 mb-0" for="inlineFormCustomSelect">Show</label>
                                                <input type="text" ng-model="paginationOptions.pageSize" class="form-control input-sm" placeholder="NoOfRows" @*ng-blur="ReSearchData(-2)"*@>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="">
                                <table id="main-table" class="main-table table table-sm table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Alias</th>
                                            <th>Code</th>
                                            <th>Group</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr dir-paginate="sd in SearchDataColl |orderBy:sortKey:reverse|itemsPerPage:paginationOptions.pageSize" total-items="paginationOptions.TotalRows">
                                            <td>{{sd.Name}}</td>
                                            <td>{{sd.Alias}}</td>
                                            <td>{{sd.Code}}</td>
                                            <td>{{sd.GroupName}}</td>
                                            <td>
                                                <a class="p-1" href="#" ng-click="GetProductById(sd)">
                                                    <i class="fas fa-edit text-info"></i>
                                                </a>
                                                <a class="p-1" href="#" ng-click="DelProductById(sd)">
                                                    <i class="fas fa-trash-alt text-danger"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>

                                </table>
                                <dir-pagination-controls max-size="paginationOptions.pageSize" direction-links="true" boundary-links="true"
                                                         on-page-change="ReSearchData(newPageNumber)">
                                </dir-pagination-controls>
                            </div>

                        </div>
                        <!-- end modal body -->
                        <div class="modal-footer justify-content-between align-items-center py-2">
                            <h6 class="mb-0">Total Record: {{paginationOptions.TotalRows}}</h6>
                            <div class="">
                                <button type="button" class="btn btn-sm btn-outline-secondary" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
</section>
</div>

        <script>
            te = document.querySelector('textarea');
            te.addEventListener('keyup', resizeTextarea);

            function resizeTextarea(ev) {
                this.style.height = '20px';
                this.style.height = this.scrollHeight + 12 + 'px';
            }
        </script>



