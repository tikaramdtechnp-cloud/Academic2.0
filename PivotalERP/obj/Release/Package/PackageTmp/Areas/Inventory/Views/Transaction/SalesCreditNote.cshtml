                                                    <label for="inputEmail3" style="min-width:120px;">Cost Center</label>
                                                    <select autoShowHide="true" class="select2 serial form-control form-control-sm" all-cost-center ng-model="beData.TranCostCenter" id="cboTranCostCenter" style="width:95%"
