                                                <th ng-hide="HideShow.ProductWiseTaxable==true" rowspan="2" class="text-center align-middle text-truncate" title="IsTaxable" style="width: 90px; background-color: #ececec !important;">IsTaxable</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 10 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 0 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th rowspan="2" class="text-center align-middle" style="width: 90px; background-color: #ececec !important;">Action</th>
                                            </tr>
                                            <tr>
                                                <th scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2; background-color: #ececec !important;">Actual</th>
                                                <th ng-hide="HideShow.BilledQty==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2; background-color: #ececec !important;">Billed</th>
