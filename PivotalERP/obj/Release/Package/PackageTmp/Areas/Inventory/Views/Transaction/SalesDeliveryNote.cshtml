                                                    <div class="col-sm-8">
                                                        <div class="form-group d-flex">
                                                            <label for="inputEmail3" style="min-width: 120px;">Currency</label>
                                                            <select class=" form-control form-control-sm" style="width: 100%;" ng-model="beData.CurrencyId">
                                                                <option ng-repeat="curC in CurrencyColl" ng-value="curC.CurrencyId" ng-selected="curC.CurrencyId == beData.CurrencyId">
                                                                    {{curC.Name}}
                                                                </option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-4">
                                                        <div class="form-group d-flex">
                                                            <label for="inputEmail3" class="mr-2">Rate</label>
                                                            <input type="number" class="form-control form-control-sm serial" placeholder="" ng-model="beData.CurRate" onfocus="this.select();">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row" ng-show="SelectedVoucher.ActiveUDF==true">
                                            <div class="col-sm-3" ng-repeat="uf in beData.UDFFeildsColl | filter: { FieldAfter: 4 }:true">
                                                <div class="form-group d-flex">
                                                    <label style="min-width: 120px;">{{uf.Name}} <span style="color:red" ng-show="uf.IsMandatory==true">*</span></label>
                                                    <udf-field udf="uf" voucher="SelectedVoucher"></udf-field>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- end form fiels -->
                                <div class="table-scroll-x-y mb-1 table-container mb-1">
                                    <table id="main-table" class="main-table table table-sm table-bordered bg-white">
                                        <thead>
                                            <tr>
                                                <th rowspan="2" class="text-center align-middle cstm-sticky" style="width: 50px; left: 0; z-index: 3; background-color: #ececec !important;">S.N.</th>
                                                <th rowspan="2" class="text-center align-middle cstm-sticky" style="width: 300px; left: 50px; z-index: 3; background-color: #ececec !important;">Particulars</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 1 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.ProductDescription==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2; background-color: #ececec !important;">Description</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 2 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.ProductLedger==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2; background-color: #ececec !important;">Sales Ledger</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 3 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.Godown==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2; background-color: #ececec !important;">Godown</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 4 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.CurrentBalance==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2; background-color: #ececec !important;">Closing Stock</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 5 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.Batch==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.Product.BatchLabel ? SelectedVoucher.Product.BatchLabel : 'Batch No.' }}</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 6 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowEngineNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.EngineNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowChassisNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.ChassisNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowRegdNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.RegdNo}}</th>

                                                <th ng-show="SelectedVoucher.FixedProduct.ShowMode==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.Model}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowType==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.Type}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowColor==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.Color}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowKeyNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.KeyNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowCodeNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.CodeNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowMFGYear==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2; background-color: #ececec !important;">{{SelectedVoucher.FixedProduct.MFGYear}}</th>
                                                <th colspan="{{(HideShow.BilledQty==true ? 2 : 3 )}}" scope="colgroup" class="text-center" style="width: 250px; z-index: 2; background-color: #ececec !important;">Quantity</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 7 }:true" rowspan="2" class="text-center align-middle" ng-style="{ 'width': ud.ColWidth+'px' };" style="background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-hide="HideShow.FreeQty==true" rowspan="2" class="text-center align-middle" style="width: 90px; background-color: #ececec !important;">Free Qty.</th>
                                                <th ng-hide="HideShow.AlternetUnit1==true" colspan="2" scope="colgroup" class="text-center" style="width: 180px; z-index: 2; background-color: #ececec !important;">Alt.Unit 1</th>
