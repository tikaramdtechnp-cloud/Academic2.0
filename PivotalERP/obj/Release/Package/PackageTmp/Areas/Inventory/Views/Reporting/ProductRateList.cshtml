
@{

    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/ag-grid-community.js"></script>
<link href="~/Scripts/ag-grid.css" rel="stylesheet" />
<link href="~/Scripts/ag-theme-balham.css" rel="stylesheet" />
<script src="~/jslib/main.js"></script>
<script src="~/jslib/inventory/reporting/ProductRateList.js?v=20251012"></script>
<style>
    .third-level-scroll-only {
        height: 100px !important;
        overflow: unset;
    }
</style>
<div ng-app="app" ng-controller="ProductRateListCtrl" ng-class="{waiting: loadingstatus=='running'}" ng-init="GetAllProductRateList()">
    @RenderPage("~/Views/Global/_WaitDialog.cshtml")
    <!-- Main content -->
    <div class="content  third-level-scroll-only">
        <!-- Page Content  -->
        <div class="container-fluid">
            <div class="container-fluid" style="width:100%;">
                <div class="card overflow-hidden">
                    <div class="card-header header-corner-triangle border">
                        <form action="">



                            <div class="row">
                                <div class="col-sm-11" style="font-size:10px;">


                                </div>
                                <div class="col-sm-1">
                                    <div class="form-group">


                                        <a href="#" class="btn btn-primary btn-sm" style="float: right; max-width: 100px" data-toggle="tooltip" data-placement="top" title="" data-original-title="Load" ng-click="GetAllProductRateList()" ng-disabled="loadingstatus=='running'"><i class="fas fa-download">Load</i></a>

                                    </div>
                                </div>

                            </div>


                        </form>


                    </div>

                </div>

            </div>

            <div class="row">
                <div class="col-md-12">
                    <div id="datatable" style="height: calc(100vh - 200px);width: 100%;" class="ag-theme-balham"></div>
                </div>
            </div>




        </div>
        <!-- /.container-fluid -->
    </div>



    <footer class="main-footer control-btns" @*style="height:50px"*@>

        <div class="row">


            <div class="col-sm-3" style="font-size:12px;">
                <div class="form-group row">
                    <label for="inputEmail3" class="col-sm-3 col-form-label">Find</label>
                    <div class="col-sm-9">
                        <input type="text" name="table_search" class="form-control form-control-sm" placeholder="Search" ng-model="search" ng-change="onFilterTextBoxChanged()">
                    </div>
                </div>
            </div>

            <div class="col-sm-3">


            </div>
            <div class="col-sm-6 text-right">

                <button type="button" class="btn-secondary btn-sm" ng-disabled="loadingstatus=='running'"><i class="fas fa-print">Print</i></button>
                <button type="button" class="btn-primary btn-sm" ng-click="onBtExport()" ng-disabled="loadingstatus=='running'"><i class="fas fa-file-export">Export</i></button>
            </div>
        </div>
    </footer>



</div>