
<script src="~/jslib/inventory/transaction/StockTransfor.js?v=20251012"></script>
<script>
    var VoucherType = @ViewBag.VoucherType;
    var EntityId = @ViewBag.EntityId;
</script>
<style>
    .form-group {
        margin-bottom: 0.4rem !important;
    }
</style>
<div ng-app="myApp" ng-controller="StockTransfor" ng-cloak>
    <section class="iframe-content-starts-here" style="padding-top:45px!important">
        <div class="container-fluid">
            <div class="sale-page-wrap">
                <div class="overflow-hidden">
                    @RenderPage("~/Views/Global/_WaitDialog.cshtml")
                    @RenderPage("~/Views/Global/_LedgerDetails.cshtml")

                    <!-- Page Content  -->
                    <div class="content-wrapper-new">
                        <div class="card mt-2 bodypart">
                            <div class="demo-container">
                                <div class="demo-header">
                                    <div class="card-header pt-2 pb-0 header-corner-triangle overflow-hidden" ng-hide="HideShow.CostClass==true && HideShow.VoucherType==true">
