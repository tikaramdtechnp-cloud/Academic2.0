                                                <th ng-hide="HideShow.ProductDescription==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2;background-color:#ececec!important;">Description</th>
                                                <th ng-hide="HideShow.ProductLedger==true" rowspan="2" class="text-center align-middle" style="width: 150px; z-index: 2;background-color:#ececec!important;">Sales Ledger</th>
                                                <th ng-hide="HideShow.Batch==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.Product.BatchLabel ? SelectedVoucher.Product.BatchLabel : 'Batch No.' }}</th>
                                                <th ng-hide="HideShow.MFGDate==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">MFG.Date</th>
                                                <th ng-hide="HideShow.EXPDate==true" rowspan="2" class="text-center align-middle" style="width: 100px; z-index: 2;background-color:#ececec!important;">EXP.Date</th>

                                                <th ng-show="SelectedVoucher.FixedProduct.ShowEngineNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.EngineNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowChassisNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.ChassisNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowRegdNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.RegdNo}}</th>

                                                <th ng-show="SelectedVoucher.FixedProduct.ShowModel==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.Model}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowType==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.Type}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowColor==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.Color}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowKeyNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.KeyNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowCodeNo==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.CodeNo}}</th>
                                                <th ng-show="SelectedVoucher.FixedProduct.ShowMFGYear==true && beData.ForVehicle==true" rowspan="2" class="text-center align-middle" style="width: 120px; z-index: 2;background-color:#ececec!important;">{{SelectedVoucher.FixedProduct.MFGYear}}</th>

                                                <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 80px; z-index: 2;background-color:#ececec!important;">Weight<br />(gm)</th>
                                                <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 80px; z-index: 2;background-color:#ececec!important;">Loss<br />(gm)</th>

                                                <th colspan="{{(HideShow.BilledQty==true ? 2 : 3 )}}" scope="colgroup" class="text-center" style="width: 250px; z-index: 2;background-color:#ececec!important;">{{comDet.Maintain==2 ? 'Total' :'Quantity' }}</th>
                                                <th ng-hide="HideShow.AlternetUnit1==true" colspan="2" scope="colgroup" class="text-center" style="width: 170px; z-index: 2;background-color:#ececec!important;">Alt.Unit 1</th>
                                                <th ng-hide="HideShow.AlternetUnit2==true" colspan="2" scope="colgroup" class="text-center" style="width: 170px; z-index: 2;background-color:#ececec!important;">Alt.Unit 2</th>
                                                <th rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Rate</th>
                                                <th ng-hide="HideShow.Discount==true" colspan="2" scope="colgroup" class="text-center" style="width: 180px; z-index: 2;background-color:#ececec!important;">Discount</th>
                                                <th ng-hide="HideShow.Amount==true" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Amount</th>

                                                <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Making</th>
                                                <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Stone/Extra</th>
                                                <th ng-show="comDet.Maintain==2" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Total</th>

                                                <th ng-hide="HideShow.ExciseDuty==true" rowspan="2" class="text-center align-middle" style="width: 100px;background-color:#ececec!important;">Excise Duty</th>
                                                <th ng-hide="HideShow.Vat==true" rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Taxable</th>
                                                <th ng-hide="HideShow.Vat==true" rowspan="2" class="text-center align-middle" style="width: 80px;background-color:#ececec!important;">VAT</th>
                                                <th ng-hide="HideShow.MRP==true" rowspan="2" class="text-center align-middle" style="width: 80px;background-color:#ececec!important;">MRP</th>
                                                <th ng-hide="HideShow.SalesRate==true" rowspan="2" class="text-center align-middle" style="width: 80px;background-color:#ececec!important;">Sales Rate</th>
                                                <th ng-hide="HideShow.TradeRate==true" rowspan="2" class="text-center align-middle" style="width: 80px;background-color:#ececec!important;">Trade Rate</th>
                                                <th ng-hide="HideShow.EachNarration==true" rowspan="2" class="text-center align-middle" style="width: 250px;background-color:#ececec!important;">Narration</th>
                                                <th rowspan="2" class="text-center align-middle" style="width: 90px;background-color:#ececec!important;">Action</th>
                                            </tr>
                                            <tr>
                                                <th scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">{{comDet.Maintain==2 ? 'Weight' :'Actual' }}</th>
                                                <th ng-hide="HideShow.BilledQty==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Billed</th>
                                                <th scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                                <th ng-hide="HideShow.AlternetUnit1==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Qty.</th>
                                                <th ng-hide="HideShow.AlternetUnit1==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                                <th ng-hide="HideShow.AlternetUnit2==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Qty.</th>
                                                <th ng-hide="HideShow.AlternetUnit2==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Unit</th>
                                                <th ng-hide="HideShow.Discount==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Per(%)</th>
                                                <th ng-hide="HideShow.Discount==true" scope="col" class="cstm-sticky text-center" style="top: 30px; z-index: 2;background-color:#ececec!important;">Amount</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="itemDet in beData.SourceItemDetailsColl">
                                                <th class="align-middle cstm-sticky" style="width: 50px; left: 0; z-index: 2; background-color: #fff">{{$index+1}}</th>
                                                <td style="width: 250px; z-index: 2; left: 50px; background-color: #fff">
                                                    <select autoShowHide="true" class="form-control form-control-sm select2 serial" all-product ng-model="itemDet.ProductId" style="width:100%"
                                                            placeholder="** Select Product  **" showdetails="true" confirm-action="ProductSelectionChange(itemDet,$index)" product-detail="itemDet.productDetail" side-bar-data="itemDet.sideBarData" voucher-id="SelectedVoucher.VoucherId" vouchertype="19" godown-id="beData.SourceGodownId"></select>
                                                </td>
                                                <td ng-hide="HideShow.ProductDescription==true"><input type="text" class="form-control form-control-sm" ng-model="itemDet.Description"></td>
                                                <td style="z-index: 1;" ng-hide="HideShow.ProductLedger==true">
                                                    <select ng-disabled="itemDet.AutoCharge==true" class="form-control form-control-sm select2bs4" active-select2 style="width: 100%; background-color: #fff" ng-model="itemDet.LedgerId" data-placeholder="**Select Sales Ledger**">
                                                        <option ng-value="sl.LedgerId" ng-repeat="sl in SalesLedgerColl" ng-selected="itemDet.LedgerId==sl.LedgerId">{{sl.Name}}</option>
                                                    </select>
                                                </td>
                                                <td ng-hide="HideShow.CurrentBalance==true">{{itemDet.ClosingQty}}</td>
                                                <td ng-hide="HideShow.Batch==true">
                                                    <select ng-hide="itemDet.productDetail.MaintainBatchWise==false" class="form-control form-control-sm select2bs4" active-select2 style="width: 100%; background-color: #fff" ng-model="itemDet.Batch" data-placeholder="**Select Batch**" ng-change="ChangeBatch(itemDet)">
                                                        <option ng-value="bc.BatchNo" ng-repeat="bc in itemDet.productDetail.BatchWiseColl" ng-selected="itemDet.Batch==bc.BatchNo">{{bc.BatchNo}}-ED:{{bc.EXPDate |expFormat }}({{bc.BalQty |number:2}})</option>
                                                    </select>
                                                </td>
                                                <td ng-hide="HideShow.MFGDate==true">
                                                    <input ng-hide="itemDet.productDetail.UseMfgDate==false" exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.MFGDate" date-detail="itemDet.MFGDateDet">
                                                </td>
                                                <td ng-hide="HideShow.EXPDate==true">
                                                    <input ng-hide="itemDet.productDetail.UseExpDate==false" exp-date type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.EXPDate" date-detail="itemDet.EXPDateDet">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowEngineNo==true && beData.ForVehicle==true">
                                                    <select ng-hide="itemDet.RowType=='L'" class="form-control form-control-sm select2bs4" active-select2 style="width: 100%; background-color: #fff" ng-model="itemDet.EngineNo" data-placeholder="**Select Engine No**" ng-change="ChangeEngineNo(itemDet)">
                                                        <option ng-value="bc.EngineNo" ng-repeat="bc in itemDet.productDetail.FixedProductColl" ng-selected="itemDet.EngineNo==bc.EngineNo">{{bc.EngineNo}}-{{bc.RegdNo}}({{bc.ChassissNo}})</option>
                                                    </select>
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowChassisNo==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.ChassisNo">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowRegdNo==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.RegdNo">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowModel==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.Model">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowType==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.Type">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowColor==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="thDynaVoucherTypesAccountAccountmicsBusinessEntity.select();" ng-model="itemDet.Color">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowKeyNo==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.KeyNo">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowCodeNo==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="text" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.CodeNo">
                                                </td>
                                                <td ng-show="SelectedVoucher.FixedProduct.ShowMFGYear==true && beData.ForVehicle==true">
                                                    <input ng-hide="itemDet.RowType=='L'" type="number" class="form-control form-control-sm serial hideSideBar" style="text-align:left" onfocus="this.select();" ng-model="itemDet.MFGYear">
                                                </td>
                                                <td ng-show="comDet.Maintain==2">
