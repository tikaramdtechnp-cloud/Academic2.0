                                                    <label for="inputEmail3" class="col-sm-4 ">Voucher Type<span style="color:red">*</span></label>
                                                    <div class="col-sm-8">
                                                        <select class="form-control form-control-sm  hideSideBar" style="width: 100%;" ng-model="beData.VoucherId" ng-change="getVoucherNo()" ng-disabled="beData.TranId>0">
                                                            <option ng-repeat="vt in VoucherTypeColl track by vt.VoucherId" ng-value="vt.VoucherId" ng-selected="vt.VoucherId == beData.VoucherId">
                                                                {{vt.VoucherName}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-4" ng-hide="HideShow.CostClass==true">
                                                <div class="form-group row mb-1">
                                                    <label for="inputEmail3" class="col-sm-4 col-form-label">Fiscal Year<span style="color:red">*</span></label>
                                                    <div class="col-sm-8">
                                                        <select class="form-control form-control-sm  hideSideBar" style="width: 100%;" ng-model="beData.CostClassId" ng-change="getVoucherNo()" ng-disabled="beData.TranId>0">
                                                            <option ng-repeat="cc in CostClassColl track by cc.CostClassId" ng-value="cc.CostClassId" ng-selected="cc.CostClassId == beData.CostClassId">
                                                                {{cc.Name}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="card-body p-2">
                                        <div class="row mt-1 ">
                                            <div class="col-sm-3">
                                                <div class="form-group d-flex">
                                                    <label for="inputEmail3" style="min-width: 120px;">Transfor No.<span style="color:red">*</span></label>
                                                    <input type="text" class="form-control form-control-sm" placeholder="" ng-model="beData.AutoManualNo" readonly ng-hide="HideShow.AutoVoucherNo==true">
                                                    <input type="text" class="form-control form-control-sm serial hideSideBar" placeholder="" ng-model="beData.ManualVoucherNO" ng-hide="HideShow.AutoVoucherNo==false">
                                                </div>
                                            </div>
                                            <div class="col-sm-9">
                                                <div class="row">
                                                    <div class="col-sm-4">
                                                        <div class="form-group d-flex" ng-hide="HideShow.RefNo==true">
                                                            <label for="inputEmail3" style="min-width: 120px;">{{SelectedVoucher.RefNoName ? SelectedVoucher.RefNoName : 'Ref. No.'}}</label>
                                                            <input type="text" class="form-control form-control-sm serial hideSideBar" placeholder="" ng-model="beData.RefNo" onfocus="this.select();">

                                                        </div>
                                                    </div>
                                                    <div class="col-sm-2">
                                                        <div class="form-group d-flex" ng-show="comDet.Maintain==5">
                                                            <div class="custom-control custom-checkbox">
                                                                <input class="custom-control-input" type="checkbox"
                                                                       id="customCheckbox1" ng-model="beData.ForVehicle">
                                                                <label for="customCheckbox1"
                                                                       class="custom-control-label">
                                                                    Fixed Product
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group d-flex" @*ng-hide="HideShow.EntryDate==true"*@>
                                                            <label for="inputEmail3" style="min-width: 100px;">{{SelectedVoucher.EntryDateLabel ? SelectedVoucher.EntryDateLabel : 'Entry Date'}}</label>
                                                            <input type="text" class="form-control form-control-sm float-right serial hideSideBar" name="EntryDate" date-picker ng-model="beData.EntryDate_TMP" date-detail="beData.EntryDateDet" />
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group d-flex">
                                                            <label for="inputEmail3" style="min-width: 100px;">{{SelectedVoucher.VoucherDateLabel ? SelectedVoucher.VoucherDateLabel : 'Invoice Date'}}<span style="color:red">*</span></label>
                                                            <input type="text" class="form-control form-control-sm float-right serial hideSideBar" name="VoucherDate" date-picker ng-model="beData.VoucherDate_TMP" date-detail="beData.VoucherDateDet" ng-disabled="ButtonED.IRD==true" confirm-action="getVoucherNoOnly()" />

                                                        </div>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">

                                            <div class="col-sm-3">
                                                <div class="form-group">
                                                    <label>From Godown</label>
                                                    <select class="form-control form-control-sm select2" ng-model="beData.SourceGodownId">
                                                        <option ng-value="at.GodownId" ng-repeat="at in GodownColl" ng-selected="at.GodownId == beData.SourceGodownId">
                                                            {{at.Name}}
                                                        </option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="form-group">
                                                    <label>To Godown</label>
                                                    <select class="form-control form-control-sm select2" ng-model="beData.TargetGodownId" ng-change="GodownSelectionChange()">
                                                        <option ng-value="at.GodownId" ng-repeat="at in ToGodownColl" ng-selected="at.GodownId == beData.TargetGodownId">
                                                            {{at.Name}}
                                                        </option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="col-sm-2">
                                                <div class="form-group">
                                                    <label>Brand</label>
                                                    <select class="select2 form-control form-control-sm" ng-model="beData.ProductBrandId" data-placeholder="**Select Brand**">
                                                        <option ng-value="at.ProductBrandId" ng-repeat="at in ProductBrandColl" ng-selected="at.ProductBrandId == beData.ProductBrandId">
                                                            {{at.Name}}
                                                        </option>
                                                    </select>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="table-scroll-x-y  table-container">
                                    <table id="main-table" class="main-table table table-sm table-bordered">
