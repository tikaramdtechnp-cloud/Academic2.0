<script src="~/jslib/AppCMS/Aboutus.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="AboutusController" ng-init="LoadData()">
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top: 52px!important;">
                <div class="tab-content" id="table-listing" >
                    <div class="card">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">
                                    <!-- select for others -->
                                    <div class="col-md-2">
                                        <div class="form-group text-center">
                                            <!-- <label for="">Logo</label> -->
                                            <div class="card-body">
                                                <!-- <label for="">Photo</label> -->
                                                <div class="person-photo-attachment-wrap text-center">
                                                    <div class="img-wrap mx-auto">
                                                        <img ng-src="{{newAboutUs.PhotoDataLogo || newAboutUs.LogoPath}}" class="img-fluid" alt="" id="imgLogo1">
                                                    </div>
                                                    <!-- Button trigger modal -->
                                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updateLogo">
                                                        <i class="fas fa-camera w-t"></i> Upload Logo
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- select for others -->
                                    <div class="col-md-2">
                                        <div class="form-group text-center">
                                            <!-- <label for="">Logo</label> -->
                                            <div class="card-body">
                                                <!-- <label for="">Photo</label> -->
                                                <div class="person-photo-attachment-wrap text-center">
                                                    <div class="img-wrap mx-auto">
                                                        <img ng-src="{{newAboutUs.SchoolPhotoData || newAboutUs.SchoolPhoto}}" class="img-fluid" alt="" id="imgSchoolPhoto">
                                                    </div>
                                                    <!-- Button trigger modal -->
                                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updateSchoolPhoto">
                                                        <i class="fas fa-camera w-t"></i> Upload Front View Photo
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- select for others -->
                                    <div class="col-md-2">
                                        <div class="form-group text-center">
                                            <!-- <label for="">Logo</label> -->
                                            <div class="card-body">
                                                <!-- <label for="">Photo</label> -->
                                                <div class="person-photo-attachment-wrap text-center">
                                                    <div class="img-wrap mx-auto">
                                                        <img ng-src="{{newAboutUs.AffiliatedLogoData || newAboutUs.AffiliatedLogo}}" class="img-fluid" alt="" id="imgAffiliatedLogo1">
                                                    </div>
                                                    <!-- Button trigger modal -->
                                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updateAffiliatedLogo">
                                                        <i class="fas fa-camera w-t"></i> Upload Affiliated Logo
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- fields after clicking radio button ends -->
                                    <div class="col-md-2">
                                        <div class="form-group text-center">
                                            <!-- <label for="">Header</label> -->
                                            <div class="card-body">
                                                <!-- <label for="">Photo</label> -->
                                                <div class="person-photo-attachment-wrap text-center">
                                                    <div class="img-wrap mx-auto" style="width:100%;">
                                                        <img ng-src="{{newAboutUs.PhotoDataHeader || newAboutUs.ImagePath}}" class="img-fluid" alt="" id="imgHeader1">
                                                    </div>
                                                    <!-- Button trigger modal -->
                                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updateHeader">
                                                        <i class="fas fa-camera w-t"></i> Upload Header Banner
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group text-center">
                                            <!-- <label>Banner</label> -->
                                            <div class="card-body">
                                                <!-- <label for="">Photo</label> -->
                                                <div class="person-photo-attachment-wrap text-center">
                                                    <div class="img-wrap mx-auto" style="width:100%">
                                                        <img ng-src="{{newAboutUs.PhotoDataBanner || newAboutUs.BannerPath}}" class="img-fluid" alt="" id="imgBanner1">
                                                    </div>
                                                    <!-- Button trigger modal -->
                                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updateBanner">
                                                        <i class="fas fa-camera w-t"></i> Upload Footer Banner
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="">Content</label>
                                            <textarea class="textarea" placeholder="Place some text here"
                                                      style="width: 100%; height: 200px; font-size: 14px; line-height: 18px; border: 1px solid #dddddd; padding: 10px;" ng-model="newAboutUs.Content" html-editor></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="time-label">
                                    <span class="d-block border-0 p-3 text-center">
                                        <button type="button"
                                                class="btn btn-sm btn-default"
                                                style="min-width: 90px" ng-click="ClearAboutUs()">
                                            Reset
                                        </button>
                                        <button type="button"
                                                class="btn btn-sm btn-success"
                                                style="min-width: 90px" ng-click="SaveUpdateAboutUs()">
                                            {{newAboutUs.Mode}}
                                        </button>
                                    </span>
                                </div>
                            </div>
                            <!-- col-md-9 -->
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
    <div class="modal fade" id="updateLogo" tabindex="-1" aria-labelledby="updateLogoLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="updateLogoLabel">Upload Logo</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i class="flaticon-close"></i>
                    </button>
                </div>
                <div class="modal-body text-center" border-0 pb-0" file-dropzone="[image/png, image/jpeg]"
                     data-max-file-size="3" ng-model="newAboutUs.PhotoLogo_TMP" @*after-selected="AddAboutUsPhoto()"*@ fileread="newAboutUs.PhotoDataLogo">
                    <div class="photo-upload-wrap">
                        <span class="static-content">
                            <img ng-src="{{newAboutUs.PhotoDataLogo || newAboutUs.LogoPath}}" class="img-fluid" id="imgLogo">
                            <span>Drag & Drop your<br>image here</span>
                        </span>
                    </div>
                    <div class="text-muted pb-3">or</div>
                    <label for="choose-fileLogo" type="button" class="btn btn-secondary btn-light btn-sm">Upload Image</label>
                    <input id="choose-fileLogo" type="file" accept=".jpg,.jpeg,.pdf,doc,docx,application/msword,.png" style="display: none;" file-model ng-model="newAboutUs.PhotoLogo_TMP" @*after-selected="AddStudentPhoto()"*@ fileread="newAboutUs.PhotoDataLogo" />
                    <a ng-click="ClearAboutUsPhotoLogo()" href="#" class="text-danger d-block mt-3">remove current image</a>
                </div>
                <div class="modal-footer justify-content-center">
                    <button type="button" class="btn btn-dafault" data-dismiss="modal">Ok</button>
                    @*<button type="button" class="btn btn-secondary px-4" ng-click="AddAboutUsImage()">Save</button>*@
                </div>
            </div>
        </div>
    </div>

