<script src="~/jslib/account/creation/CostClass.js?v=20251115"></script>
<div ng-app="myApp" ng-controller="CostClass" ng-class="{waiting: loadingstatus=='running'}" ng-init="GetAllCostClass()" ng-cloak>
    <div class="container-fluid">
        <section class="iframe-content-starts-here" style="padding-top:50px!important">
            <div class="tab-content">
                <div class="card-primary card-tabs">
                    <div class="card-header with-gradient-bg p-0 border-bottom-0">
                        <div class="card-header p-0 border-bottom-0">
                            <ul class="nav nav-tabs pt-1" id="custom-tabs-four-tab" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" id="custom-tabs-four-home-tab" data-toggle="pill" href="#custom-tabs-four-home" role="tab" aria-controls="custom-tabs-four-home" aria-selected="true"> List</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" id="custom-tabs-four-profile-tab" data-toggle="pill" href="#custom-tabs-four-profile" role="tab" aria-controls="custom-tabs-four-profile" ng-click="ClearFields()" aria-selected="false">Fiscal Year</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                        <div class="card-body">
                            <div class="tab-content" id="custom-tabs-four-tabContent">
                                <div class="tab-pane fade show active" id="custom-tabs-four-home" role="tabpanel" aria-labelledby="custom-tabs-four-home-tab">
                                    <header class="row">
                                        <div class="col-md-8">
                                            <div class="clearfix">
                                                <form class="form-inline">
                                                    <div class="input-group input-group-sm">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text cstm">Show</span>
                                                        </div>
                                                        <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.CostClass">
                                                            <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.CostClass">{{pp.text}}</option>
                                                        </select>
                                                        <div class="input-group-append">
                                                            <span class="input-group-text bg-white">entries</span>
                                                        </div>
                                                    </div>
                                                    <div class="input-group input-group-sm table-search-filed">
                                                        <input class="form-control form-control-navbar" type="search"
                                                               placeholder="Search" aria-label="Search" ng-model="searchData.CostClass">
                                                        <div class="input-group-append">
                                                            <button class="btn btn-default border-left-0" type="submit">
                                                                <i class="fas fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                        <!-- end col-md-8 -->
                                        <div class="col-md-4 text-right">
                                            <div class="dropdown dnld-file" title="Download File">
                                                <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button"
                                                        id="dropdownMenuButton" data-toggle="dropdown"
                                                        aria-haspopup="true" aria-expanded="false">
                                                    <i class="fas fa-download"></i>
                                                </button>
                                                <div class="dropdown-menu dropdown-menu-right"
                                                     aria-labelledby="dropdownMenuButton">
                                                    <span class="dropdown-item">Choose an option:</span>
                                                    <a class="dropdown-item wait" export-excel table-id="'#datatable'" sheet-name="'debtorcreditortypes'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                    <a class="dropdown-item wait" onClick="$('#datatable').tableExport({type:'csv',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                   
                                                </div>
                                                <a print-tbl table-id="'#datatable'" class="btn btn-secondary btn-light btn-sm wait"
                                                   title="Print">
                                                    <i class="fas fa-print"></i>
                                                </a>`
                                            </div>
                                        </div>
                                    </header>
                                    <div class="search-toolbar">
                                        <div class="">
                                            <table cellpadding="12" class="main-table table table-bordered table-striped table-hover table-sm" id="datatable">
                                                <tr>
                                                    <th ng-click="sort('CostClassId')" style="text-align: center; width: 10%;">

                                                        S.No
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='CostClassId'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>

                                                    <th ng-click="sort('Name')" style="text-align: left; width: 20%;">
                                                        Name
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='Name'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>

                                                    <th ng-click="sort('Alias')" style="text-align: left; width: 12%;">
                                                        Alias
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='Alias'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>
                                                    <th ng-click="sort('Code')" style="text-align: left; width: 12%;">

                                                        Code
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='Code'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>
                                                    <th ng-click="sort('Code')" style="text-align: center; width: 12%;">

                                                        Start Miti
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='Code'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>
                                                    <th ng-click="sort('Code')" style="text-align: center; width: 12%;">

                                                        End Miti
                                                        <span class="glyphicon sort-icon" ng-show="sortKey=='Code'" ng-class="{'glyphicon-chevron-up':reverse,'glyphicon-chevron-down':!reverse}"></span>
                                                    </th>
                                                    <th style="text-align:center;width: 10%;">Is Default</th>
                                                    <th style="text-align:center;width: 10%;">Action</th>

                                                </tr>
                                                <tr dir-paginate="beData in CostClassColl | orderBy:sortKey:reverse | filter:searchData.CostClass | itemsPerPage:perPage.CostClass" current-page="currentPages.CostClass" pagination-id="CostClassPagination">


                                                    <td style="text-align:center;margin:auto">
                                                        {{$index+1}}
                                                    </td>
                                                    <td style="text-align: left; margin: auto">
                                                        {{beData.Name}}
                                                    </td>

                                                    <td style="text-align: left; margin: auto">
                                                        {{beData.Alias}}
                                                    </td>
                                                    <td style="text-align: left; margin: auto">
                                                        {{beData.Code}}
                                                    </td>
                                                    <td style="text-align: center; margin: auto">
                                                        {{beData.StartMiti}}
                                                    </td>
                                                    <td style="text-align: center; margin: auto">
                                                        {{beData.EndMiti}}
                                                    </td>
                                                    <td style="text-align: center; margin: auto">
                                                        {{beData.IsDefault}}
                                                    </td>
                                                    <td class="project-actions text-center" style="text-align:center">
                                                        <div class="btn-group-sm">
                                                            <a href="#" class="btn btn-info btn-sm" ng-click="getCostClassById(beData)"><i class="fas fa-pencil-alt"></i></a>
                                                            <a href="#" class="btn btn-danger btn-sm" ng-click="deleteCostClass(beData,$index)"><i class="fas fa-trash"></i></a>
                                                        </div>
                                                    </td>
                                                </tr>
                                            </table>
                                            <div class="col-md-12">
                                                <dir-pagination-controls pagination-id="CostClassPagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/Scripts/dirPagination.tpl.html">
                                                </dir-pagination-controls>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane fade" id="custom-tabs-four-profile" role="tabpanel" aria-labelledby="custom-tabs-four-profile-tab">

                                    <div class="p-2">
                                        <form>

                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label>Name<span style="color:red">*</span></label>
                                                        <input type="text" class="form-control form-control-sm" ng-model="beData.Name">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label>Alias</label>
                                                        <input type="text" class="form-control form-control-sm" ng-model="beData.Alias">
                                                    </div>
                                                </div>

                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label>Code</label>
                                                        <input type="text" class="form-control form-control-sm" ng-model="beData.Code">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label>Start Date</label>
                                                        <input type="text"
                                                               class="form-control form-control-sm" date-picker ng-model="beData.StartDate_TMP" date-detail="beData.StartDateDet">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3">
                                                    <div class="form-group">
                                                        <label>End Date</label>
                                                        <input type="text"
                                                               class="form-control form-control-sm" date-picker ng-model="beData.EndDate_TMP" date-detail="beData.EndDateDet">
                                                    </div>
                                                </div>
                                                <div class="col-sm-3 col-lg-3 col-md-3">
                                                    <div class="form-group mt-4">
                                                        @*<label>Is Default</label>
        <input type="checkbox"
               class="form-control form-control-sm"
               placeholder="" ng-model="beData.IsDefault" />*@

                                                        <div class="custom-control custom-checkbox">
                                                            <input class="custom-control-input" type="checkbox"
                                                                   id="customCheckbox111" value="option1" ng-model="beData.IsDefault" >
                                                            <label for="customCheckbox111"
                                                                   class="custom-control-label">
                                                                Is Default
                                                            </label>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </form>
                                        <footer class="mt-4">
                                            <div class="text-center control-btns">
                                                <button type="button" class="btn btn-sm btn-default btn" ng-click="ClearFields()" style="min-width:70px"> <span class="glyphicon glyphicon-remove"></span>Clear</button>
                                                <button type="button" class="btn btn-sm btn-success btn" ng-click="AddCostClass()" ng-disabled="loadingstatus=='running'" style="min-width:70px"><span class="glyphicon glyphicon-save"></span>Save</button>
                                            </div>
                                        </footer>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /.card -->
                    </div>
                
                </div>
            </section>
    </div>
</div>




