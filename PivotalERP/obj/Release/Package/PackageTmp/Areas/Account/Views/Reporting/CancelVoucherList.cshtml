    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:50px!important">
                <div class="tab-content">
                    <div id="day-book-section">
                        <div class="row px-2">
                            <div class="col-md-2">
                                <div class="form-group mb-1">
                                    <label>Date From</label>
                                    <input type="text"
                                           class="form-control form-control-sm" date-picker ng-model="dayBook.DateFrom_TMP" date-detail="dayBook.DateFromDet">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group mb-1">
                                    <label>Date To</label>
                                    <input type="text"
                                           class="form-control form-control-sm" date-picker ng-model="dayBook.DateTo_TMP" date-detail="dayBook.DateToDet">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group mb-1">
                                    <label for="name">Select Branch</label>
                                    <select class="form-control form-control-sm select2" style="width: 100%;" ng-model="dayBook.BranchId">
                                        <option ng-value="0">**Select Branch**</option>
                                        <option ng-value="cl.id" ng-repeat="cl in BranchList" ng-selected="cl.id==newDayBook.BranchId">{{cl.text}}</option>
                                    </select>

                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group mb-1">
                                    <label for="name">Select Voucher</label>
                                    <select class="form-control form-control-sm select2" ng-model="dayBook.VoucherId">
                                        <option ng-value="0" selected>All</option>
                                        <option ng-value="vt.id" ng-repeat="vt in VoucherTypeList">{{vt.text}}</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-2 mt-4">
                                <button type="button" class="btn btn-sm btn-success float-right" style="min-width: 90px;" ng-click="GetDayBook()">Load</button>
                            </div>
                        </div>
                        <div class="card p-2 mb-0">
                            <header class="row">
                                <div class="col-md-5 col-sm-5">
                                    <div class="clearfix">
                                        <form class="form-inline">
                                            <div class="input-group input-group-sm mr-sm-2">
                                                <div class="input-group-prepend">
                                                    <span class="input-group-text cstm">Find:</span>
                                                </div>
                                                <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                                <select class="form-control pr-0"
                                                        id="exampleFormControlSelect1">
                                                    <option>Contain</option>
                                                    <option>Prefix</option>
                                                </select>
                                            </div>
                                            <div class="input-group input-group-sm" style="width: 150px;">
                                                <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

                                                <div class="input-group-append">
                                                    <button type="submit" class="btn btn-default">
                                                        <i class="fas fa-search"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>

                                <div class="col-md-7 col-sm-7 text-right">
                                    <form class="form-inline  float-right">
                                        <div class="input-group input-group-sm mr-sm-2">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text cstm">Search By:</span>
                                            </div>
                                            <select class="form-control pr-0" id="exampleFormControlSelect1">
                                                <option>Date(A.D.)</option>
                                                <option>Date(B.S.)</option>
                                                <option>Particulars</option>

                                                <option>Modify By</option>

                                            </select>
                                        </div>
                                        <div class="input-group input-group-sm" style="width: 150px;">
                                            <input type="text" name="table_search" class="form-control float-right" placeholder="Search">

                                            <div class="input-group-append">
                                                <button type="submit" class="btn btn-default">
                                                    <i class="fas fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                        <div class="dropdown dnld-file ml-2" data-toggle="tooltip"
                                             data-placement="top" title="Download File">
                                            <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                                    type="button" id="dropdownMenuButton" data-toggle="dropdown"
                                                    aria-haspopup="true" aria-expanded="false">
                                                <i class="fas fa-download"></i>
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-right"
                                                 aria-labelledby="dropdownMenuButton">
                                                <span class="dropdown-item">Choose an option:</span>
                                                <a class="dropdown-item" href="#" ng-click="DownloadAsXls()">
                                                    <img src="~/wwwroot/dynamic-erp/images/flaticons/excel.png"
                                                         class="img-fluid exports-icon" alt="Excel image">
                                                    Excel
                                                </a>
                                                <a class="dropdown-item" href="#" ng-click="onBtExportCSV()">
                                                    <img src="~/wwwroot/dynamic-erp/images/flaticons/csv.png"
                                                         class="img-fluid exports-icon">
                                                    CSV
                                                </a>
                                            </div>
                                        </div>
                                        <a href="#" class="btn btn-secondary btn-light btn-sm ml-2"
                                           data-toggle="tooltip" data-placement="bottom" title="Print" ng-click="Print()">
                                            <i class="fas fa-print"></i>
                                        </a>
                                    </form>

                                </div>

                            </header>                         
                            <div id="datatable" style="height: calc(100vh - 11.5rem); max-width: 100%;" class="ag-theme-alpine" ag-grid="gridOptions"></div>
                        </div>

                    </div>

                </div>

            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
    <!-- Add ReportTemplates Modal Dailog -->
    <div class="modal fade bd-example-modal-xl sales-invoice-page-modal" id="FrmPrintReport" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenter2Title" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title" id="exampleModalCenterTitle">Report Viewer</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <iframe id="frmRpt" style='width: 100%; height: 600px; background-color: white; visibility:visible;' frameborder='0' scrolling='yes'></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
