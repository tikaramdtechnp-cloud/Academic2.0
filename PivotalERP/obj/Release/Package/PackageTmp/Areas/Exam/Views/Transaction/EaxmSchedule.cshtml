                                                                    <td>
                                                                        <textarea cols="30" rows="1" class="form-control form-control-sm" ng-model="es.Remarks"></textarea>

                                                                    </td>
                                                                    <td class="text-center">

                                                                        <a class="p-1" ng-click="AddExamScheduleDetails($index)">
                                                                            <i class="fas fa-plus text-primary"></i>
                                                                        </a>
                                                                        <a class="p-1" ng-click="delExamScheduleDetails($index)">
                                                                            <i class="fas fa-trash-alt w-t text-danger"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>

                                                <div class="time-label">
                                                    <span class="d-block border-0 p-3 text-center">
                                                        <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearExamSchedule()">Reset</button>
                                                        <button type="button" class="btn btn-sm btn-danger" style="min-width: 90px;">Delete</button>
                                                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateExamSchedule()">{{newExamSchedule.Mode}}</button>
                                       
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Exam Schedule Tab Ends  -->
                                        <!-- Exam Schedule Status Starts -->
                                        <div class="tab-pane fade main-section" id="profile" role="tabpanel"
                                             aria-labelledby="profile-tab">
                                            <div id="examschedule-section">
