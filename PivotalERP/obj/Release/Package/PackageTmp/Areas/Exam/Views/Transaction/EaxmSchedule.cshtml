                                                                    </div>

                                                                </div>
                                                                <div class="tab-pane fade" id="custom-tabs-one-profile" role="tabpanel"
                                                                     aria-labelledby="custom-tabs-one-profile-tab">
                                                                    <div class="pt-3">

                                                                        <table id="dt-basic-example"
                                                                               class="table table-hover table-bordered table-sm bg-white">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th style="width:100px">S.No.</th>
                                                                                    <th style="width:350px">Class/Sec.</th>

                                                                                    <th style="width:120px">Action</th>

                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr ng-repeat="es in newExamScheduleStatusPending.ExamScheduleStatusPendingDetailsColl">
                                                                                    <td>{{$index+1}}</td>
                                                                                    <td>{{es.ClassSection}}</td>
                                                                                    <td>
                                                                                        <a class="btn btn-sm btn-info" ng-click="CreateExamScheduleStatusPending($index)" style="min-width: 90px; color:#fff">
                                                                                            <i class="fas fa-plus"></i> &nbsp; Create Schedule
                                                                                        </a>
                                                                                    </td>

                                                                                </tr>

                                                                            </tbody>
                                                                        </table>
                                                                        <!-- table end -->
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                        <!-- /.card -->
                                                    </div>
                                                </div>


                                            </div>
                                        </div>
                                        <!-- Exam Schedule Status Ends -->
                                        <!-- Print Admit Card Starts -->
                                        <div class="tab-pane fade main-section" id="contact" role="tabpanel"
                                             aria-labelledby="contact-tab">
