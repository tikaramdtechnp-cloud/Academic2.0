                                                                        <table id="dt-basic-example"
                                                                               class="table table-hover table-bordered table-sm bg-white">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th style="width:150px">S.No.</th>
                                                                                    <th style="width:250px">Class/Sec.</th>
                                                                                    <th style="width:250px">Exam Date</th>
                                                                                    <th style="width:250px">Exam time</th>
                                                                                    <th style="width:100px">Action</th>

                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <tr ng-repeat="es in newExamScheduleStatus.ExamScheduleStatusDetailsColl">
                                                                                    <td>{{$index+1}}</td>
                                                                                    <td>{{es.ClassSection}}</td>
                                                                                    <td>{{es.ExamDate}}</td>
                                                                                    <td>{{es.ExamTime}}</td>
                                                                                    <td>
                                                                                        <a class="btn btn-sm btn-info" ng-click="ViewExamScheduleStatusDetails($index)" style="min-width: 90px; color:#fff">
                                                                                            <i class="fas fa-eye"></i> &nbsp; View
                                                                                        </a>
                                                                                    </td>
                                                                                </tr>

                                                                            </tbody>
                                                                        </table>
