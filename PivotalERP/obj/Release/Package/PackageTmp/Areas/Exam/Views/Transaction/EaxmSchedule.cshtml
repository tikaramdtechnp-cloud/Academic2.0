                                                     <label>Exam Type</label> 
                                                     <div class="form-group">
                                                         <select class="form-control form-control-sm select2" ng-model="newPrintAdmitCard.ExamTypeId">
                                                             <option ng-value="0">**Select ExamType**</option>
                                                             <option ng-value="cl.id" ng-repeat="cl in ExamTypeList" ng-selected="cl.id==newPrintAdmitCard.ExamTypeId">{{cl.text}}</option>
                                                         </select>
                                                     </div>
                                                </div>

                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                         <label for="">Minimum Dues Amount</label> 
                                                        <input type="number"
                                                               class="form-control form-control-sm" placeholder="Minimum Dues Amount" ng-model="newPrintAdmitCard.MinimumDuesAmount">
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                         <span>Select Report</span> 
                                                        <select name="comment-group" id="" form="" class="form-control form-control-sm select2" ng-model="newPrintAdmitCard.SelectReport">

                                                            <option value="" selected >Select Report</option>
                                                            <option value="">Report 2</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-sm-8 col-lg-8 col-md-8 ">
