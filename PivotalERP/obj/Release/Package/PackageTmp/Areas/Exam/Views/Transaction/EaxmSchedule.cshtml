                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <label>Exam Type</label>
                                                            <div class="form-group">
                                                                <select class="form-control form-control-sm select2" ng-model="newExamSchedule.ExamTypeId">
                                                                    <option ng-value="0">**Select ExamType**</option>
                                                                    <option ng-value="cl.id" ng-repeat="cl in ExamTypeList" ng-selected="cl.id==newExamSchedule.ExamTypeId">{{cl.text}}</option>
                                                                </select>
                                                            </div>
                                                        </div>
