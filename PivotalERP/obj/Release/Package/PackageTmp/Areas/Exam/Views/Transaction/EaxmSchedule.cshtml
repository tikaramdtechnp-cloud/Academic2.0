                                                                <tr ng-repeat="es in newExamSchedule.ExamScheduleDetailsColl">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>
                                                                        <select name="comment-group"
                                                                                class="form-control form-control-sm select2" ng-model="es.SubjectName">
                                                                            <option value="" selected>Select Subject</option>
                                                                            <option value="">Subject 2</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <select name="exam-type-group"
                                                                                class="form-control form-control-sm select2" ng-model="es.PaperType">
                                                                            <option value="" selected >Select Paper Type</option>
                                                                            <option value="">TH</option>
                                                                            <option value="">PR</option>
                                                                            <option value="">Both</option>
                                                                            <option value="">Grading</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <input type="text"
                                                                               class="form-control form-control-sm" ng-model="es.ExamDate_TMP" date-picker date-detail="es.ExamDateDet"/>
