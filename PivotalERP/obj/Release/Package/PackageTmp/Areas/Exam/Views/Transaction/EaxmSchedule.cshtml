                                                        <div class="row">
                                                            <div class="col-sm-4 col-lg-4 col-md-4">
                                                                <div class="form-group">
                                                                     <label>Class</label> 
                                                                     <select class="form-control form-control-sm select2" ng-model="newExamSchedule.ClassId">
                                                                         <option ng-value="0">**Select Class**</option>
                                                                         <option ng-value="cl.id" ng-repeat="cl in ClassList" ng-selected="cl.id==newExamSchedule.ClassId">{{cl.text}}</option>
                                                                     </select>
