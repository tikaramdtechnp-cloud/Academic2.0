                                                                    <label for="">End Time</label>
                                                                    <input type="time"
                                                                           class="form-control form-control-sm" ng-model="newExamSchedule.EndTime">
                                                                </div>
                                                            </div>
                                                        </div>
                                                
                                                     </form>
                                                </div>
                                            </div>
                                            <div>
