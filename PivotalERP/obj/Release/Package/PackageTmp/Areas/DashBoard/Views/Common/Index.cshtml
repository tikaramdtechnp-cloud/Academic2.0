                    <div class="row no-gutters">
                        <div class="col-md-8">
                            <div class="card-header border-0
                                                pt-3 pb-0">
                                <h3 class="card-title">Outstandings</h3>
                            </div>
                            <div class="card-body pb-0 pt-2">
                                <h4 class="digit text-primary">
                                    {{ OutStandings.Closing |formatNumberAC}}
                                </h4>
                            </div>
                            <div class="two-calc-wrap">
                                <div class="row">
                                    <div class="col-6">
                                        <div class="title-desc-wrap
                                                            recievabes">
                                            <span class="title">Receivables:</span>
                                            <span class="desc">
                                                <small class="circle-dot-bfor-digit"></small> {{ OutStandings.Receivables |formatNumberAC}}
                                            </span>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="title-desc-wrap
                                                            payables">
                                            <span class="title">Payables:</span>
                                            <span class="desc">
                                                <small class="circle-dot-bfor-digit"></small> {{ OutStandings.Payables |formatNumberAC}}
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <div class="p-3">
                                <canvas id="outstandingPieChart" width="90" height="90"></canvas>
                            </div>
