                                <div class="title-desc-wrap">
                                    <h6 class="title">
                                        Capital Invested
                                    </h6>
                                    <span class="desc">
                                        {{ capitalDet.Closing | formatNumberAC }}
                                    </span>
                                </div>
                                <a href="#">
                                    Statistics <i class="right fas
                                                        fa-angle-right"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <!-- Capital-invested card -->
                    <div class="col-sm-6">
                        <div class="card bg-card fixed-assets">
                            <div class="card-body">
                                <div class="title-desc-wrap">
                                    <h6 class="title">
                                        Fixed Assets:
                                    </h6>
                                    <span class="desc">
                                        {{ fixedAssetsDet.Closing | formatNumberAC }}
                                    </span>
                                </div>
                                <a href="#">
                                    Statistics <i class="right fas
                                                        fa-angle-right"></i>
                                </a>
                            </div>
                        </div>
                        <!-- Capital-invested card -->
                    </div>
                </div>
                <div class="row loans-wrap">
                    <div class="col-sm-6">
                        <div class="card bg-card
                                            loan-n-liability">
                            <div class="card-body">
                                <div class="title-desc-wrap">
                                    <h6 class="title">
                                        Loan & Liability
                                    </h6>
                                    <span class="desc">
                                        {{ loansAndLiability.Closing | formatNumberAC }}
