

<script src="~/jslib/Infrastructure/ProposedSite.js"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="ProposedSiteContrioller" ng-init="LoadData()">

    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here">
                <div class="timeline registration-form mt-2">
                    <!-- timeline time label -->
                    <div class="time-label">
                        <span class="text-secondary" style="border: 1px solid #dee2e6">
                            Proposed Site for new Construction
                        </span>
                    </div>
                    <!-- /.timeline-label -->
                    <!-- timeline item -->
                    <div>
                        <!-- <i class="fas fa-envelope bg-blue"></i> -->
                        <div class="timeline-item">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="">
                                            Is there space available for storing construction materials in the site? <span style="color:red">*</span>
                                        </label>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="form-group clearfix">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="interest-calc-yes" ng-value="true" name="storing" ng-model="newDet.IsTeaching" @*ng-change="LoadTeachingDet()"*@>
                                                        <label class="form-check-label" for="interest-calc-yes">Yes</label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="interest-cal-no" ng-value="false" name="storing" ng-model="newDet.IsTeaching" @*ng-change="LoadTeachingDet()"*@>
                                                        <label class="form-check-label" for="interest-cal-no">No</label>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <input type="text" class="form-control form-control-sm" placeholder="Remarks" ng-model="newDet.PanId">
                                                </div>

                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="">
                                            Is there enough water available for construction activities? <span style="color:red">*</span>
                                        </label>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group clearfix">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="water-available-yes" ng-value="true" name="water-available" ng-model="newDet.IsTeaching">
                                                        <label class="form-check-label" for="water-available-yes">Yes</label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="water-available-no" ng-value="false" name="water-available" ng-model="newDet.IsTeaching">
                                                        <label class="form-check-label" for="water-available-no">No</label>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <input type="text" class="form-control form-control-sm" placeholder="Remarks" ng-model="newDet.PanId">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-5">
                                    <div class="form-group">
                                        <label for="">
                                            Is there space available for new building construction without demolishing existing Buildings? <span style="color:red">*</span>
                                        </label>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group clearfix">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="building-construction-yes" ng-value="true" name="building-construction" ng-model="newDet.IsTeaching">
                                                        <label class="form-check-label" for="building-construction-yes">Yes</label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="building-construction-no" ng-value="false" name="building-construction" ng-model="newDet.IsTeaching">
                                                        <label class="form-check-label" for="building-construction-no">No</label>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <input type="text" class="form-control form-control-sm" placeholder="Remarks" ng-model="newDet.PanId">
                                                </div>

                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-1"></div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="">
                                            If existing Building of the school is demolished for construction of new building, is there any alternative solution possible to continue the classes? <span style="color:red">*</span>
                                        </label>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group clearfix">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="demolished-yes" ng-value="true" name="demolished" ng-model="newDet.IsTeaching" @*ng-change="LoadTeachingDet()"*@>
                                                        <label class="form-check-label" for="demolished-yes">Yes</label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" id="demolished-no" ng-value="false" name="demolished" ng-model="newDet.IsTeaching" @*ng-change="LoadTeachingDet()"*@>
                                                        <label class="form-check-label" for="demolished-no">No</label>
                                                    </div>
                                                </div>

                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <input type="text" class="form-control form-control-sm" placeholder="Remarks" ng-model="newDet.PanId">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Total Land Area<span style="color:red">*</span></label>
                                        <input type="number" class="form-control form-control-sm" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Terrain<span style="color:red">*</span></label>
                                        <select class="form-control form-control-sm select2" ng-model="newDet.TerrainId">
                                            @*<option ng-value="c.id" ng-repeat="c in DesignationList" ng-selected="c.id==newDet.TerrainId">{{c.text}}</option>*@
                                            <option value="1">Flat Land</option>
                                            <option value="2">Slope Land</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Shape of land<span style="color:red">*</span></label>
                                        <select class="form-control form-control-sm select2" ng-model="newDet.ShapeOfLandId">
                                            @*<option ng-value="c.id" ng-repeat="c in DesignationList" ng-selected="c.id==newDet.TerrainId">{{c.text}}</option>*@
                                            <option value="1">Rectangular</option>
                                            <option value="2">Square</option>
                                            <option value="3">Trapezoid</option>
                                            <option value="4">Irregular</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Future Expand Possibility? <span style="color:red">*</span></label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="Future-Expand-yes" ng-value="true" name="Expand" ng-model="newDet.IsPossibility">
                                                <label class="form-check-label" for="Future-Expand-yes">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="Future-Expand-no" ng-value="false" name="Expand" ng-model="newDet.IsPossibility">
                                                <label class="form-check-label" for="Future-Expand-no">No</label>
                                            </div>

                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- END timeline item -->
                    <!-- timeline Acacemic Details label -->
                    <div class="time-label">
                        <span class="" style="border: 1px solid #dee2e6">
                            Site Development Need (Landscaping and Stability)
                        </span>
                    </div>
                    <!-- /.timeline-Acacemic Details-label -->
                    <!-- timeline Acacemic Details item -->
                    <div>
                        <div class="timeline-item">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="">Will there be any involuntary settlement due to the new construction of school building? <span style="color:red">*</span></label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="isInvoluntary-yes" ng-value="true" name="isInvoluntary" ng-model="newDet.isInvoluntary">
                                                <label class="form-check-label" for="isInvoluntary-yes">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="isInvoluntary-no" ng-value="false" name="isInvoluntary" ng-model="newDet.isInvoluntary">
                                                <label class="form-check-label" for="isInvoluntary-no">No</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Cut
                                            <input type="checkbox" id="CutCheckbox" ng-model="newDet.IsCut" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Fill
                                            <input type="checkbox" id="FillCheckbox" value="0" ng-model="newDet.IsFill" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Retaining
                                            <input type="checkbox" id="RetainingCheckbox" value="0" ng-model="newDet.IsRetaining" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Access Road
                                            <input type="checkbox" id="AccessRoadCheckbox" value="0" ng-model="newDet.IsAccessRoad" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Tree/Bush
                                            <input type="checkbox" id="TreeCheckbox" value="0" ng-model="newDet.IsTree" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-wrap">
                                            Boundary Wall
                                            <input type="checkbox" id="BoundaryWallCheckbox" value="0" ng-model="newDet.IsBoundaryWall" />
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsCut">
                                    <div class="form-group">
                                        <label for="">Soil to be cut(m3)<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Soil to be cut" ng-model="newDet.SoilCut">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsFill">
                                    <div class="form-group">
                                        <label for="">Soil to be filled(m3)<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Soil to be filled" ng-model="newDet.SoilFilled">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsRetaining">
                                    <div class="form-group">
                                        <label for="">Length of Retaining Wall needed<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Length of Retaining Wall needed" ng-model="newDet.LengthRetainWall">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsRetaining">
                                    <div class="form-group">
                                        <label for="">Type of Retaining Wall<span style="color:red">*</span></label>
                                        <select class="form-control form-control-sm select2" ng-model="newDet.RetainingWallId">
                                            <option value="" disabled selected>-- Select Retaining Wall Type --</option>
                                            <option ng-value="c.id" ng-repeat="c in RetainWallTypeColl" ng-selected="c.id==newDet.RetainingWallId">{{c.text}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsAccessRoad">
                                    <div class="form-group">
                                        <label for="">No. of Access Road to be built<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="No. of Access Road" ng-model="newDet.AccessRoadNo">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsAccessRoad">
                                    <div class="form-group">
                                        <label for="">Length of Access Road to built(m)<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Length of Access Road" ng-model="newDet.AccessRoadLength">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsTree">
                                    <div class="form-group">
                                        <label for="">Tree/Bush Clearance volume type<span style="color:red">*</span></label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="Clearance-volume-1" ng-value="1" name="Clearance-volume" ng-model="newDet.ClearanceVolumeType">
                                                <label class="form-check-label" for="Clearance-volume-1">Minor</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="Clearance-volume-2" ng-value="2" name="Clearance-volume" ng-model="newDet.ClearanceVolumeType">
                                                <label class="form-check-label" for="Clearance-volume-2">Medium</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="Clearance-volume-3" ng-value="3" name="Clearance-volume" ng-model="newDet.ClearanceVolumeType">
                                                <label class="form-check-label" for="Clearance-volume-3">Heavy</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsTree">
                                    <div class="form-group">
                                        <label for="">Tree/Bush Clearance Volume(m3)<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Clearance Volume" ng-model="newDet.ClearanceVolume">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsBoundaryWall">
                                    <div class="form-group">
                                        <label for="">Length of Boundary Wall needed<span style="color:red">*</span></label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Clearance Volume" ng-model="newDet.BoundaryWall">
                                    </div>
                                </div>
                                <div class="col-md-3" ng-show="newDet.IsBoundaryWall">
                                    <div class="form-group">
                                        <label for="">Type of Boundary Wall<span style="color:red">*</span></label>
                                        <select class="form-control form-control-sm select2" ng-model="newDet.BoundaryWallId">
                                            <option value="" disabled selected>-- Select Boundary Wall Type --</option>
                                            <option ng-value="c.id" ng-repeat="c in RetainWallTypeColl" ng-selected="c.id==newDet.BoundaryWallId">{{c.text}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Remarks</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Remarks" ng-model="newDet.SiteRemarks">
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- END Acacemic Details item -->
                    <!-- Emergency Contact time label -->
                    <div class="time-label">
                        <span class="" style="border: 1px solid #dee2e6">
                            Land Ownership Certificate
                        </span>
                    </div>
                    <!-- /.Emergency Contact-label -->
                    <!-- Emergency Contact item -->
                    <div>
                        <div class="timeline-item">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="">Has Land Ownership Certificate<span style="color:red">*</span></label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="isLandOwnership-yes" ng-value="true" name="isLandOwnership" ng-model="newDet.isLandOwnership">
                                                <label class="form-check-label" for="isLandOwnership-yes">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="isLandOwnership-no" ng-value="false" name="isLandOwnership" ng-model="newDet.isLandOwnership">
                                                <label class="form-check-label" for="isLandOwnership-no">No</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12" ng-show="newDet.isLandOwnership== 1">
                                    <div class="form-group">
                                        <div class="form-group clearfix mb-0">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" id="LandOwnership-one" ng-value="1" name="LandOwnership" ng-model="newDet.LandOwnership">
                                                <label class="form-check-label" for="LandOwnership-one">Land Ownership Certificate in the name of School</label>
                                            </div>
                                        </div>
                                        <div class="form-group clearfix mb-0">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" id="LandOwnership-two" ng-value="2" name="LandOwnership" ng-model="newDet.LandOwnership">
                                                <label class="form-check-label" for="LandOwnership-two">
                                                    Government Land & VDC/Municipality has given letter for use for School
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group clearfix mb-0">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" id="LandOwnership-three" ng-value="3" name="LandOwnership" ng-model="newDet.LandOwnership">
                                                <label class="form-check-label" for="LandOwnership-three">
                                                    Government Land & VDC/Municipality has yet to issue letter for use to School
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group clearfix mb-0">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" id="LandOwnership-four" ng-value="4" name="LandOwnership" ng-model="newDet.LandOwnership">
                                                <label class="form-check-label" for="LandOwnership-four">
                                                    Private Land & Land Owner has given the land for use for School
                                                </label>
                                            </div>
                                        </div>
                                        <div class="form-group clearfix mb-0">
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" id="LandOwnership-five" ng-value="5" name="LandOwnership" ng-model="newDet.LandOwnership">
                                                <label class="form-check-label" for="LandOwnership-five">
                                                    Private Land & Owner has yet to issue the letter for use to School
                                                </label>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-4"ng-show="newDet.isLandOwnership== 1">
                                    <label><span style="color:red;"><i> Only Image files(not more than 2MB) are allowed.</i></span></label>
                                    <input type="file" class="form-control form-control-sm">

                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END Emergency Contact item -->
                    <!-- Vernacular Context time label -->
                    <div class="time-label">
                        <span class="" style="border: 1px solid #dee2e6">Vernacular Context</span>
                    </div>
                    <!-- /.Permanent Address-label -->
                    <!-- Permanent Address item -->
                    <div>
                        <div class="timeline-item">
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Local Materials</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Local Materials" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Architectural Features</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Architectural Features" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Fenestration</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Fenestration" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Landscaping</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Landscaping" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">Others</label>
                                        <input type="text" class="form-control form-control-sm" placeholder="Others" ng-model="newDet.TotalLandArea">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- END Vernacular Context item -->
                    <!-- timeline time label -->
                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-3 text-center">
                        <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearEmployee()">Reset</button>
                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateEmployee()">Save</button>
                    </span>
                </div>
                <!-- /.timeline-label -->
            </section>
        </div>
    </section>

</div>
