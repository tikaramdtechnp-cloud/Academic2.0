
<script src="~/jslib/setup/PaymentGateway.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="paymentGatewayController" ng-init="LoadData()">
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:50px!important">
                <div class="tab-content">
                    <div class="branch-section card py-2" id="fp-section">
                        <header class="row">
                            <div class="col-md-7">
                                <div class="clearfix">
                                    <form class="form-inline">
                                        <div class="input-group input-group-sm mr-sm-2">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text cstm">Show</span>
                                            </div>
                                            <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                            <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.PaymentGateway">
                                                <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.PaymentGateway">{{pp.text}}</option>
                                            </select>
                                            <div class="input-group-append">
                                                <span class="input-group-text bg-white"><small>entries</small></span>
                                            </div>
                                        </div>
                                        <div class="input-group input-group-sm table-search-filed">
                                            <input class="form-control form-control-navbar" type="search"
                                                   placeholder="Search" aria-label="Search" ng-model="searchData.PaymentGateway">
                                            <div class="input-group-append">
                                                <button class="btn btn-default border-left-0" type="submit">
                                                    <i class="fas fa-search"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <!-- end col-md-8 -->
                            <div class="col-md-5 text-right">
                                <div class="dropdown dnld-file ml-2" data-toggle="tooltip"
                                     data-placement="top" title="Download File">
                                    <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button"
                                            id="dropdownMenuButton" data-toggle="dropdown"
                                            aria-haspopup="true" aria-expanded="false">
                                        <i class="fas fa-download"></i>
                                    </button>
                                    <div class="dropdown-menu dropdown-menu-right"
                                         aria-labelledby="dropdownMenuButton">
                                        <span class="dropdown-item">Choose an option:</span>
                                        <a class="dropdown-item wait" export-excel table-id="'#PaymentGateway'" sheet-name="'PaymentGatewayList'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                        <a class="dropdown-item wait" onClick="$('#PaymentGateway').tableExport({type:'csv',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                      
                                    </div>
                                </div>
                                <a print-tbl table-id="'#PaymentGateway'" href="#" class="btn btn-secondary btn-light btn-sm"
                                   data-toggle="tooltip" data-placement="top" title="Print">
                                    <i class="fas fa-print"></i>
                                </a>
                                <button type="button" class="btn btn-sm btn-success" id="add-fp" style="min-width: 90px;">
                                    <i class="fas fa-plus w-t"></i> Add Payment Gateway
                                </button>
                            </div>
                        </header>
                        <div>
                            <div class="table-scroll-x-y sales-main-table table-scroll">
                                <table id="PaymentGateway" class="main-table table table-sm table-bordered bg-white">
                                    <thead class=" text-nowrap">
                                        <tr>
                                            <th style="width: 50px;">S.No.</th>
                                            <th style="width: 150px;">Name</th>
                                            <th style="width: 150px;">Gateway</th>
                                            <th style="width: 250px;">Ledger</th>
                                            <th style="width: 130px;">Create At</th>
                                            <th style="width: 120px;">Create By</th>
                                            <th style="width: 130px;">Modify At</th>
                                            <th style="width: 120px;">Modify By</th>
                                            <th style="width: 85px;">Icon</th>
                                            <th style="width: 65px;">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr dir-paginate="bl in PaymentGatewayList | filter:searchData.PaymentGateway | itemsPerPage: perPage.PaymentGateway" current-page="currentPages.PaymentGateway" pagination-id="PaymentGatewayPagination">
                                            <td class="text-center">{{$index+1}}</td>
                                            <td>{{bl.Name}}</td>
                                            <td>{{bl.ForGateWay}}</td>
                                            <td>{{bl.LedgerName}}</td>
                                            <td>{{bl.CreateAt}}</td>
                                            <td>{{bl.CreateBy}}</td>
                                            <td>{{bl.ModifyAt}}</td>
                                            <td>{{bl.ModifyBy}}</td>
                                            <td>
                                                <img ng-src="{{bl.IconPath}}" class="img-fluid" alt="" style="width:75px;">
                                            </td>
                                            <td>
                                                <a class="p-1" href="#" ng-click="GetPaymentGatewayById(bl)">
                                                    <i class="fas fa-edit text-info"></i>
                                                </a>
                                                <a class="p-1" href="#" ng-click="DelPaymentGatewayById(bl)">
                                                    <i class="fas fa-trash-alt text-danger"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <!-- table end -->
                        </div>
                        <footer class="row align-items-center">
                            <div class="col-md-12">
                                <dir-pagination-controls pagination-id="PaymentGatewayPagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/Scripts/dirPagination.tpl.html">
                                </dir-pagination-controls>
                            </div>
                        </footer>
                    </div>
                </div>

                <!-- form area starts -->
                <div id="fp-form">
                    <div class="card card-default">
                        <div class="card-body px-0 py-2">
                            <div class="row">
                                <div class="col-md-10">
                                    <div class="row">
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label for="">For</label>
                                                <select class="form-control form-control-sm" ng-model="newPaymentGateway.ForGateWay">
                                                    <option ng-value="cl.id" ng-repeat="cl in GatewayTypesColl" ng-selected="cl.id==newPaymentGateway.ForGateWay">{{cl.text}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Name</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.Name">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Private Key</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.PrivateKey">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">Public Key</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.PublicKey">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">School Code</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.SchoolId">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">User Name</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.UserName">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">Pwd</label>
                                                <input type="password" class="form-control form-control-sm" ng-model="newPaymentGateway.Pwd">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">Merchant Code</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.MerchantId">
                                            </div>
                                        </div>
                                        <!--New Field Added by suresh on 16 baiskakh-->
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="">Merchant Name</label>
                                                <input type="text" class="form-control form-control-sm" ng-model="newPaymentGateway.MerchantName">
                                            </div>
                                        </div>
                                        <!--Ends-->
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>Ledger Tag</label>
                                                <select autoShowHide="true" class="form-control form-control-sm select2 serial" all-ledger ng-model="newPaymentGateway.LedgerId" id="cboParty" @*style="width:95%"*@ ledgertype="1"
                                                        placeholder="** Select Ledger **" showdetails="true" @*confirm-action="PartySelectionChange(newFeeItem)"*@ ledger-detail="newPaymentGateway.FeeLedger" side-bar-data="newPaymentGateway.FeeSideBarData"></select>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                           

                                <div class="col-md-2">
                                    <label for="" class="invisible">Photo</label>
                                    <div class="person-photo-attachment-wrap text-center">
                                        <div class="img-wrap mx-auto" style="width:100%;">
                                            <img ng-src="{{newPaymentGateway.PhotoData[0] || newPaymentGateway.IconPath}}" class="img-fluid" alt="" id="imgPhoto1">
                                        </div>
                                        <label for="choose-file" type="button" class="btn btn-outline-secondary btn-sm">Upload Icon</label>
                                        <input id="choose-file" class="btn btn-outline-secondary btn-sm" style="display: none;" type="file" accept=".jpg,.jpeg,.png" file-model-resize ng-model="newPaymentGateway.Photo_TMP" fileread="newPaymentGateway.PhotoData" />
                                        <a ng-click="ClearSliderPhoto()" href="#" class="text-danger d-block mt-3">remove current image</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- timeline time label -->
                        <div class="time-label pb-3" style="margin-top: -10px;">
                            <span class="d-block border-0 p-1 text-center">
                                <button type="button" class="btn btn-sm btn-info" id="back-to-list-fp" style="min-width: 90px;">Back To List</button>
                                <button type="button"
                                        class="btn btn-sm btn-default"
                                        style="min-width: 90px" ng-click="ClearPaymentGateway()">
                                    Reset
                                </button>
                                <button type="button"
                                        class="btn btn-sm btn-success"
                                        style="min-width: 90px" ng-click="SaveUpdatePaymentGateway()">
                                    {{newPaymentGateway.Mode}}
                                </button>
                            </span>
                        </div>
                        <!-- /.timeline-label -->
                    </div>
                </div>
                <!-- form area ends -->
            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
</div>
