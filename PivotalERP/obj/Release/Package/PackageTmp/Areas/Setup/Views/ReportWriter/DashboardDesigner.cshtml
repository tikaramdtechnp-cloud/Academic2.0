                                                                    <button class="btn btn-sm btn-success" style="width:50px;" ng-click="AddChartCategoryX()"><i class="fas fa-angle-right" style="font-size:20px"></i></button>
                                                                </div>
                                                                <div>
                                                                    <form action="#">
                                                                        <select class="form-control p-0" id="lang" size="6" ng-model="newCreateDashboard.SelectedFieldX" style="min-width:160px;">
                                                                            <option ng-repeat="ds in ChartCategoryXColl" value="{{$index}}">{{ds.text}}</option>
                                                                        </select>
                                                                    </form>
                                                                </div>
                                                                <div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-info" style="width:60px;" ng-click="upChartCategoryX()">Up</button>
                                                                    </div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-info" style="width:60px;" ng-click="downChartCategoryX()">Down</button>
                                                                    </div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-danger" style="width:60px;" ng-click="delChartCategoryX()">Delete</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <p class="mb-1" style="margin-left:57px;"><b>Chart Series</b></p>
                                                            </div>
                                                            <div class="d-flex">
                                                                <div class="mr-2 mt-4">
                                                                    <button class="btn btn-sm btn-success" style="width:50px;" ng-click="AddChartSeries()"><i class="fas fa-angle-right" style="font-size:20px"></i></button>
                                                                </div>
                                                                <div>
                                                                    <form action="#">
                                                                        <select class="form-control p-0" id="lang" size="6" ng-model="newCreateDashboard.SelectedSeries" style="min-width:160px;">
                                                                            <option ng-repeat="ds in ChartSeriesColl" value="{{$index}}">{{ds.text}}</option>
                                                                        </select>
                                                                    </form>
                                                                </div>
                                                                <div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-info" style="width:60px;" ng-click="upChartSeries()">Up</button>
                                                                    </div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-info" style="width:60px;" ng-click="downChartSeries()">Down</button>
                                                                    </div>
                                                                    <div class="ml-3 mb-2">
                                                                        <button class="btn btn-sm btn-danger" style="width:60px;" ng-click="delChartSeries()">Delete</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="mt-4 row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label>Data Expression</label>
                                                            <select class="form-control form-control-sm select2" ng-model="newCreateDashboard.DataExpression">
                                                                <option ng-repeat="ds in DataSetFieldColl" ng-value="ds.text">{{ds.text}}</option>
                                                            </select>
                                                        </div>
                                                    </div>                                        
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="card" style="border:1px solid #ccc">
                                                    <div class="card-header p-2">
                                                        <h6 class="mb-0">Chart Preview</h6>
                                                        <button class="float-right btn btn-sm btn-info" style="position:relative;margin-top:-25px;" ng-click="PreviewChart()"> <i class="fas fa-eye mr-2"></i>Preview</button>
                                                    </div>
                                                    <!--Bar Chart-->
                                                    <div class="card-body p-2">
                                                        <canvas id="chartPreview" width="800" height="430"></canvas>
                                                    </div>                                                  
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="card" style="border:1px solid #ccc">
                                                <div class="card-header p-2">
                                                    <h6 class="mb-0">Table Footer</h6>                                                    
                                                </div>
                                              
                                                <div class="card-body p-2">
                                                    <div class="table-responsive table-striped p-0" style="height: 350px;">
                                                        <table class="table table-head-fixed text-nowrap" id="tbl{{$index}}">
                                                            <thead>
                                                                <tr>
                                                                    <th style="width: 90px">S.No.</th>
                                                                    <th style="width: 120px">Column</th>
                                                                    <th style="width: 120px">Data As</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="rd in DataSetFooterColl">
                                                                    <td>{{rd.id}}</td>
                                                                    <td>{{rd.Field}}</td>
                                                                    <td>
                                                                        <select class="form-control form-control-sm select2" ng-model="rd.FunName">
                                                                            <option ng-repeat="ds in FunctionColl" ng-value="ds" ng-selected="ds==rd.FunName">{{ds}}</option>
                                                                        </select>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="time-label">
                                            <span class="d-block border-0 p-3 text-center">
                                                <button type="button"
                                                        class="btn btn-sm btn-info"
                                                        id="backbtn"
                                                        style="min-width: 90px">
                                                    Back to List
                                                </button>
                                                <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearCreateDashboard()">Reset</button>
                                                <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateCreateDashboard()">{{newCreateDashboard.Mode}}</button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
</div>
