
<script src="~/jslib/Setup/importData.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="importDataController" ng-init="LoadData()">
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:50px!important">
                <div class="tab-content">
                    <!-- form area starts -->
                    <div id="allow-branch-form">
                        <div class="card card-default">
                            <div class="card-body p-2">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">Entity </label>
                                            <select class="form-control form-control-sm select2" ng-model="ImportData.SelectedEntity">
                                                <option ng-value="cl" ng-repeat="cl in EntityColl">{{cl.text}}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-3" ng-show="ImportData.SelectedEntity.IsUpdate==true">
                                        <div class="form-group">
                                            <label for="">Update By </label>
                                            <select class="form-control form-control-sm select2" ng-model="ImportData.UpdateBy">
                                                <option ng-value="cl.Value" ng-repeat="cl in UpdateByColumns">{{cl.Text}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-2 mt-4 ">
                                        <div class="btn btn-info btn-sm btn-file">
                                            <i class="fa fa-file-excel-o"></i> Select Excel File
                                            <input class="form-control form-control-sm p-0" id="documents" type="file" style="padding: 1px" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" file-model ng-model="ImportData.Files_TMP" after-selected="UploadToSrv()" />
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>

                </div>

            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
    <!-- model form for table input -->
    <div class="modal fade"
         id="AddNewExcel"
         tabindex="-1"
         role="dialog"
         aria-labelledby="exampleModalLabel"
         aria-hidden="true"
         style="margin-top:40px;">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title" id="exampleModalLabel">
                        Import Data
                    </h6>
                    <button type="button"
                            class="close"
                            data-dismiss="modal"
                            aria-label="Close">
                        <div aria-hidden="true">&times;</div>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label">
                                Select Sheet Name <span id="ErrName" style="color:red;"></span>
                            </label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                <select ng-model="SelectedSheet" class="form-control form-control-sm input-sm" ng-change="LoadProperties()">
                                    <option ng-repeat="ttl in SheetColl" value="{{ttl}}">{{ttl}}</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12" id="tblPart">
                        <div class="box-body scroller">
                            <div class="table-scrollable">
                                <table cellpadding="12" class="table table-bordered table-hover enquiry-table" id="clientTable">
                                    <thead>
                                        <tr>
                                            <td class="col2">S.No.</td>
                                            <td class="col1">Target Fields Name</td>
                                            <td class="col3">Source Fields Name</td>
                                            <td class="col4">Default Value</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr id="tablerow0" ng-repeat="newBeData in PropertiesColl ">
                                            <td class="col1">
                                                {{$index+1}}
                                            </td>
                                            <td class="col3">
                                                {{newBeData.PropertyName}}
                                            </td>
                                            <td class="col4">
                                                <select ng-model="newBeData.Id" class="form-control input-sm">
                                                    @*<option ng-repeat="cl in ColumnColl track by cl.Id" value="{{cl.Id}}">
            {{cl.Name}}
        </option>*@
                                                    <option ng-repeat="cl in ColumnColl track by cl.Id" ng-value="cl.Id" ng-selected="cl.Id==newBeData.Id">
                                                        {{cl.Name}}
                                                    </option>
                                                </select>
                                            </td>
                                            <td>
                                                <input type="text" class="form-control input-sm" ng-model="newBeData.DefaultValue" />
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-3 text-center">
                        <button type="button" class="btn btn-sm btn-danger" style="min-width: 90px;" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="ImportDataExcel()">Import Data</button>
                    </span>
                </div>
            </div>
        </div>
    </div>
    <!-- modal form for table input close -->
    <!-- model form for table input -->
    <div class="modal fade"
         id="UpdateNewExcel"
         tabindex="-1"
         role="dialog"
         aria-labelledby="exampleModalLabel"
         aria-hidden="true"
         style="margin-top:40px;margin-left: 60px;">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">
                        Update Data
                    </h5>
                    <button type="button"
                            class="close"
                            data-dismiss="modal"
                            aria-label="Close">
                        <div aria-hidden="true">&times;</div>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label">
                                Select Sheet Name <span id="ErrName" style="color:red;"></span>
                            </label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="fa fa-info"></i></span>
                                <select ng-model="SelectedSheet" class="form-control input-sm" ng-change="LoadProperties()">
                                    <option ng-repeat="ttl in SheetColl" value="{{ttl}}">{{ttl}}</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12" id="tblPart">
                        <div class="box-body scroller">
                            <div class="table-scrollable">
                                <table cellpadding="12" class="table table-bordered table-hover enquiry-table" id="clientTable">
                                    <thead>
                                        <tr>
                                            <td class="col2">S.No.</td>
                                            <td class="col1">Target Fields Name</td>
                                            <td class="col3">Source Fields Name</td>
                                            <td class="col4">Default Value</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr id="tablerow0" ng-repeat="newBeData in SelectedPropertiesColl ">
                                            <td class="col1">
                                                <label class="col-sm-12">
                                                    {{$index+1}}
                                                </label>
                                            </td>
                                            <td class="col3">
                                                <div class="editor-field">
                                                    <select ng-model="newBeData.PropertyName" class="form-control input-sm">
                                                        <option ng-repeat="et in PropertiesColl" value="{{et.PropertyName}}">
                                                            {{et.PropertyName}}
                                                        </option>
                                                    </select>
                                                </div>
                                            </td>
                                            <td class="col4">
                                                <select ng-model="newBeData.Id" class="form-control input-sm">
                                                    <option ng-repeat="cl in ColumnColl track by cl.Id" value="{{cl.Id}}">
                                                        {{cl.Name}}
                                                    </option>
                                                </select>
                                            </td>
                                            <td>
                                                <a class="btn btn-default btn-xs" ng-click="addrow($index)"><i class="fa fa-plus"></i></a>
                                                <a class="btn btn-default btn-xs" ng-click="delete($index)"><i class="fa fa-trash"></i></a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="time-label">
                    <span class="d-block border-0 p-3 text-center">
                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" data-dismiss="modal">Cancel</button>
                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="UpdateDataExcel()">ImportData</button>
                    </span>
                </div>
            </div>
        </div>
    </div>
    <!-- modal form for table input close -->
</div>