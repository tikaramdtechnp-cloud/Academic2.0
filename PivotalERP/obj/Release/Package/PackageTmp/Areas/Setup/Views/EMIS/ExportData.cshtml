
<script src="~/jslib/Setup/EMIS.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="emisController" ng-init="LoadData()">
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:50px!important">
                <div class="tab-content">

                    <!-- form area starts -->
                    <div id="allow-branch-form">
                        <div class="card card-default">
                            <div class="card-body px-0 py-2">
                                <div class="row">
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label>School Code</label>
                                        <input type="text"
                                               class="form-control form-control-sm" ng-model="newEMIS.SchoolCode" />
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <label>Academic Year</label>
                                        <input type="number"
                                               class="form-control form-control-sm" ng-model="newEMIS.AcademicYear" />
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <div class="form-group">
                                            <label>Regd. Id As</label>
                                            <div class="form-group">
                                                <select class="form-control form-control-sm" ng-model="newEMIS.RegdIdAs">
                                                    <option ng-value="cl.id" ng-repeat="cl in RegdIdAsList" ng-selected="cl.id==newEMIS.RegdIdAs">{{cl.text}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <div class="form-group">
                                            <label>Teacher Id As</label>
                                            <div class="form-group">
                                                <select class="form-control form-control-sm" ng-model="newEMIS.TeacherIdAs">
                                                    <option ng-value="cl.id" ng-repeat="cl in TeacherIdAsList"  ng-selected="cl.id==newEMIS.TeacherIdAs">{{cl.text}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 col-md-4">
                                        <div class="form-group">
                                            <label>ClassName As</label>
                                            <div class="form-group">
                                                <select class="form-control form-control-sm" ng-model="newEMIS.ClassNameAs">
                                                    <option ng-value="cl.id" ng-repeat="cl in ClassNameAsList"  ng-selected="cl.id==newEMIS.ClassNameAs">{{cl.text}}</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="btn btn-info btn-sm btn-file">
                                            <i class="fa fa-file-excel-o"></i> Select Excel File For Import Student/Employee
                                            <input class="form-control form-control-sm p-0" type="file" style="padding: 1px" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" file-model ng-model="ImportData.Files_TMP" after-selected="UploadToSrv()" />
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="btn btn-info btn-sm btn-file">
                                            <i class="fa fa-file-excel-o"></i> Select Excel File For Export Student/Employee
                                            <input class="form-control form-control-sm p-0" type="file" style="padding: 1px" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" file-model ng-model="ImportData.Files_TMP" after-selected="UploadToSrvForExport()" />
                                        </div>
                                    </div>
                                    <div class="col-md-3" ng-show="newEMIS.ShowDownload==true">
                                        <div class="btn btn-info btn-sm btn-file">
                                            <i class="fa fa-download"><a href="~/DownloadHandler.ashx?fileName={{newEMIS.FilePath}}">Download EMIS XLSX File</a></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
   
</div>