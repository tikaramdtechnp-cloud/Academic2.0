<script src="~/jslib/Infirmary/Disease.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="DiseaseController" ng-init="LoadData()">
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here">
                <div class="tab-content" id="pills-tabContent">
                    <div class="tab-pane fade show active" id="pills-second" role="tabpanel"
                         aria-labelledby="pills-second-tab">
                        <header class="if-page-have-nav-tabs px-2">
                            <div class="row upper-tab">
                                <div class="col-md-12">
                                    <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active" id="myTab" role="tablist">
                                        <li class="nav-item" role="presentation">
                                            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab"
                                               aria-controls="home" aria-selected="true">Health Issue/Disease</a>
                                        </li>
                                        <li class="nav-item" role="presentation">
                                            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab"
                                               aria-controls="profile" aria-selected="false">Vaccine</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </header>
                        <!-- end if-page-have-nav-tabs -->
                        <div class="tab-content" id="myTabContent">
                            <!-- Disease Starts -->
                            <div class="tab-pane fade show active main-section" id="home" role="tabpanel"
                                 aria-labelledby="home-tab">
                                <div class="HealthIssue-section card px-0 py-2 mb-0" id="HealthIssue-section">
                                    <header class="row">
                                        <div class="col-md-7">
                                            <div class="clearfix">
                                                <form class="form-inline mb-2">
                                                    <div class="input-group input-group-sm mr-sm-2">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text cstm">Show</span>
                                                        </div>
                                                        <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.HealthIssue">
                                                            <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.HealthIssue">{{pp.text}}</option>
                                                        </select>
                                                        <div class="input-group-append">
                                                            <span class="input-group-text bg-white"><small>entries</small></span>
                                                        </div>
                                                    </div>
                                                    <div class="input-group input-group-sm table-search-filed">
                                                        <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search" ng-model="searchData.HealthIssue" />
                                                        <div class="input-group-append">
                                                            <button class="btn btn-default border-left-0" type="submit">
                                                                <i class="fas fa-search"></i>
                                                            </button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                        <!-- end col-md-8 -->
                                        <div class="col-md-5 text-right">
                                            <div class="dropdown dnld-file" data-toggle="tooltip" data-placement="top" title="Download File">
                                                <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <i class="fas fa-download"></i>
                                                </button>
                                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
                                                    <span class="dropdown-item">Choose an option:</span>
                                                    <a class="dropdown-item wait" export-excel table-id="'#PTM'" sheet-name="'ClassWiseSubject'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                    <a class="dropdown-item wait" onClick="$('#HealthIssue').tableExport({type:'csv',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                    <a class="dropdown-item wait" onClick="$('#HealthIssue').tableExport({type:'pdf',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/pdf.png" class="img-fluid exports-icon" alt="Excel image"> PDF</a>
                                                    <a class="dropdown-item wait" onClick="$('#HealthIssue').tableExport({type:'doc',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/word.png" class="img-fluid exports-icon" alt="Excel image"> Word</a>
                                                    <a class="dropdown-item wait" onClick="$('#HealthIssue').tableExport({type:'json',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/gmail.png" class="img-fluid exports-icon" alt="Excel image"> JSON</a>
                                                </div>
                                            </div>
                                            <a print-tbl table-id="'#HealthIssue'" href="#" class="btn btn-secondary btn-light btn-sm"
                                               data-toggle="tooltip" data-placement="top" title="Print">
                                                <i class="fas fa-print"></i>
                                            </a>

                                            <button type="button" class="btn btn-sm btn-success" ng-click="ClearHealthIssue()" id="add-HealthIssue" style="min-width: 90px">
                                                <i class="fas fa-plus w-t"></i> Add HealthIssue
                                            </button>
                                        </div>
                                    </header>
                                    <div>
                                        <div class="table-scroll-x-y sales-main-table table-scroll mb-2">
                                            <table id="HealthIssue" class="main-table table table-sm table-bordered bg-white">
                                                <thead class="text-nowrap">
                                                    <tr>

                                                        <th style="width: 74px">Id</th>
                                                        <th style="width: 170px">Name</th>
                                                        <th style="width: 100px">Severity</th>
                                                        <th style="width: 250px">Description</th>
                                                        <th style="width: 60px">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr dir-paginate="cl in HealthIssueList | filter:searchData.HealthIssue | itemsPerPage: perPage.HealthIssue" current-page="currentPages.HealthIssue" pagination-id="HealthIssuePagination">
                                                        <td>{{cl.OrderNo}}</td>
                                                        <td>{{cl.Name}}</td>
                                                        <td>{{cl.SeverityTypeName}}</td>
                                                        <td>{{cl.Description}}</td>
                                                        <td>
                                                            <a class="p-1" href="#" ng-click="GetHealthIssueById(cl)">
                                                                <i class="fas fa-edit text-info"></i>
                                                            </a>
                                                            <a class="p-1" href="#" ng-click="DelHealthIssueById(cl)">
                                                                <i class="fas fa-trash-alt text-danger"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- table end -->
                                    </div>
                                    <footer class="row align-items-center">
                                        <div class="col-md-12">
                                            <dir-pagination-controls pagination-id="HealthIssuePagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)"></dir-pagination-controls>
                                        </div>
                                    </footer>
                                </div>
                                <div id="HealthIssue-form">
                                    <div class="card card-default">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-6" id="meet-others">
                                                            <div class="form-group">
                                                                <label for="">Name</label> <span style="color:red">*</span>
                                                                <input type="text" class="form-control form-control-sm" ng-model="newHealthIssue.Name" />
                                                            </div>
                                                        </div>
                                                        <!-- fields after clicking radio button ends -->
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label>Severity</label><span style="color:red">*</span>
                                                                <select class="form-control  form-control-sm" ng-model="newHealthIssue.Severity">
                                                                    <option ng-repeat="s in SeverityList" ng-value="s.id" ng-selected="newHealthIssue.Severity==s.id">{{s.text}}</option>
                                                                </select>
                                                                
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label>Order No.</label>
                                                                <input type="number" class="form-control form-control-sm" ng-model="newHealthIssue.OrderNo" />
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label for="">Description</label>
                                                                <textarea class="form-control form-control-sm" rows="1" placeholder="" ng-model="newHealthIssue.Description"></textarea>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <!-- timeline time label -->
                                        <div class="time-label pb-3" style="margin-top: -10px">
                                            <span class="d-block border-0 p-1 text-center">
                                                <button type="button" class="btn btn-sm btn-info" ng-click="ClearHealthIssue()" id="back-to-list-HealthIssue"
                                                        style="min-width: 90px">
                                                    Back To List
                                                </button>
                                                <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearHealthIssue()">Reset</button>
                                                <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateHealthIssue()">{{newHealthIssue.Mode}}</button>
                                            </span>
                                        </div>
                                        <!-- /.timeline-label -->
                                    </div>
                                </div>
                                <!-- form area ends -->
                            </div>
                            <!-- Disease Ends -->
                            <!-- Vaccine Starts -->
                            <div class="tab-pane fade main-section" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                                <div class="card px-0 py-2 mb-0">
                                    <div id="Nametable">
                                        <header class="row">
                                            <div class="col-md-7">
                                                <div class="clearfix">
                                                    <form class="form-inline mb-2">
                                                        <div class="input-group input-group-sm mr-sm-2">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text cstm">Show</span>
                                                            </div>
                                                            <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                                            <select class="form-control border-left-0 border-right-0 pr-0" ng-model="perPage.Vaccine">
                                                                <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.Vaccine">{{pp.text}}</option>
                                                            </select>
                                                            <div class="input-group-append">
                                                                <span class="input-group-text bg-white"><small>entries</small></span>
                                                            </div>
                                                        </div>
                                                        <div class="input-group input-group-sm table-search-filed">
                                                            <input class="form-control form-control-navbar"
                                                                   type="search" placeholder="Search" ng-model="searchData.Vaccine"
                                                                   aria-label="Search">
                                                            <div class="input-group-append">
                                                                <button class="btn btn-default border-left-0"
                                                                        type="submit">
                                                                    <i class="fas fa-search"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                            <!-- end col-md-8 -->
                                            <div class="col-md-5 text-right">
                                                <div class="dropdown dnld-file" data-toggle="tooltip"
                                                     data-placement="top" title="Download File">
                                                    <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button"
                                                            id="dropdownMenuButton" data-toggle="dropdown"
                                                            aria-haspopup="true" aria-expanded="false">
                                                        <i class="fas fa-download"></i>
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-right"
                                                         aria-labelledby="dropdownMenuButton">
                                                        <span class="dropdown-item">Choose an option:</span>
                                                        <a class="dropdown-item wait" export-excel table-id="'#Vaccine'" sheet-name="'VaccineList'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                        <a class="dropdown-item wait" onClick="$('#Vaccine').tableExport({type:'csv',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                        <a class="dropdown-item wait" onClick="$('#Vaccine').tableExport({type:'pdf',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/pdf.png" class="img-fluid exports-icon" alt="Excel image"> PDF</a>

                                                    </div>
                                                </div>
                                                <a print-tbl table-id="'#Vaccine'" href="#" class="btn btn-secondary btn-light btn-sm"
                                                   data-toggle="tooltip" data-placement="top" title="Print">
                                                    <i class="fas fa-print"></i>
                                                </a>

                                                <button type="button"
                                                        class="btn btn-sm btn-success"
                                                        id="add-Vaccine-btn"
                                                        style="min-width: 90px" ng-click="ClearVaccine()">
                                                    <i class="fas fa-plus w-t"></i> Add Vaccine
                                                </button>
                                            </div>
                                        </header>

                                        <div class="table-scroll-x-y sales-main-table table-scroll">
                                            <table id="Vaccine" class="main-table table table-sm table-bordered bg-white">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 70px;">Order No</th>
                                                        <th style="width: 200px;">Vaccine Name</th>
                                                        <th style="width: 170px;"> Brand</th>
                                                        <th style="width: 150px;">Vaccine For</th>
                                                        <th style="width: 300px;">Description</th>
                                                        <th style="width: 80px;">Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr dir-paginate="cl in VaccineList | filter:searchData.Vaccine | itemsPerPage: perPage.Vaccine" current-page="currentPages.Vaccine" pagination-id="VaccinePagination">
                                                        <td>{{cl.OrderNo}}</td>
                                                        <td>{{cl.Name}}</td>
                                                        <td>{{cl.CompanyName}}</td>
                                                        <td>{{cl.VaccineForName}}</td>
                                                        <td>{{cl.Description}}</td>
                                                        <td>
                                                            <a class="p-1" href="#" ng-click="GetVaccineById(cl)">
                                                                <i class="fas fa-edit text-info"></i>
                                                            </a>
                                                            <a class="p-1" href="#" ng-click="DelVaccineById(cl)">
                                                                <i class="fas fa-trash-alt text-danger"></i>
                                                            </a>
                                                        </td>
                                                    </tbody>
                                            </table>
                                            <!-- table end -->
                                        </div>
                                        <footer class="row align-items-center">
                                            <div class="col-md-12">
                                                <dir-pagination-controls pagination-id="VaccinePagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)"></dir-pagination-controls>
                                            </div>
                                        </footer>
                                    </div>


                                    <div id="Nameform">
                                        <div id="groupform">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <div class="form-group">
                                                                <label for="">Vaccine Name</label> <span style="color:red">*</span>
                                                                <input type="text" class="form-control form-control-sm" ng-model="newVaccine.Name" />
                                                            </div>
                                                        </div>

                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label>Company Name (Brand)</label>
                                                                <input type="text" class="form-control form-control-sm" ng-model="newVaccine.CompanyName" />
                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="">Order No</label>
                                                                <input type="text" class="form-control form-control-sm" ng-model="newVaccine.OrderNo" />
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label>Vaccine For (Health Issue/Disease)</label> <span style="color:red">*</span>
                                                                <select class="form-control form-control-sm select2" ng-model="newVaccine.VaccineForId">
                                                                    <option ng-repeat="s in HealthIssueList" ng-value="s.HealthIssueId" ng-selected="newVaccine.VaccineForId==D.HealthIssueId">{{s.Name}}</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label>Description</label>
                                                                <textarea class="form-control form-control-sm" rows="1" ng-model="newVaccine.Description"></textarea>
                                                            </div>
                                                            
                                                        </div>
                                                      
                                                    </div>
                                                </div>
                                            </div>
                                            
                                            <div class="time-label">
                                                <span class="d-block border-0 p-3 text-center">
                                                    <button type="button"
                                                            class="btn btn-sm btn-info"
                                                            id="back-Vaccine-list"
                                                            style="min-width: 90px"
                                                            ng-click="ClearVaccine()">
                                                        Back to List
                                                    </button>
                                                    <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearVaccine()">Reset</button>
                                                    <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateVaccine()">{{newVaccine.Mode}}</button>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        <!--/. container-fluid -->
    </section>
   
</div>



