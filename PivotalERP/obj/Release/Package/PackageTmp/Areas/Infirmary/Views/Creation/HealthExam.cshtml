                                                    <label>Class</label>

                                                    <select class="form-control  form-control-sm" ng-model="newExamSetup.ClassTypeId">
                                                        <option ng-repeat="s in ClassList" ng-value="s.id" ng-selected="newExamSetup.ClassTypeId==s.id">{{s.text}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Section</label>


                                                    <select class="form-control  form-control-sm" ng-model="newExamSetup.SectionTypeId">
                                                        <option ng-repeat="s in SectionList" ng-value="s.id" ng-selected="newExamSetup.SectionTypeId==s.id">{{s.text}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Exam Type</label>


                                                    <select class="form-control  form-control-sm" ng-model="newExamSetup.ExamTypeId">
                                                        <option ng-repeat="s in ExamTypeList" ng-value="s.id" ng-selected="newExamSetup.ExamTypeId==s.id">{{s.text}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Copy To class</label>


                                                    <select class="form-control  form-control-sm" ng-model="newExamSetup.ClassCopyId">
                                                        <option ng-repeat="s in ClassList" ng-value="s.id" ng-selected="newExamSetup.ClassCopyId==s.id">{{s.text}}</option>
                                                    </select>
                                                </div>

                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Copy to section</label>
                                                    <select class="form-control  form-control-sm" ng-model="newExamSetup.SectionCopyId">
                                                        <option ng-repeat="s in SectionList" ng-value="s.id" ng-selected="newExamSetup.SectionCopyId==s.id">{{s.text}}</option>
                                                    </select>


                                                </div>

                                            </div>

                                        </div>
                                        <!-- form start end here -->
                                        <div>
                                            <!-- table input start  -->
                                            <div class="timeline-item">
                                                <div class="table-responsive">
                                                    <div class="table-scroll-x-y sales-main-table table-scroll mt-2">
                                                        <table id="" class="table table-hover table-bordered th-fixed-top bg-white w-100">
                                                            <thead class="text-nowrap">
                                                                <tr>
                                                                    <th style="width: 30px">S.No</th>
                                                                    <th style="width: 100px">Test Name</th>
                                                                    <th style="width: 100px">Default value</th>
                                                                    <th style="width: 100px">Default Remarks</th>
                                                                    <th class="text-center" style="width: 30px;">Action</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="S in newExamSetup.ExamCheckUpDetColl">
                                                                    <td>{{$index+1}}</td>
                                                                    <td>

                                                                        <select class="form-control  form-control-sm" ng-model="S.TestNameTypeId">
                                                                            <option ng-repeat="T in TestNameList" ng-value="T.TestNameId" ng-selected="S.TestNameTypeId==T.TestNameId">{{T.Name}}</option>
                                                                        </select>
                                                                    </td>
                                                                    <td><input type="text" class="form-control form-control-sm" ng-model="S.DefaultValue"></td>
                                                                    <td>
                                                                        <textarea class="form-control form-control-sm" rows="1" placeholder="" ng-model="S.DefaultRemarks"></textarea>
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <a class="p-1" ng-click="AddExamsetupDetails($index)">
                                                                            <i class="fas fa-plus text-secondary"></i>
                                                                        </a>
                                                                        <a class="p-1" ng-click="delExamSetupDetails($index)">
                                                                            <i class="fas fa-trash-alt text-danger"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>

                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- tabel input end -->
                                        </div>
                                        <div class="time-label">
                                            <span class="d-block border-0 p-3 text-center">
                                                <button type="button" class="btn btn-sm btn-info" id="back-btn" style="min-width: 90px;">Back to List</button>
                                                <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearHealthChekUpExam()">Reset</button>
                                                <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateHealthChekUpExam()">{{newExamSetup.Mode}}</button>
                                            </span>
                                        </div>
                                    </div>

                                </div>

                            </div>

                            @*----------Copy Exam Term Tab------------*@

                            <div class="tab-pane fade main-section" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                                <div id="section-content" class="card p-2">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>From Exam Term</label>

                                                <select class="form-control  form-control-sm" ng-model="NewCopyExam.FromExamTermId">
                                                    <option ng-repeat="s in ExamTypeList" ng-value="s.id" ng-selected="NewCopyExam.FromExamTermId==s.id">{{s.text}}</option>
                                                </select>

                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label>To Exam Term</label>

                                                <select class="form-control form-control-sm" ng-model="NewCopyExam.ToExamTermId" ng-options="s.id as s.text for s in ExamTypeListFiltered">
                                                    <option value="" disabled>Select an To exam term</option>
                                                </select>

                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group my-4">
                                                <button class="btn btn-sm btn-success" style="width:70px;" ng-click="SaveUpdateCopyExamType()"><i class="far fa-save mr-2"></i>{{NewCopyExam.Mode}}</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="table-scroll-x-y table-scroll" style="height:300px!important;">

                                        <table id="#ExamHealth" class="main-table table table-sm table-bordered bg-white">
