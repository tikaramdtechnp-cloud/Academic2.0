                                                        <td>{{ppC.ClassName}}  -   {{ppC.SectionName}}</td>
                                                        <td>{{ppC.TestName}}</td>
                                                        <td class="text-center">
                                                            <a class="p-1" href="#">
                                                                <i class="fas fa-edit text-info" ng-click="GetHealthChekUpExamById(ppC)"></i>
                                                            </a>
                                                            <a class="p-1" href="#">
                                                                <i class="fas fa-trash-alt text-danger" ng-click="DelHealthChekUpExamById(ppC,$index)"></i>
                                                            </a>
                                                        </td>
                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>

