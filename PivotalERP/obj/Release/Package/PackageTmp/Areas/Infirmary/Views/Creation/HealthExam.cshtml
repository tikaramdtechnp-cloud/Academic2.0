                        <div class="table-scroll-x-y table-scroll">

                            <table id="#ExamHealth" class="main-table table table-sm table-bordered bg-white">
                                <thead>
                                    <tr>
                                        <th style="width: 45px;">S.No.</th>
                                        <th style="width: 180px;">From Exam</th>
                                        <th style="width: 180px;">To Exam</th>
                                        <th style="width: 140px;">Copied Date</th>
                                        <th style="width: 200px;">Copied By</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="C in CopyExamTypeList">
                                        <td>{{$index+1}}</td>
                                        <td>{{C.FromExamName}}</td>
                                        <td>{{C.ToExamName}}</td>
                                        <td>{{C.LogDateTime}}</td>
                                        <td>{{C.CopyBy}}</td>                                   
                                    </tr>

                                </tbody>
                            </table>
                            <!-- table end -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Copy Exam Detail Modal Ends-->
</div>

