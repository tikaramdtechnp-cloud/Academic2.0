                                                    <th style="width: 80px;">Order No.</th>
                                                    <th style="width: 200px;">Name</th>
                                                    <th style="width: 200px;">Display Name</th>
                                                    <th style="width: 120px;">Code</th>

                                                    <th style="width: 60px;">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr dir-paginate="cl in InsuranceTypeList | filter:searchData.InsuranceType | itemsPerPage: perPage.InsuranceType" current-page="currentPages.InsuranceType" pagination-id="InsuranceTypePagination">
                                                    <td>{{cl.OrderNo}}</td>
                                                    <td>{{cl.Name}}</td>
                                                    <td>{{cl.DisplayName}}</td>
                                                    <td>{{cl.Code}}</td>
                                                    <td>
                                                        <a class="p-1" href="#" ng-click="GetInsuranceTypeById(cl)">
                                                            <i class="fas fa-edit text-info"></i>
                                                        </a>
                                                        <a class="p-1" href="#" ng-click="DelInsuranceTypeById(cl)">
                                                            <i class="fas fa-trash-alt text-danger"></i>
                                                        </a>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <!-- table end -->
                                    </div>
                                    <footer class="row align-items-center">
                                        <div class="col-md-12">
                                            <dir-pagination-controls pagination-id="InsuranceTypePagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)"></dir-pagination-controls>
                                        </div>
                                    </footer>
                                </div>
                                <!-- Complain Type Form Starts -->
                                <div id="InsuranceType-form" class="card">
                                    <div class="py-2">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Name</label>
                                                    <input type="text" placeholder="" class="form-control form-control-sm" ng-model="newInsuranceType.Name">
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Display Name</label>
                                                    <input type="text" placeholder="" class="form-control form-control-sm" ng-model="newInsuranceType.DisplayName">
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label>Code</label>
                                                    <input type="text" placeholder="" class="form-control form-control-sm" ng-model="newInsuranceType.Code">
                                                </div>
                                            </div>
                                            <div class="col-md-2">
                                                <div class="form-group">
                                                    <label>Order No.</label>
                                                    <input type="number"
                                                           class="form-control form-control-sm"
                                                           placeholder="" ng-model="newInsuranceType.OrderNo" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="time-label">
                                            <span class="d-block border-0 p-3 text-center">
                                                <button type="button" class="btn btn-sm btn-info" id="back-insurance-type" style="min-width: 90px;">Back to List</button>
                                                <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearInsuranceType()">Reset</button>
                                                <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateInsuranceType()">{{newInsuranceType.Mode}}</button>

                                            </span>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </section>
            <!-- /.row -->

        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
</div>

