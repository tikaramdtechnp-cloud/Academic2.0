                                    <label for="customCheckbox1"
                                           class="custom-control-label">
                                        Show Left Student
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="input-group-prepend">
                            <select class="input-group-text dropdown-toggle p-0" style="outline: none; border-top-right-radius: unset; border-bottom-right-radius: unset; margin-right: -3px; z-index: 1;" ng-model="newStudentPortfolio.SelectStudent">
                                <option ng-repeat="ss in StudentSearchOptions" ng-value="ss.value"
                                        ng-selected="ss.value==newStudentPortfolio.SelectStudent">
                                    {{ss.text}}
                                </option>
                            </select>
                            <select class="form-control select2 form-control-sm" all-student
                                    ng-model="newStudentPortfolio.StudentId" student-detail="newStudentPortfolio.StudentDetails"
                                    side-bar-data="newStudentPortfolio.SideBarData" search-by="newStudentPortfolio.SelectStudent" confirm-action="getStudentPortfolio()" include-left="newStudentPortfolio.ShowLeftStudent"></select>
                        </div>
                    </div>

