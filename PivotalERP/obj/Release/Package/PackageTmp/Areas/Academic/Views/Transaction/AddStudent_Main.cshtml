                                                        <td>
                                                            <a class="p-1" href="#" ng-click="Print(st.StudentId)">
                                                                <i class="fas fa-print text-info"></i>
                                                            </a>
                                                            <a class="p-1" href="#" ng-click="GetStudentById(st)">
                                                                <i class="fas fa-edit text-info"></i>
                                                            </a>
                                                            <a class="p-1" href="#" ng-click="DelStudentById(st)">
                                                                <i class="fas fa-trash-alt text-danger"></i>
                                                            </a>
                                                            <a class="p-1" href="#" ng-click="ShowSibling(st)">
                                                                <i class="fas fa-info text-info"></i>
                                                            </a>
                                                        </td>
                                                        <td>{{st.AutoNumber}}</td>
                                                        <td>{{st.RegdNo}}</td>
                                                        <td>{{st.ClassName}}</td>
                                                        <td>{{st.SectionName}}</td>
                                                        <td ng-show="AcademicConfig.ActiveSemester == true || AcademicConfig.ActiveClassYear == true">{{st.Semester}} {{st.ClassYear}}</td>
                                                        <td ng-show="AcademicConfig.ActiveBatch == true">{{st.Batch}}</td>
                                                        <td>{{st.RollNo}}</td>
                                                        <td>{{st.Name}}</td>
                                                        <td>{{st.Gender}}</td>
                                                        <td ng-show="LANG=='np'">{{st.DOB_BS}}</td>
                                                        <td ng-show="LANG=='np'">{{st.DOB_AD | dateFormat}}</td>
                                                        <td ng-hide="LANG=='np'">{{st.DOB_AD | dateFormat}}</td>
                                                        <td>{{st.FatherName}}</td>
                                                        <td>{{st.ContactNo}}</td>
                                                        <td>{{st.Address}}</td>
                                                        <td ng-show="viewStudent.ShowPhoto">
                                                            <img ng-click="ShowPersonalImg(st)" style="height:50px; width:50px;" class="rounded-circle" ng-src="{{ (st.PhotoPath && st.PhotoPath.length>0 ? st.PhotoPath : DefaultPhoto)}}" />
                                                            <label for="choose-file{{$index}}" class="p-1">
                                                                <i class="fas fa-file-upload"></i>
                                                            </label>
                                                            <input id="choose-file{{$index}}" type="file" accept=".jpg,.jpeg,.pdf,doc,docx,application/msword,.png" style="display: none;" file-model ng-model="st.Photo_TMP" fileread="st.PhotoData" be-data="st" after-selected="UploadStudentPhoto(st)" />
                                                        </td>

                                                    </tr>
                                                </tbody>
                                            </table>
