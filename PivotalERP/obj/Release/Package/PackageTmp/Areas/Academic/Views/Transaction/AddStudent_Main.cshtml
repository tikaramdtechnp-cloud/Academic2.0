                                                    <tr class="text-nowrap">
                                                        <th style="width:120px">Action</th>
                                                        <th style="width:50px" ng-click="sort('AutoNumber')">Id</th>
                                                        <th style="width: 100px" ng-click="sort('RegdNo')">{{'REGDNO_LNG' | translate}}</th>
                                                        <th style="width: 150px" ng-click="sort('ClassName')">Class</th>
                                                        <th style="width: 80px" ng-click="sort('SectionName')">Section</th>
                                                        <th style="width: 150px" ng-click="sort('Semester')" ng-show="AcademicConfig.ActiveSemester == true || AcademicConfig.ActiveClassYear == true">Semester/Year</th>
                                                        <th style="width: 150px" ng-click="sort('Batch')" ng-show="AcademicConfig.ActiveBatch == true">Batch</th>
                                                        <th style="width: 80px" ng-click="sort('RollNo')">Roll No.</th>
                                                        <th style="width: 250px" ng-click="sort('Name')">Name</th>
                                                        <th style="width: 100px" ng-click="sort('Gender')">Gender</th>
                                                        <th style="width: 110px" ng-click="sort('DOB_AD')" ng-show="LANG=='np'">DOB(B.S.)</th>
                                                        <th style="width: 110px" ng-click="sort('DOB_AD')" ng-show="LANG=='np'">DOB(A.D.)</th>
                                                        <th style="width: 110px" ng-click="sort('DOB_AD')" ng-hide="LANG=='np'">DOB</th>
                                                        <th style="width: 250px" ng-click="sort('FatherName')">Father Name</th>
                                                        <th style="width: 150px" ng-click="sort('ContactNo')">ContactNo</th>
                                                        <th style="width: 150px" ng-click="sort('Address')">Address</th>
