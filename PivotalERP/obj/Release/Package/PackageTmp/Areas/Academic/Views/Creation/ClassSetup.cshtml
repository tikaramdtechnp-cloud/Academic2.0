                                                <tr>
                                                    <th style="width: 50px;">S.No.</th>
                                                    <th style="width: 150px;">Name</th>
                                                    <th style="width: 200px;">Description</th>
                                                    <th style="width: 100px;">Order No</th>
                                                    <th style="width: 50px;">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr dir-paginate="cl in ClassGroupsList | filter:searchData.ClassGroup | itemsPerPage: perPage.ClassGroup" current-page="currentPages.ClassGroup" pagination-id="classgroupPagination">
