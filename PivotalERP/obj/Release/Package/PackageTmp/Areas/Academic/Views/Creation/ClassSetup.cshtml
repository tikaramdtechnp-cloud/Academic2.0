                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Name<span style="color:red">*</span></label>
                                                    <input type="text" name="" id="" class="form-control form-control-sm" ng-model="newClassGroups.Name">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Description</label>
                                                    <textarea class="form-control form-control-sm" rows="1"
                                                              placeholder="" ng-model="newClassGroups.Description"></textarea>
                                                </div>
                                            </div>
                                            <div class="col-sm-2 col-lg-2 col-md-2">
                                                <div class="form-group">
                                                    <label>Order No.</label>
                                                    <input type="number"
                                                           class="form-control form-control-sm"
                                                           placeholder="" ng-model="newClassGroups.OrderNo" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6 col-sm-12 col-lg-6">
                                                <div class="table-scroll-x-y table-scroll" style="max-height: calc(100vh - 15rem) ">
                                                    <table id="main-table" class="main-table table table-sm table-bordered bg-white">
                                                        <thead>
                                                            <tr>
                                                                <th style="width: 100px; z-index: 3" class="text-center;">
