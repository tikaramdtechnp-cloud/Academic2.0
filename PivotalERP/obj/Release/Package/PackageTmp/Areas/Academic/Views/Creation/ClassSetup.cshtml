                                                        <button type="button" class="btn btn-sm btn-info" id="back-btn-classgroup" style="min-width: 90px;">Back to List</button>
                                                        <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearClassGroups()">Reset</button>
                                                        <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdateClassGroups()">{{newClassGroups.Mode}}</button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>

