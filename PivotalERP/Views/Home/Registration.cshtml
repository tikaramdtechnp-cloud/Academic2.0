<body style="background:linear-gradient( rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.5) )" ng-app="myApp" ng-controller="regController" ng-init="LoadData()">
    <div class="container" style="background-color: aliceblue;">
        <div class="container">
            <section class="row" style="margin-left: 7px;margin-right: 7px;">
                <div class="row dark-bg" style="padding:0px;">
                    <img ng-src="{{Abt.ImagePath}}" class="img-responsive" style="max-height:200px;width:100%">
                </div>
            </section>
            <div class="card">
                <div class="card-header text-center">
                    <h3><b>{{Abt.Name}}</b></h3>
                    <label>{{Abt.Address}}</label>
                </div>
                <div compile class="card-body" ng-bind-html="Abt.Content | unsafe">
                </div>
            </div>
            <section class="personal-details">
                <div class="card">
                    <!-- personal Details Part starts -->
                    <div class="card-header" style="background-color: #ccc;">
                        <h5 class="mb-0">Personal Information</h5>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-9">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">First Name<span style="color:red">*</span></label>
                                            <input type="text" class="form-control form-control-sm" ng-model="newRegistration.FirstName">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Middle Name</label>
                                            <input type="text" class="form-control form-control-sm" ng-model="newRegistration.MiddleName">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Last Name<span style="color:red">*</span></label>
                                            <input type="text" class="form-control form-control-sm" ng-model="newRegistration.LastName">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Gender<span style="color:red">*</span></label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.Gender" ng-click="GenderChange()">
                                                <option ng-value="gen.id" ng-repeat="gen in GenderColl" ng-selected="gen.id==newRegistration.Gender">{{gen.text}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Date of Birth </label><span class="text-info" style="font-size:12px;"> (Age: {{newRegistration.DOBDet.age}})</span>
                                            <input type="text"
                                                   class="form-control form-control-sm" date-picker ng-model="newRegistration.DOB_TMP" date-detail="newRegistration.DOBDet">
                                        </div>
                                    </div>
                                    @*<div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Date of Birth</label>
                                            <input type="text" class="form-control form-control-sm"
                                                   date-picker ng-model="newRegistration.DOB_TMP" date-detail="newRegistration.DOBDet"
                                                   before-days="438000" after-days="0">
                                            </div>
                                    </div>*@
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Birth Certificate No</label>
                                            <input type="text" class="form-control form-control-sm" ng-model="newRegistration.BirthCertificateNo">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">{{'CAST_LNG' | translate}}</label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.CasteId" ng-change="CasteChange()">
                                                <option ng-value="c.CasteId" ng-repeat="c in CasteList" ng-selected="c.CasteId==newRegistration.CasteId">{{c.Name}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Nationality</label>
                                            <input type="text" class="form-control-sm form-control" data-ng-model="newRegistration.Nationality" list="lstNationality">
                                            <datalist id="lstNationality">
                                                <option data-ng-repeat="ttl in CountryList" value="{{ttl}}">
                                            </datalist>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Religion</label>
                                            <input type="text" class="form-control-sm form-control" data-ng-model="newRegistration.Religion" list="lstReligion">
                                            <datalist id="lstReligion">
                                                <option data-ng-repeat="ttl in ReligionList" value="{{ttl}}">
                                            </datalist>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Contact No.<span style="color:red">*</span></label>
                                            <input type="text"
                                                   class="form-control form-control-sm" ng-model="newRegistration.ContactNo">
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Email</label>
                                            <input type="text"
                                                   class="form-control form-control-sm" ng-model="newRegistration.Email">
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            @*<label for="">Admission applied for the class<span style="color:red">*</span> </label>*@
                                            <label for="">Appliesd Class<span style="color:red">*</span> </label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.ClassId" ng-change="CourseChange()">
                                                <option ng-value="cl.id" ng-repeat="cl in ClassList" ng-selected="cl.id==newRegistration.ClassId">{{cl.text}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <!--NEw Fielld added by Suresh on 1 Jestha-->
                                    @*<div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Shift</label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.ClassShiftId" ng-change="ShiftChange()">
                                                <option ng-repeat="S in ClassShiftColl " ng-value="S.ClassShiftId" ng-selected="newRegistration.Shift==S.ClassShiftId">{{S.Name}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">SLC/SEE</label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.SEETypeId"  ng-change="SEETypeChange()">
                                                <option ng-repeat="S in SEETypeList " ng-value="S.id" ng-selected="newRegistration.SEETypeId==S.ClassShiftId">{{S.text}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label for="">Plus 2</label>
                                            <select class="form-control form-control-sm select2" ng-model="newRegistration.PlusTwoId" ng-change="PlusTwoChange()">
                                                <option ng-repeat="S in PlusTwoTypeList " ng-value="S.id" ng-selected="newRegistration.PlusTwoId==S.id">{{S.text}}</option>
                                            </select>
                                        </div>
                                    </div>*@
                                    <!--Ends-->
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label for="" class="invisible">Photo</label>
                                <div class="person-photo-attachment-wrap text-center">
                                    <div class="img-wrap mx-auto">
                                        <img ng-src="{{newRegistration.PhotoData}}" class="img-fluid" alt="" id="imgPhoto1">
                                    </div>
                                    <button type="button" class="btn btn-outline-secondary btn-sm" data-toggle="modal" data-target="#updatePhoto">
                                        <i class="fas fa-camera w-t"></i> Upload Photo
                                    </button>
                                </div>

                            </div>
                        </div>

                    </div>

                    @*Parents Detail Starts*@
                    <div class="card-header" style="background-color: #ccc;">
