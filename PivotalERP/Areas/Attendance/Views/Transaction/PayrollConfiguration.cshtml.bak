


<script src="~/jslib/Attendance/PayrollConfiguration.js"></script>
<div class="hold-transition layout-navbar-fixed" ng-app="myApp" ng-controller="PayrollConfigurationController" ng-init="LoadData()">
    <section class="content">
        <div class="container-fluid" style="margin-top: -10px;">
            <section class="iframe-content-starts-here">
                <div>
                    <div class="card p-2">
                        <div class="card border">
                            <div class="p-2 card-header" style="background-color: #bdddab63">
                                <h6 class="mb-0">Journal Voucher</h6>
                            </div>
                            <div class="p-2 card-body">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">Voucher</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.JV_VoucherId">
                                                <option ng-repeat="P in VoucherTypeColl" ng-value="P.VoucherId" ng-selected="P.VoucherId==newPayrollConfig.JV_VoucherId">{{P.VoucherName }}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">CostClass</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.JV_CostClassId">
                                                <option ng-repeat="P in CostClassColl" ng-value="P.CostClassId" ng-selected="P.CostClassId==newPayrollConfig.JV_CostClassId">{{P.Name}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <label>Auto Generate Journal </label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HL-yes" name="HL" ng-value="true" ng-model="newPayrollConfig.JV_AutoGenerate">
                                                <label class="form-check-label" for="HL-yes">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HL-no" name="HL" ng-value="false" ng-model="newPayrollConfig.JV_AutoGenerate">
                                                <label class="form-check-label" for="HL-no">No</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label>Auto Cancel and re-generate Journal</label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HT-yesJCancel" name="HT" ng-value="true" ng-model="newPayrollConfig.JV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="HT-yesJCancel">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HT-noJCancel" name="HT" ng-value="false" ng-model="newPayrollConfig.JV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="HT-noJCancel">No</label>
                                            </div>
                                        </div>
                                    </div>


                                </div>

                            </div>
                        </div>

                        <div class="card border">
                            <div class="p-2 card-header" style="background-color: #bdddab63">
                                <h6 class="mb-0">Payment Voucher</h6>
                            </div>
                            <div class="p-2 card-body">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">Voucher</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.PV_VoucherId">
                                                <option ng-repeat="P in VoucherTypeColl" ng-value="P.VoucherId" ng-selected="P.VoucherId==newPayrollConfig.PV_VoucherId">{{P.VoucherName }}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">CostClass</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.PV_CostClassId">
                                                <option ng-repeat="P in CostClassColl" ng-value="P.CostClassId" ng-selected="P.CostClassId==newPayrollConfig.PV_CostClassId">{{P.Name}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <label>Auto Generate Voucher </label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HL-yesV" name="HLA" ng-value="true" ng-model="newPayrollConfig.PV_AutoGenerate">
                                                <label class="form-check-label" for="HL-yesV">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HL-noV" name="HLA" ng-value="false" ng-model="newPayrollConfig.PV_AutoGenerate">
                                                <label class="form-check-label" for="HL-noV">No</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label>Auto Cancel and re-generate Voucher</label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HT-yesVCancel" name="HTC" ng-value="true" ng-model="newPayrollConfig.PV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="HT-yesVCancel">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="HT-noVCancel" name="HTC" ng-value="false" ng-model="newPayrollConfig.PV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="HT-noVCancel">No</label>
                                            </div>
                                        </div>
                                    </div>


                                </div>

                            </div>
                        </div>

                        <div class="card border mb-0">
                            <div class="p-2 card-header" style="background-color: #bdddab63">
                                <h6 class="mb-0">Advance Voucher</h6>
                            </div>
                            <div class="p-2 card-body">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">Voucher</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.AV_VoucherId">
                                                <option ng-repeat="P in VoucherTypeColl" ng-value="P.VoucherId" ng-selected="P.VoucherId==newPayrollConfig.AV_VoucherId">{{P.VoucherName }}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="">CostClass</label>
                                            <select class="form-control form-control-sm select2" ng-model="newPayrollConfig.AV_CostClassId">
                                                <option ng-repeat="P in CostClassColl" ng-value="P.CostClassId" ng-selected="P.CostClassId==newPayrollConfig.AV_CostClassId">{{P.Name}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <label>Auto Generate Voucher </label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="AVHL-yesAV" name="AVHLA" ng-value="true" ng-model="newPayrollConfig.AV_AutoGenerate">
                                                <label class="form-check-label" for="AVHL-yesAV">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="AVHL-noAV" name="AVHLA" ng-value="false" ng-model="newPayrollConfig.AV_AutoGenerate">
                                                <label class="form-check-label" for="AVHL-noAV">No</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <label>Auto Cancel and re-generate Voucher</label>
                                        <div class="form-group clearfix">
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="AV-yesVCancel" name="AVHTC" ng-value="true" ng-model="newPayrollConfig.AV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="AV-yesVCancel">Yes</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input" type="radio" id="AV-noVCancel" name="AVHTC" ng-value="false" ng-model="newPayrollConfig.AV_AutoCancelRegenerate">
                                                <label class="form-check-label" for="AV-noVCancel">No</label>
                                            </div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                        </div>

                        <div class="p-2 card-body">
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="">No Of Decimals</label>
                                        <input class="form-control form-control-sm" type="number" ng-model="newPayrollConfig.NoOfDecimal" />
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="time-label pb-3">
                            <span class="d-block border-0 text-center">
                                <button type="button" class="btn btn-sm btn-default" style="min-width: 90px;" ng-click="ClearPayrollConfig()">Reset</button>
                                <button type="button" class="btn btn-sm btn-success" style="min-width: 90px;" ng-click="SaveUpdatePayrollConfig()">Save</button>
                            </span>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
</div>


