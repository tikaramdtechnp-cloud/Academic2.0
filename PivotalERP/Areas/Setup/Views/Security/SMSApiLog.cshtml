
<script src="~/jslib/setup/security.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="smsapiLogCtrl" ng-class="{waiting: loadingstatus=='running'}">
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:55px!important">
                @RenderPage("~/Views/Global/_WaitDialog.cshtml")
                <div class="card overflow-hidden">
                    <div class="card-header p-2 header-corner-triangle border">
                        <div class="row">
                            <div class="col-sm-7">
                                <h6 class="text-success">SMS API LOG:</h6>
                            </div>
                            <div class="input-group input-group-sm col-sm-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">From </span>
                                </div>
                                <input type="text" class="form-control form-control-sm" style="max-width:100px" ng-model="DateFrom_TMP" date-picker date-detail="DateFromDet" before-days="40" after-days="40" />
                            </div>
                            <div class="input-group input-group-sm col-sm-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">To </span>
                                </div>
                                <input type="text" class="form-control form-control-sm" style="max-width:100px" ng-model="DateTo_TMP" date-picker date-detail="DateToDet" before-days="40" after-days="40" />
                            </div>
                            <div class="col-sm-1">
                                <div class="form-group">
                                    <a href="#" class="btn btn-success btn-sm" style="float: right; max-width: 100px" data-toggle="tooltip" data-placement="top" title="" data-original-title="Load" ng-click="GetData()" ng-disabled="loadingstatus=='running'"><i class="fas fa-download"></i> Load</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card-body py-2 px-0">
                        <div class="row">
                            <div class="col-md-12">
                                <div id="datatable" style="height: calc(100vh - 11rem);width: 100%;" class="ag-theme-balham"></div>
                            </div>
                        </div>
                    </div>
                </div>

                        <footer class="main-footer control-btns py-2">
                            <div class="row">
                                <div class="col-sm-3">
                                    <div class="form-group row">
                                        <div class="input-group input-group-sm">
                                            <div class="input-group-prepend">
                                                <label class="mt-1 mr-2">Find </label>
                                            </div>
                                            <input type="text" name="table_search" class="form-control text-dark input-sm" placeholder="Search" ng-model="search" ng-change="onFilterTextBoxChanged()">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">

                                </div>
                                <div class="col-sm-6 text-right">
                                    <button type="button" class="btn btn-sm btn-secondary"><i class="fas fa-print" ng-disabled="loadingstatus=='running'"></i> Print</button>
                                    <button type="button" class="btn btn-sm btn-info" ng-click="onBtExport()"><i class="fas fa-file-export" ng-disabled="loadingstatus=='running'"></i> Export</button>
                                </div>
                            </div>
                        </footer>
</section>

        </div>

    </section>
</div>
