<script src="~/jslib/export/tableExport.js"></script>
<script src="~/jslib/export/jquery.base64.js"></script>
<script src="~/jslib/setup/PrintLog.js?v=20251115"></script>
<div class="hold-transition layout-navbar-fixed" ng-app="myApp" ng-controller="PrintlogController" ng-init="LoadData()">
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here">
                <div class="tab-content">
                    <div class="root">
                        <div class="card-primary card-tabs ">

                            <header class="if-page-have-nav-tabs px-2">
                                <div class="row upper-tab">
                                    <div class="col-md-12">
                                        <ul class="nav nav-tabs cstm-nav-tabs-border-bottom-active" id="myTab"
                                            role="tablist">
                                            <li class="nav-item" role="presentation">
                                                <a class="nav-link active" id="Marksheet-tab" data-toggle="tab"
                                                   href="#Marksheet" role="tab" aria-controls="Marksheet"
                                                   aria-selected="true">Marksheet</a>
                                            </li>
                                           
                                        </ul>
                                    </div>

                                </div>
                            </header>

                            <div class="tab-content" id="myTabContent">
                                <div class="tab-pane fade main-section active show " id="Marksheet" role="tabpanel" aria-labelledby="Marksheet-tab">
                                    <header class="row mt-2">
                                        <div class="col-md-7 col-sm-7">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group mb-1 d-flex">
                                                        <label class="mt-1 mr-2">Select</label>
                                                        <select class="form-control form-control-sm" ng-model="filter.ExamType" style="width:100%;">
                                                            <option ng-repeat="pp in ExamTypeColl" ng-value="pp.id" ng-selected="pp.id==filter.ExamType">
                                                                {{pp.text}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-8" ng-show="filter.ExamType == 1">
                                                    <div class="form-group d-flex mb-1">
                                                        <label class="mt-1" style="min-width:70px">Exam Type</label>
                                                        <select class="form-control form-control-sm select2" ng-model="filter.ExamTypeId" style="width:100%;">
                                                            <option ng-repeat="pp in ExamTypeList" ng-value="pp.id" ng-selected="pp.id == filter.ExamTypeId">
                                                                {{pp.text}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-8" ng-show="filter.ExamType == 2">
                                                    <div class="form-group mb-1 d-flex">
                                                        <label class="mt-1" style="min-width:110px">Exam Type Group</label>
                                                        <select class="form-control form-control-sm select2" ng-model="filter.ExamTypeGroupId" style="width:100%;">
                                                            <option ng-repeat="pp in ExamTypeGroupList" ng-value="pp.id" ng-selected="pp.id == filter.ExamTypeGroupId">
                                                                {{pp.text}}
                                                            </option>
                                                        </select>
                                                    </div>
                                                </div>
                                                
                                            </div>
                                        </div>
                                        <div class="col-md-5 col-sm-5 text-right">
                                            <form class="form-inline float-right ng-pristine ng-valid ng-submitted">
                                                <div class="input-group input-group-sm table-search-filed mr-1">
                                                    <input type="text" name="table_search" class="form-control float-right" placeholder="Search" ng-change="onFilterTextBoxChanged()" ng-model="search">
                                                    <div class="input-group-append">
                                                        <button class="btn btn-default border-left-0" type="submit">
                                                            <i class="fas fa-search"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                                <div class="dropdown dnld-file" data-toggle="tooltip"
                                                     style="margin-right:5px;"
                                                     data-placement="top" title="Download File">
                                                    <button class="btn btn-secondary btn-light btn-sm dropdown-toggle" type="button"
                                                            id="dropdownMenuButton" data-toggle="dropdown"
                                                            aria-haspopup="true" aria-expanded="false">
                                                        <i class="fas fa-download py-1"></i>
                                                    </button>
                                                    <div class="dropdown-menu dropdown-menu-right"
                                                         aria-labelledby="dropdownMenuButton">
                                                        <span class="dropdown-item">Choose an option:</span>
                                                        <a class="dropdown-item wait" export-excel table-id="'#main-table'" ng-click="onBtExportExcel()" sheet-name="'ClassWiseSubject'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                                        <a class="dropdown-item wait" ng-click="onBtExport()"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                                    </div>
                                                </div>
                                                <button class="btn btn-sm btn-success mr-2" ng-click="GetExamTypeData()">
                                                    <i class="fas fa-sync-alt"></i> Load
                                                </button>
                                            </form>
                                        </div>
                                    </header>
                                    <div id="main-table" class="card py-1 mb-0">
                                        <div>
                                            <div id="ExamTypeData" style="height: calc(100vh - 9rem);width: 100%;" class="ag-theme-alpine"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </section>
</div>


