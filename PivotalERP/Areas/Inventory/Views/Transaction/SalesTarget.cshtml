<div ng-app="myApp" ng-controller="SalesQuotation" ng-cloak>
    <div class="sale-page-wrap">
        <div class="overflow-hidden">
            @RenderPage("~/Views/Global/_WaitDialog.cshtml")
            @RenderPage("~/Views/Global/_LedgerDetails.cshtml")
            <!-- Page Content  -->
            <div class="content-wrapper-new">
                <div class="card">
                    <div class="card-header header-corner-triangle overflow-hidden">
                        <div class="row">
                            <h3 class="card-title">Sales Target</h3>
                        </div>
                    </div>
                    <div class="card-body">
                        <form action="">
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="form-group row">
                                        <label for="inputEmail3" class="col-sm-6 col-form-label">Target No. :</label>
                                        <div class="col-sm-6 col-md-6">
                                            <input type="text" class="form-control form-control-sm serial" placeholder="" ng-model="beData.AutoManualNo" readonly ng-hide="HideShow.AutoVoucherNo==true">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group row">
                                        <label for="inputEmail3" class="col-sm-6 col-form-label">Voucher Date :</label>
                                        <div class="col-sm-6 col-md-6">
                                            <input type="text" class="form-control form-control-sm float-right serial hideSideBar" name="VoucherDate" date-picker ng-model="beData.VoucherDate" date-detail="beData.VoucherDateDet" />
                                            @*<input type="text" class="form-control float-right serial hideSideBar" name="VoucherDate" voucher-date ng-model="beData.VoucherDate" readonly />*@
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-4">
                                    <div class="form-group row">
                                        <label for="inputEmail3" class="col-sm-6 col-form-label">Credit Limit :</label>
                                        <div class="col-sm-6 col-md-6">
                                            <input type="text" class="form-control form-control-sm serial hideSideBar" placeholder="" ng-model="beData.CreditLimit">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group row">
                                        <label for="inputEmail3" class="form-control form-control-sm col-sm-4 col-form-label">Party A/C No :</label>
                                        <div class="col-sm-8">
                                            <div class="d-flex">
                                                <div class="input-group">
                                                    <select autoShowHide="true" class="select2 serial" all-ledger ng-model="beData.PartyLedgerId" id="cboParty" style="width:95%" ledgertype="3"
                                                            placeholder="** Select Party Name **" showdetails="true" confirm-action="PartySelectionChange(beData)" ledger-detail="beData.PartyLedger" side-bar-data="beData.partySideBarData"></select>

                                                </div>
                                                <button type="button" class="btn btn-default ml-2" data-toggle="modal" data-target="#partyAccRightBtn">
                                                    <i class="fas fa-bars"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <!-- row -->
                        </form>
                    </div>
                    <!-- end form fiels -->
