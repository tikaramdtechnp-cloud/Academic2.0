                                        <th rowspan="2" class="text-center align-middle particular-col-width">Particulars</th>
                                        <th rowspan="2" class="text-center align-middle" style="width: 150px" ng-hide="HideShow.CurrentBalance==true">Closing Stock</th>
                                        @*<th rowspan="2" class="text-center align-middle" style="width: 150px" ng-hide="HideShow.ProductLedger==true">Sales Ledger</th>*@
                                        <th ng-colspan=" HideShow.BilledQty==true ? 2 : 1" scope="colgroup" class="text-center" style="width: 200px">Quantity</th>
                                        @*<th colspan="2" scope="colgroup" class="text-center" style="width: 200px" ng-hide="HideShow.FreeQty==true">Fee</th>
                                            <th rowspan="2" class="text-center align-middle" style="width: 150px" ng-hide="HideShow.AlternetUnit==true">Alternate Unit</th>*@
                                        <th rowspan="2" class="text-center align-middle" style="width: 100px">Rate</th>
                                        <th rowspan="2" class="text-center align-middle" style="width: 100px">Per</th>
                                        @*<th colspan="2" scope="colgroup" class="text-center" style="width: 180px" ng-hide="HideShow.Discount==true">Discount</th>
                                            <th colspan="2" scope="colgroup" class="text-center" style="width: 150px" ng-hide="HideShow.Scheme==true">Scheme</th>*@
                                        <th rowspan="2" class="text-center align-middle" style="width: 150px">Amount</th>
                                        <th rowspan="2" class="text-center align-middle" style="width: 80px">Action</th>
                                    </tr>

                                </thead>
                                <tbody>
                                    <tr ng-repeat="itemDet in beData.ItemDetailsColl">
                                        <th align="center">
                                            {{$index+1}}
                                        </th>
                                        <td>
                                            <select autoShowHide="true" class="form-control select2 serial" all-product ng-model="itemDet.ProductId" style="width:100%"
                                                    placeholder="** Select Product  **" showdetails="true" confirm-action="ProductSelectionChange(itemDet)" product-detail="itemDet.productDetail" side-bar-data="itemDet.sideBarData"></select>
                                        </td>
                                        <td ng-hide="HideShow.CurrentBalance==true">
                                            <input type="text" class="form-control" readonly name="Current Balance" ng-model="itemDet.ClosingQty">
                                        </td>

                                        <td><input type="text" number-input class="form-control serial hideSideBar" name="ActualQty" style="text-align:center" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'aQty')" ng-model="itemDet.ActualQty" ng-style="itemDet.IsNegativeQty==true && { 'color' : 'red' }"></td>
                                        <td ng-hide="HideShow.BilledQty==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:center" name="BilledQty" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'bQty')" ng-model="itemDet.BilledQty"></td>
                                        <td ng-hide="HideShow.FreeQty==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:center" name="FreeQty" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'fQty')" ng-model="itemDet.FreeQty"></td>
                                        <td ng-hide="HideShow.FreeQty==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" name="TotalQty" onfocus="this.select();" ng-model="itemDet.TotalQty"></td>
                                        <td ng-hide="HideShow.AlternetUnit==true"><input type="text" class="form-control hideSideBar" name="Alternet Unit" ng-model="itemDet.AlternetUnitDet"></td>
                                        <td><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" name="Rate" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'rate')" noofdecimal="itemDet.RateDecimal" ng-model="itemDet.Rate" ng-readonly="itemDet.CanEditRate==false"></td>
                                        <td>
                                            <input type="text" class="form-control hideSideBar" readonly name="Unit" ng-model="itemDet.UnitName">
                                        </td>
                                        <td ng-hide="HideShow.Discount==true || HideShow.DiscountPer==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'disPer')" ng-model="itemDet.DiscountPer"></td>
                                        <td ng-hide="HideShow.Discount==true || HideShow.DiscountAmt==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'disAmt')" ng-model="itemDet.DiscountAmt"></td>
                                        <td ng-hide="HideShow.Scheme==true || HideShow.SchemePer==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'schPer')" ng-model="itemDet.SchamePer"></td>
                                        <td ng-hide="HideShow.Scheme==true || HideShow.SchemeAmt==true"><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'schAmt')" ng-model="itemDet.SchameAmt"></td>
                                        <td><input type="text" number-input class="form-control serial hideSideBar" style="text-align:right" onfocus="this.select();" name="Amount" ng-model="itemDet.Amount" readonly ng-blur="AddRowInItemDetails($index,true)"></td>
                                        <td>
                                            <div class="btn-group btn-group-sm">
                                                <a href="#" class="btn btn-info" data-toggle="tooltip" data-placement="top" title="" data-original-title="Add Row" ng-click="AddRowInItemDetails($index,false)"><i class="fas fa-plus"></i></a>
                                                <a href="#" class="btn btn-danger" data-toggle="tooltip" data-placement="top" title="" data-original-title="Delete Row." ng-click="delRowFromItemDetails($index)"><i class="fas fa-trash"></i></a>
                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- card-body -->
                    <div class="card-body pt-0">
                        <div class="row">
                            <div class="col-md-8 col-xl-9">
                                <div class="form-group">
                                    <input type="text" list="lstNarration" class="form-control form-control-sm hideSideBar" name="" id="" rows="3" placeholder="Narration" ng-model="beData.Narration">
                                    <datalist id="lstNarration">
                                        <option ng-repeat="x1 in NarrationList track by $index" value="{{x1}}">
                                    </datalist>
                                </div>
                            </div>
                            <div class="col-md-4 col-xl-3">
                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-5 col-sm-6 col-form-label font-weight-bold">Sub Total:</label>
                                    <div class="col-7 col-sm-6 col-md-6">
                                        <input type="text" number-input class="form-control form-control-sm hideSideBar" style="text-align:right" placeholder="" readonly ng-model="beData.SubTotal">
                                    </div>
                                </div>
                                <div class="form-group row" ng-repeat="ledAll in beData.AditionalCostColl">
                                    <label for="inputEmail3" class="col-5 col-sm-6 col-form-label font-weight-bold">{{ledAll.Name}}:</label>
                                    <div class="col-7 col-sm-6 col-md-6">
                                        <input type="text" number-input class="form-control form-control-sm hideSideBar" style="text-align:right" placeholder="" ng-model="ledAll.Amount" ng-readonly="ledAll.CanEditAmount==false">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- /.card body -->
                </div>
                <div style="height: 30px"></div>
            </div>
        </div>
        <!-- /.container-fluid -->
    </div>

    <footer class="main-footer control-btns pt-0">
        <div class="secondary-footer mb-2 mt-2" style="padding-left: 27px; padding-right: 40px;">
            <div class="row">
                <div class="col-md-8 col-xl-9">

                </div>
                <div class="col-md-4 col-xl-3">
