                            <td ng-hide="HideShow.FreeQty==true"><input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" name="TotalQty" onfocus="this.select();" ng-model="itemDet.TotalQty"></td>
                            <td ng-hide="HideShow.AlternetUnit==true"><input type="text" class="form-control form-control-sm hideSideBar" name="Alternet Unit" ng-model="itemDet.AlternetUnitDet"></td>
                            <td><input type="text" number-input class="form-control form-control-sm serial hideSideBar" style="text-align:right" name="Rate" onfocus="this.select();" ng-change="ChangeItemRowValue(itemDet,'rate')" ng-model="itemDet.Rate" ng-readonly="itemDet.CanEditRate==false"></td>
