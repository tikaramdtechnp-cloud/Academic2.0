                    <div class="time-label">
                        <span style="border:1px solid #ececec">Facilities</span>
                    </div>

                    <div class="timeline-item">
                        <div class="ml-5">
                            <div class="table-scroll-x-y table-scroll">
                                <table id="Class" class="main-table table table-sm table-bordered bg-white" style="height: auto;">
                                    <thead>
                                        <tr>
                                            <th style="width: 50px;" class="text-center">S.No</th>
                                            <th style="width: 200px;">Facility Name</th>
                                            <th style="width: 100px;" class="text-center">Is Available?</th>

                                            <th style="width: 200px;" class="text-center">Source</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="A in newGeneralInfoDet.GeneralInfoFacilitiesColl">
                                            <td class="text-center">{{$index+1}}</td>
                                            <td>{{A.Name}}</td>
                                            <td class="text-center">
                                                <div class="custom-control custom-checkbox">
                                                    <input class="custom-control-input" type="checkbox" id="chkbox{{$index+1}}" ng-model="A.IsAvailable"
                                                           ng-change="handleFacilityChange(A)">
                                                    <label for="chkbox{{$index+1}}" class="custom-control-label"></label>
