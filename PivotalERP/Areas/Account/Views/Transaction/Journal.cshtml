                                    {{ beData.DrAmount |number:SelectedVoucher.NoOfDecimalPlaces }}
                                </label>
                            </div>
                        </div>
                        <div class=" col-md-2">
                            <div class="form-group row mb-1">
                                <label for="" class="col-md-2">CR.</label>
                                <label class="form-control form-control-sm col-md-9">
                                    {{ beData.CrAmount |number:SelectedVoucher.NoOfDecimalPlaces }}
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-md-4">
                            <button type="button" class="btn btn-sm btn-default" data-toggle="modal" data-target="#documentAttachModal">
                                Document Attach ({{ (beData.AttechFiles.length+(beData.DocumentColl ? beData.DocumentColl.length : 0)) }} )
                            </button>

                        </div>
                        <div class="col-md-8">
                            <span class="d-block text-right">
                                @*<button type="button" class="btn btn-default" style="min-width: 80px;" data-toggle="modal" data-target="#tds">Test</button>*@
                                <button type="button" class="btn btn-sm btn-default" style="min-width: 80px;" ng-click="SearchData()">Search</button>
                                <button type="button" class="btn btn-sm btn-default" style="min-width: 80px;" ng-click="Print()">Re-Print</button>
                                <button type="button" class="btn btn-sm btn-default" style="min-width: 80px;" ng-click="Clear()">Reset</button>
                                <button type="button" class="btn btn-sm btn-success" style="min-width: 80px;" ng-click="SaveJournal()" ng-disabled="loadingstatus=='running'">{{beData.Mode}}</button>
                            </span>
                        </div>
                    </div>

                </div>
            </footer>
            <div class="modal fade sales-quotation-page-modal document-attach-modal" id="searVoucherRightBtn" tabindex="-1" role="dialog" aria-labelledby="partyAccRightBtn" aria-hidden="true" style="margin-top:20px!important">
                <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-lg" role="document" style="min-width: 1080px">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h6 class="modal-title" id="exampleModalCenterTitle">Search Journal</h6>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body gradient-bg p-2">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group mb-0 row">
                                        <label for="" class="col-sm-2 col-form-label col-form-label-sm text-center">Search By</label>
                                        <div class="col-sm-10">
                                            <div class="input-group input-group-sm">
                                                <div class="input-group-prepend">
                                                    <select class="form-control form-control-sm" style="width: 100%;" ng-model="paginationOptions.SearchColDet">
                                                        <option ng-value="asp" ng-repeat="asp in VoucherSearchOptions">{{asp.text}}</option>
                                                    </select>
                                                </div>
                                                <input type="text" class="form-control" placeholder="" ng-model="paginationOptions.SearchVal">
                                                <div class="input-group-append">
                                                    <div class="input-group-text">
                                                        <span class="fas fa-search" ng-click="ReSearchData(-1)"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <form class="float-right mb-2">
                                        <div class="form-row align-items-center">
                                            <div class="d-flex align-items-center">
                                                <label class="mr-sm-2 mb-0" for="inlineFormCustomSelect">Show</label>
                                                <input type="text" ng-model="paginationOptions.pageSize" class="form-control form-control-sm input-sm" placeholder="NoOfRows" @*ng-blur="ReSearchData(-2)"*@>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="">
                                <table id="main-table" class="main-table table table-sm table-striped table-bordered mb-1">
                                    <thead>
                                        <tr>
                                            <th style="z-index: 3; background-color: #ececec !important;">Voucher No</th>
                                            <th style="z-index: 3; background-color: #ececec !important;">RefNo</th>
                                            <th style="z-index: 3; background-color: #ececec !important;" ng-show="GenConfig.DateStyle == 1">Date</th>
                                            <th style="z-index: 3; background-color: #ececec !important;" ng-show="GenConfig.DateStyle == 2">Miti</th>
                                            <th style="z-index: 3; background-color: #ececec !important;">Voucher Name</th>
                                            <th style="z-index: 3; background-color: #ececec !important;">CostClass</th>
                                            <th style="z-index: 3; background-color: #ececec !important;">Party</th>
                                            <th style="z-index: 3; background-color: #ececec !important;">Amount</th>
                                            <th class="cstm-sticky" style="z-index: 5; right:0; background-color: #ececec !important;">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr dir-paginate="sd in SearchDataColl |orderBy:sortKey:reverse|itemsPerPage:paginationOptions.pageSize" total-items="paginationOptions.TotalRows">
                                            <td align="center">{{sd.VoucherNo}}</td>

                                            <td>{{sd.RefNo}}</td>
                                            <td align="center" ng-show="GenConfig.DateStyle == 1">{{sd.VoucherDate |dateFormat}}</td>
                                            <td align="center" ng-show="GenConfig.DateStyle == 2">{{sd.VoucherMiti}}</td>
                                            <td>{{sd.VoucherName}}</td>
                                            <td>{{sd.CostClass}}</td>
                                            <td>{{sd.PartyName}}</td>
                                            <td align="right">{{sd.Amount |number:3}}</td>
                                            <td class="text-center cstm-sticky" style="z-index: 3; right: 0;  background-color: white !important;">
                                                <a class="p-1" href="#" ng-click="GetTransactionById(sd)" ng-hide="ButtonED.IRD==true">
                                                    <i class="fas fa-edit text-info"></i>
                                                </a>
                                                <a class="p-1" href="#" ng-click="GetTransactionById(sd)" ng-hide="ButtonED.IRD==false">
                                                    <i class="fa fa-eye text-info"></i>
                                                </a>
                                                <a class="p-1" href="#" ng-click="DelTransactionById(sd)" ng-hide="ButtonED.IRD==true">
                                                    <i class="fas fa-trash-alt text-danger"></i>
                                                </a>
                                                <a class="p-1" href="#" ng-click="PrintVoucher(sd.TranId,sd.VoucherId)">
                                                    <i class="fas fa-print text-primary"></i>
                                                </a>
                                            </td>
                                        </tr>
                                    </tbody>

                                </table>
                                <dir-pagination-controls max-size="paginationOptions.pageSize" direction-links="true" boundary-links="true"
                                                         on-page-change="ReSearchData(newPageNumber)">
                                </dir-pagination-controls>
                            </div>

                        </div>
                        <!-- end modal body -->
                        <div class="modal-footer justify-content-between align-items-center py-2">
                            <h6 class="mb-0">Total Record: {{paginationOptions.TotalRows}}</h6>
                            <div class="">
                                <button type="button" class="btn btn-sm btn-outline-secondary" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>



