                                                <th style="width: 60px; background-color: #ececec !important">DR/CR</th>
                                                <th style="width: 200px; background-color: #ececec !important;z-index:3">Particulars</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 1 }:true" rowspan="2" class="text-center align-middle" style="width: 120px;background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th ng-show="SelectedVoucher.ActiveBranch==true" style="width: 150px; background-color: #ececec !important">For Branch</th>
                                                <th style="width: 150px; background-color: #ececec !important">Current Bal.</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 5 }:true" rowspan="2" class="text-center align-middle" style="width: 120px;background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th style="width: 180px; background-color: #ececec !important" ng-show="SelectedVoucher.UseSubLedger==true">Sub Ledger</th>
                                                <th style="width: 120px; background-color: #ececec !important">Dr.Amount</th>
                                                <th style="width: 120px; background-color: #ececec !important">Cr.Amount</th>
                                                <th ng-show="SelectedVoucher.EachNarrationEntry==true" style="width: 150px; background-color: #ececec !important">Narration</th>
                                                <th ng-repeat="ud in SelectedVoucher.VoucherProductUDFColl  | filter: { FieldAfter: 0 }:true" rowspan="2" class="text-center align-middle" style="width: 120px;background-color:#ececec!important;">{{ud.Label}}</th>
                                                <th class="text-center align-middle" style="width: 60px; background-color: #ececec !important">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="ledAllocation in beData.LedgerAllocationColl">
                                                <td align="center">{{$index+1}}</td>
                                                <td>
                                                    <select class="form-control form-control-sm rounded-0" ng-model="ledAllocation.DrCr" ng-change="ChangeDrCr(ledAllocation)">
                                                        <option ng-value="d.id" ng-repeat="d in DrCrList" ng-selected="d.id==ledAllocation.DrCr">{{d.text}}</option>
                                                    </select>
                                                </td>
                                                <td>
                                                    <select autoShowHide="true" class="form-control form-control-sm select2 serial" all-ledger ng-model="ledAllocation.LedgerId" ledgertype="1" fortransaction="true"
                                                            placeholder="** Select Ledger Name **" showdetails="true" confirm-action="ChangeParticularLedger(ledAllocation)" ledger-detail="ledAllocation.LedgerDetails" side-bar-data="ledAllocation.partySideBarData" voucher-id="SelectedVoucher.VoucherId" vouchertype="3"></select>

                                                </td>
                                                <td ng-repeat="uf in ledAllocation.UDFFeildsColl | filter: { FieldAfter: 1 }:true ">
                                                    <udf-field udf="uf" voucher="SelectedVoucher"></udf-field>
                                                </td>
                                                <td ng-show="SelectedVoucher.ActiveBranch==true">
                                                    <select class="form-control form-control-sm select2" style="width: 100%; background-color: #fff" ng-model="ledAllocation.ForBranchId" data-placeholder="**For Branch**">
                                                        <option ng-repeat="cc in BranchColl track by cc.BranchId" ng-value="cc.BranchId" ng-selected="cc.BranchId == ledAllocation.ForBranchId">
                                                            {{cc.Name}}
                                                        </option>
                                                    </select>
                                                </td>
                                                <td>{{ledAllocation.CurrentBal |accountFormat}}</td>
                                                <td ng-repeat="uf in ledAllocation.UDFFeildsColl | filter: { FieldAfter: 5 }:true ">
                                                    <udf-field udf="uf" voucher="SelectedVoucher"></udf-field>
                                                </td>
                                                <td ng-show="SelectedVoucher.UseSubLedger==true">
                                                    <select autoShowHide="true" class="form-control form-control-sm select2 serial" all-cost-center ng-model="ledAllocation.SubLedgerId"
                                                            placeholder="** Select Sub Ledger **" showdetails="false" costcenter-detail="ledAllocation.SubLedgerDetails" side-bar-data="ledAllocation.subLedgerSideBarData"></select>
                                                </td>
                                                <td><input type="text" number-input noofdecimal="SelectedVoucher.NoOfDecimalPlaces" style="text-align:right" onfocus="this.select();" class="form-control form-control-sm" ng-model="ledAllocation.DrAmount" ng-blur="AddRowInLedgerAllocation($index,true)" ng-change="ChangeDrCrAmount(ledAllocation)" ng-disabled="ledAllocation.DrCr==2" positive="true" ng-style="ledAllocation.IsNegativeBudget==true && { 'color' : 'red' }"></td>
                                                <td><input type="text" number-input noofdecimal="SelectedVoucher.NoOfDecimalPlaces" style="text-align:right" onfocus="this.select();" class="form-control form-control-sm" ng-model="ledAllocation.CrAmount" ng-blur="AddRowInLedgerAllocation($index,true)" ng-change="ChangeDrCrAmount(ledAllocation)" ng-disabled="ledAllocation.DrCr==1" positive="true"></td>

                                                <td ng-show="SelectedVoucher.EachNarrationEntry==true"><input type="text" onfocus="this.select();" class="form-control form-control-sm" ng-model="ledAllocation.Narration"></td>
                                                <td ng-repeat="uf in ledAllocation.UDFFeildsColl | filter: { FieldAfter:0 }:true ">
                                                    <udf-field udf="uf" voucher="SelectedVoucher"></udf-field>
                                                </td>
                                                <td class="text-center">
                                                    <a class="p-1" href="#" data-original-title="Add Row" ng-click="AddRowInLedgerAllocation($index)">
                                                        <i class="fas fa-plus text-info"></i>
                                                    </a>
                                                    <a class="p-1" href="#" data-original-title="Delete Row." ng-click="delRowLedgerAllocation($index)">
                                                        <i class="fas fa-trash-alt text-danger"></i>
                                                    </a>

                                                </td>
                                            </tr>

                                        </tbody>
                                    </table>
                                </div>

                            </div>

                        </div>
