                    </div>
                </div>

            </div>
            <footer class="main-footer control-btns pt-0 pb-2">
                <div class="secondary-footer mb-2 mt-2">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="form-group mb-1">
                                <textarea type="text" list="lstNarration" class="form-control form-control-sm hideSideBar" name="" id="" rows="1" placeholder="Narration" ng-model="beData.Narration"></textarea>
                                <datalist id="lstNarration">
                                    <option ng-repeat="x1 in NarrationList track by $index" value="{{x1}}">
                                </datalist>
