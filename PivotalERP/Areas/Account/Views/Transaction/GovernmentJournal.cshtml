                        <div class="card mt-2 bodypart">
                            <div class="demo-container">
                                <div class="demo-header">
                                    <div class="card-header header-corner-triangle overflow-hidden py-1" ng-hide="HideShow.CostClass==true && HideShow.VoucherType==true">
                                        <div class="row">
                                            <h3 class="card-title mr-4">{{SelectedVoucher && SelectedVoucher.VoucherName ? SelectedVoucher.VoucherName : 'Journal'}}</h3>
                                            <div class="col-md-4" ng-hide="HideShow.VoucherType==true">
