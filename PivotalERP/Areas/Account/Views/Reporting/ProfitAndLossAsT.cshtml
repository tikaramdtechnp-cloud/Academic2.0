
<script>
    var EntityId = "@Convert.ToInt32(Dynamic.BusinessEntity.Global.RptFormsEntity.ProfitAndLossAsTFormat)";
</script>
<script src="~/jslib/account/reporting/ProfitLossAsT.js?v=20251115"></script>
@*<script src="~/jslib/account/reporting/ProfitAndLossAsT.js?v=20240131"></script>*@
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="ProfitAndLossAsTController" ng-init="LoadData()">
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here" style="padding-top:50px!important">
                <div class="tab-content">
                    <div id="day-book-section">
                        <div class="card py-2 mb-0">
                            <div class="row">
                                <div class="col-sm-4">
                                    <div class="d-inline-block">
                                        <div class="input-group input-group-sm mb-0">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">From:</span>
                                            </div>
                                            <input class="form-control text-dark input-sm" type="text" date-picker ng-model="newProfitAndLossAsT.DateFrom_TMP" style="width:100px" date-detail="newProfitAndLossAsT.DateFromDet" />
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">To:</span>
                                            </div>
                                            <input class="form-control text-dark input-sm" type="text" date-picker ng-model="newProfitAndLossAsT.DateTo_TMP" style="width:100px" date-detail="newProfitAndLossAsT.DateToDet" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3" >
                                    <div class="row"ng-show="BranchList.length>1">
                                        <div class="col-md-4">
                                            <label>Branch</label>
                                        </div>
                                        <div class="col-md-8">
                                            <select class="form-control form-control-sm select2" ng-model="newProfitAndLossAsT.BranchId" data-placeholder="*Select Branch*">
                                                <option ng-value="cl.id" ng-repeat="cl in BranchList" ng-selected="cl.id==newProfitAndLossAsT.BranchId">{{cl.text}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-5 text-right">
                                    <button type="button" class="btn btn-sm btn-success load" style="min-width: 70px;" ng-click="LoadReport()"><i class="fas fa-sync-alt mr-2"></i>Load</button>
                                    <a href="#" class="btn btn-default" data-toggle="tooltip" data-placement="top" title="" data-original-title="Load" ng-click="Print()" ng-disabled="loadingstatus=='running'"><i class="fas fa-print"></i></a>
                                    <a href="#" class="btn btn-default"  data-toggle="tooltip" data-placement="top" title="Export To Excel" data-original-title="Export To Excel" ng-click="DownloadAsXls()" ng-disabled="loadingstatus=='running'"><i class="fas fa-file-excel"></i></a>
                                </div>
                                
                            </div>
                        </div>
                        <div class="card mb-0">

                            <div class="row">

                                <div class="col-md-6">
                                    <div id="datatable" style="height: calc(100vh - 7rem); max-width: 100%;" class="ag-theme-alpine" ag-grid="gridOptions"></div>
                                </div>
                                <div class="col-md-6">
                                    <div id="datatable1" style="height: calc(100vh - 7rem); max-width: 100%;" class="ag-theme-alpine" ag-grid="gridOptions1"></div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>

            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
    <!-- Add ReportTemplates Modal Dailog -->
    <div class="modal fade bd-example-modal-xl sales-invoice-page-modal" id="FrmPrintReport" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenter2Title" aria-hidden="true" style="margin-top:30px;">
        <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title" id="exampleModalCenterTitle">Report Viewer</h6>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <iframe id="frmRpt" style='width: 100%; height: 600px; background-color: white; visibility:visible;' frameborder='0' scrolling='yes'></iframe>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>