<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="marksheetRptController" ng-init="LoadData()">
    <!-- Main content -->
    <section class="content">

        <div class="container-fluid">
            <section class="iframe-content-starts-here">
                <div class="tab-content">
                    <!-- Normal Tabulation Starts -->
                    <div class="tab-pane fade show active main-section" id="home" role="tabpanel"
                         aria-labelledby="home-tab">
                        <div class="card mb-0">
                            <div class="card-body p-0">
                                <div class="row mt-3">
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label>From Date</label>
                                            <input type="text" id="dtDateFrom"
                                                   class="form-control form-control-sm" ng-model="newDailyVoucher.FromDate_TMP" date-picker date-detail="newDailyVoucher.FromDateDet" before-days="360" after-days="0">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <label>To Date </label>
                                        <input type="text" id="dtDateTo"
                                               class="form-control form-control-sm" ng-model="newDailyVoucher.ToDate_TMP" date-picker date-detail="newDailyVoucher.ToDateDet" before-days="360" after-days="0">
                                    </div>
                                    <div class="col-md-5">
                                        <label>Voucher </label>
                                        <select name="comment-group" id="" form="" class="form-control form-control-sm select2" multiple ng-model="newDailyVoucher.VoucherIdColl">
                                            <option ng-value="0" ng-selected="true">Select Voucher</option>
                                            <option ng-value="rt.VoucherId" ng-repeat="rt in newDailyVoucher.VoucherColl">{{rt.VoucherName}}</option>
                                        </select>
                                    </div>
                                    <div class="col-md-3">
                                        <label>Report </label>
                                        <select name="comment-group" id="" form="" class="form-control form-control-sm select2" ng-change="PrintVoucher()" ng-model="newDailyVoucher.RptTranId">
                                            <option ng-value="0" ng-selected="true">Select Report</option>
                                            <option ng-value="rt.RptTranId" ng-repeat="rt in newDailyVoucher.TemplatesColl">{{rt.ReportName}}</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="row px-2" id="schedule-report-table">
                                    <iframe id="frmRptTabulation" style='width: 100%;height:calc(100vh - 8rem);background-color: white;visibility: visible;' frameborder='0' scrolling='yes'></iframe>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Normal Tabulation Ends -->

                </div>

            </section>
            <!-- /.row -->
        </div>
        <!--/. container-fluid -->
    </section>
    <!-- /.content -->
