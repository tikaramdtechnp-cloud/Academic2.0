                                        <option>Employee Name1</option>
                                        <option>EmployeeName2</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Date</label>
                                    <input type="text"
                                           class="form-control form-control-sm" date-picker ng-model="newTask.Taskdate_TMP" date-detail="newTask.TaskdateDet">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Time</label>
