
<style>
    @@font-face {
        font-family: 'Preeti';
        src: url('path-to-preeti-font/Preeti.ttf') format('truetype');
    }

    .nepali-input {
        font-family: 'Preeti', sans-serif;
        font-size: 18px;
    }

    .nepali-show {
        font-family: 'Preeti', sans-serif;
        font-size: 11px;
        font-weight: 600 !important;
    }

    .btn-add-file {
        padding: 25px 25px;
        border: 2px dotted green;
    }

    .fa-plus-circle {
        color: #28a745;
        font-size: 20px;
    }

    .dz-image img {
        width: 130px;
        height: 130px;
    }

    .note-editor {
        margin-bottom: 0px !important;
    }

    .modal {
        margin-top: 30px !important;
    }

    .modal-dialog {
        margin-bottom: 40px !important;
    }


    canvas {
        max-height: 200px !important;
        width: 100%
    }
</style>

<script>
    var entityStudentSummary = "@Convert.ToInt32(Dynamic.BusinessEntity.Global.RptFormsEntity.StudentSummaryList)";
    var entityStudentSummaryForSMS = @Convert.ToInt32(AcademicLib.BE.Global.ENTITIES.StudentReport);
    var entityStudentBirthdayForSMS = @Convert.ToInt32(AcademicLib.BE.Global.ENTITIES.STUDENT_BIRTHDAY);
</script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.2.2/Chart.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/qrcodejs/1.0.0/qrcode.min.js"></script>

<link rel="stylesheet" href="/wwwroot/dynamic/css/dropzone.css" />
<script src="/wwwroot/dynamic/js/dropzone.js"></script>
<script src="~/jslib/export/tableExport.js"></script>
<script src="~/jslib/export/jquery.base64.js"></script>
<script src="~/jslib/export/jspdf/libs/sprintf.js"></script>
<script src="~/jslib/export/jspdf/jspdf.js"></script>
<script src="~/jslib/export/jspdf/libs/base64.js"></script>

<script src="~/jslib/academic/reporting/StudentDetail.js?v=20251115"></script>
<div class="hold-transition  layout-navbar-fixed" ng-app="myApp" ng-controller="StudentDetailsController" ng-init="LoadData()">
    <section class="content">
        <div class="container-fluid">
            <section class="iframe-content-starts-here">
                <div>
                    <!--the buttons are clickable only when the students are selected through checkbox in the table-->
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowEmailDialog()" @*data-toggle="modal" data-target="#Emailmodal"*@><i class="fas fa-envelope mr-1"></i>Email</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="SendSMSToStudent()" @*data-toggle="modal" data-target="#Smsmodal"*@><i class="fas fa-sms mr-1"></i>SMS</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="SendNoticeToStudent()" @*data-toggle="modal" data-target="#Notificationmodal"*@><i class="fas fa-bell mr-1"></i>Notification</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowRemarksModal()"><i class="fas fa-file-signature mr-1"></i>Add Remarks</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowPTMModal()" @*data-toggle="modal" data-target="#PTMmodal"*@><i class="fas fa-user-friends mr-1"></i>Add PTM</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowIDCardModal()" @*data-toggle="modal" data-target="#StudentIdCard"*@><i class="fas fa-id-card-alt mr-1"></i>ID Card</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowAdmitCardModal()" @*data-toggle="modal" data-target="#AdmitCard"*@><i class="fas fa-id-card mr-1"></i>Admit Card</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowMarksheetModal()" @*data-toggle="modal" data-target="#Marksheetmodal"*@><i class="fas fa-file-excel mr-1"></i>Marksheet</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowStudentLeftModal()"><i class="fas fa-user-alt-slash mr-1"></i>Left</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowStdDocModal()"><i class="far fa-file-alt mr-1"></i>Document</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowCertificateModal()" @*data-toggle="modal" data-target="#certificatemodal"*@><i class="fas fa-certificate mr-1"></i>Certificate</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowStdLeaveEntryModal()"><i class="fab fa-wpforms mr-1"></i>Leave Entry</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowStdVisitorModal()"><i class="fas fa-user-edit mr-1"></i>Visitors Record</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" ng-click="ShowClassUpgradeModal()"><i class="fas fa-user-check mr-1"></i>Upgrade</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" data-toggle="modal" data-target="#Logmodal"><i class="fas fa-list-ul mr-1"></i>Show Log</button>
                    <!--Additional buttons to preview the templates-->
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" data-toggle="modal" data-target="#feereceipt"><i class="fas fa-list-ul mr-1"></i>Fee Receipt</button>
                    <button class="btn btn-success btn-outline-success btn-light border-green btn-sm mr-2 mb-2" data-toggle="modal" data-target="#billprint"><i class="fas fa-list-ul mr-1"></i>Bill Print</button>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group d-flex">
                            <label class="mt-1 mr-2">Class</label>
                            <select class="form-control form-control-sm select2" ng-model="newStudentSummaryList.ClassId" ng-change="GetStudentSummaryList()" style="width:100%">
                                <option ng-value="0">**All**</option>
                                <option ng-value="cl.id" ng-repeat="cl in ClassSection.ClassList">{{cl.text}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group d-flex">
                            <label class="mr-2 mt-1">Section</label>
                            <select class="form-control form-control-sm select2" data-placeholder="Select Section" ng-model="newStudentSummaryList.SectionId" ng-change="GetStudentSummaryList()" style="width:100%">
                                <option ng-value="se.SectionId" ng-repeat="se in ClassSection.SectionList |  filter: { ClassId: newStudentSummaryList.ClassId }:true">{{se.SectionName}}</option>
                            </select>
                        </div>
                    </div>
                </div>

                <header class="row">
                    <div class="col-md-8">
                        <div class="clearfix">
                            <form class="form-inline mb-2">
                                <div class="input-group input-group-sm mr-sm-2">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text cstm">Show</span>
                                    </div>
                                    <!-- <input type="text" class="form-control border-left-0" placeholder=""> -->
                                    <select class="form-control border-left-0 border-right-0 pr-0"
                                            ng-model="perPage.StudentDetails">
                                        <option ng-value="pp.value" ng-repeat="pp in perPageColl" ng-selected="pp.value==perPage.StudentDetails">{{pp.text}}</option>
                                    </select>
                                    <div class="input-group-append">
                                        <span class="input-group-text bg-white"><small>entries</small></span>
                                    </div>
                                </div>
                                <div class="input-group input-group-sm table-search-filed">
                                    <input class="form-control form-control-navbar"
                                           type="search"
                                           placeholder="Search"
                                           aria-label="Search" ng-model="searchData.StudentDetails" />
                                    <div class="input-group-append">
                                        <button class="btn btn-default border-left-0"
                                                type="submit">
                                            <i class="fas fa-search"></i>
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                    <!-- end col-md-8 -->
                    <div class="col-md-4">
                        <div class="form-inline float-right">
                            <div class="custom-control custom-checkbox float-right">
                                <input class="custom-control-input" type="checkbox"
                                       id="detailcheckbox" value="option1" ng-model="newStudentDetails.showdetail">
                                <label for="detailcheckbox"
                                       class="custom-control-label">
                                    View Detail
                                </label>
                            </div>
                            <div class="dropdown dnld-file ml-4"
                                 data-toggle="tooltip"
                                 data-placement="top"
                                 title="Download File">
                                <button class="btn btn-secondary btn-light btn-sm dropdown-toggle"
                                        type="button"
                                        id="dropdownMenuButton"
                                        data-toggle="dropdown"
                                        aria-haspopup="true"
                                        aria-expanded="false">
                                    <i class="fas fa-download"></i>
                                </button>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
                                    <span class="dropdown-item">Choose an option:</span>
                                    <a class="dropdown-item wait" export-excel table-id="'#studentdetails'" sheet-name="'studentdetailsList'"><img src="~/wwwroot/dynamic/images/flaticons/excel.png" class="img-fluid exports-icon" alt="Excel image"> Excel</a>
                                    <a class="dropdown-item wait" onClick="$('#studentdetails').tableExport({type:'csv',escape:'false'});"><img src="~/wwwroot/dynamic/images/flaticons/csv.png" class="img-fluid exports-icon" alt="Excel image"> CSV</a>
                                </div>
                            </div>

                            <a print-tbl table-id="'#studentdetails'" class="btn btn-secondary btn-light btn-sm ml-2 wait" title="Print">
                                <i class="fas fa-print"></i>
                            </a>
                        </div>
                    </div>
                </header>
                <!--Table nav menu Ending -->
                <div class="table-responsive pb-3">
                    <div class="table-scroll-x-y table2">
                        <table id="studentdetails" class="table  table-bordered th-fixed-top bg-white table-head-fixed">
                            <thead>
                                <tr>
                                    <th class="text-center" style="width: 30px;">
                                        <div class="custom-control custom-checkbox">
                                            <input class="custom-control-input" type="checkbox" id="chkCheckAllStudent" ng-model="newStudentDetails.CheckAll" ng-change="CheckAllStudent()">
                                            <label for="chkCheckAllStudent" class="custom-control-label"> </label>
                                        </div>
                                    </th>
                                    <th class="text-center" style="width: 75px;">Action</th>
                                    <th class="text-center" style="width: 80px;">Image</th>
                                    <th class="text-center" style="width: 200px;">Name</th>
                                    <th style="width: 120px;z-index:2">Class/Sec</th>
                                    <th style="width: 200px; z-index: 2">Address</th>
                                    <th style="width: 120px; z-index: 2">Username</th>
                                    <th style="width: 120px; z-index: 2">Password</th>
                                </tr>
                            </thead>
                            <tbody dir-paginate="st in AllStudentList | filter:searchData.StudentDetails | itemsPerPage: perPage.StudentDetails" current-page="currentPages.StudentDetails" pagination-id="StudentDetailsPagination">
                                <tr>
                                    <td class="text-center">
                                        <div class="custom-control custom-checkbox">
                                            <input class="custom-control-input" type="checkbox" id="chkSt{{st.StudentId}}" ng-model="st.IsSelected">
                                            <label for="chkSt{{st.StudentId}}" class="custom-control-label"> </label>
                                        </div>
                                    </td>
                                    <td class="text-center">
                                        <button class="btn btn-info btn-sm" ng-click="GetStudentById(st)"><i class="fas fa-edit"></i></button>
                                        <button class="btn btn-sm btn-success" ng-click="PrintStudentInfo(st)" data-toggle="modal" data-target="#StudentProfile"><i class="fas fa-user"></i></button>
                                    </td>
                                    <td class="text-center">
                                        <img ng-src="{{ (st.PhotoPath && st.PhotoPath.length>0 ? st.PhotoPath : DefaultPhoto)}}" ng-click="ShowPersonalImg(st)" style="max-height:50px;max-width:45px;" id="imgStudent1" />
                                        <label for="choose-file{{$index}}" class="p-1">
                                            <i class="fas fa-camera tablecamera text-info"></i>
                                        </label>
                                        <input id="choose-file{{$index}}" type="file" accept=".jpg,.jpeg,.pdf,doc,docx,application/msword,.png" style="display: none;" file-model ng-model="st.Photo_TMP" fileread="st.PhotoData" be-data="st" after-selected="UploadStudentPhoto(st)" />
                                    </td>
                                    <td class="cstm-sticky">
                                        <p class="mb-0"><b>{{st.Name}}</b></p>
                                        <label>Adm.No :{{st.RegNo}}</label><label class="ml-3">Roll No: <i>{{st.RollNo}}</i></label>
                                    </td>
                                    <td>{{st.ClassName}} {{st.SectionName}}</td>
                                    <td>{{st.Address}}</td>
                                    <td>{{st.UserName}}</td>
                                    <td>
                                        <div ng-if="st.Pwd">
                                            <span id="passwordText{{st.StudentId}}">{{"*******"}}</span>
                                            <input id="editinput{{st.StudentId}}" type="password" value="{{st.Pwd}}"
                                                   class="form-control form-control-sm" style="display:none;" disabled>
                                            <div class="d-flex float-right">
                                                <a class="p-1" id="covereye{{st.StudentId}}" ng-click="togglePasswordVisibility(st, true)">
                                                    <i class="fas fa-eye-slash text-secondary"></i>
                                                </a>
                                                <a class="p-1" id="openeye{{st.StudentId}}" ng-click="togglePasswordVisibility(st, false)" style="display:none;">
                                                    <i class="fas fa-eye text-secondary"></i>
                                                </a>
                                                <a class="p-1" id="editpassword{{st.StudentId}}" ng-click="enablePasswordEdit(st)">
                                                    <i class="fas fa-edit text-info"></i>
                                                </a>
                                                <a class="p-1" id="savepassword{{st.StudentId}}" ng-click="savePassword(st)" style="display:none;">
                                                    <i class="fas fa-save text-success"></i>
                                                </a>
                                            </div>
                                        </div>
                                    </td>


                                </tr>
                                <tr ng-show="newStudentDetails.showdetail == true">
                                    <td colspan="2"></td>
                                    <td colspan="6">
                                        <div class="row">
                                            <div class="col-md-2">
                                                <b>Gender :</b><br />{{st.Gender}}
                                            </div>
                                            <div class="col-md-3">
                                                <b>Father Name :</b><br /> {{st.FatherName}}
                                            </div>
                                            <div class="col-md-2">
                                                <b>Father Contact No :</b><br /> {{st.F_ContactNo}}
                                            </div>
                                            <div class="col-md-3">
                                                <b>Mother Name :</b><br /> {{st.MotherName}}
                                            </div>
                                            <div class="col-md-2">
                                                <b>Mother Contact No :</b><br /> {{st.M_ContactNo}}
                                            </div>
                                        </div>
                                    </td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- Pagination -->
                <footer class="row align-items-center">
                    <div class="col-md-12">
                        <dir-pagination-controls pagination-id="StudentDetailsPagination" boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)"></dir-pagination-controls>
                    </div>
                </footer>
            </section>
            <!--Data-->
            <!--Modal for Email Starts-->
            <div class="modal fade" id="Emailmodal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header py-2">
                            <h6 class="modal-title mb-0" id="exampleModalLabel">Send Email</h6>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body py-2">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-5">
                                        <div class="form-group d-flex">
                                            <label class="mr-2 mt-1" style="min-width:98px;">SMS Template: </label>
                                            <select class="form-control form-control-sm select2" style="width: 100%" ng-model="CurEmailSend.Template" ng-change="EmailTemplateChanged(CurEmailSend.Template)">
                                                <option ng-repeat="st in EmailTemplatesColl" ng-value="st">
                                                    {{st.Name}}
                                                </option>
                                            </select>
                                        </div>
                                    </div>


                                    <div class="col-7">
                                        <div class="form-group d-flex">
                                            <label class="mr-2 mt-1" style="min-width:90px;">Email Send To</label>
                                            <div class="checkbox-wrapper  d-flex">
                                                <div class="custom-control custom-checkbox mr-2">
                                                    <input class="custom-control-input" type="checkbox"
                                                           id="customCheckbox211" ng-model="CurEmailSend.Primary">
                                                    <label for="customCheckbox211"
                                                           class="custom-control-label">Primary No.</label>
                                                </div>

                                                <div class="custom-control custom-checkbox mr-2">
                                                    <input class="custom-control-input" type="checkbox"
                                                           id="customCheckbox311" ng-model="CurEmailSend.Father">
                                                    <label for="customCheckbox311"
                                                           class="custom-control-label">Father No.</label>
                                                </div>

                                                <div class="custom-control custom-checkbox mr-2">
                                                    <input class="custom-control-input" type="checkbox"
                                                           id="customCheckbox411" ng-model="CurEmailSend.Mother">
                                                    <label for="customCheckbox411"
                                                           class="custom-control-label">Mother No.</label>
                                                </div>
                                                <div class="custom-control custom-checkbox mr-2">
                                                    <input class="custom-control-input" type="checkbox"
                                                           id="customCheckbox511" ng-model="CurEmailSend.Guardian">
                                                    <label for="customCheckbox511"
                                                           class="custom-control-label">Mother No.</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-12">
                                    <div class="form-group d-flex">
                                        <label class="mr-2 mt-1" style="min-width:90px;">CC Employee </label>
                                        <select class="form-control form-control-sm select2" multiple ng-model="CurEmailSend.EmployeeColl" style="width:99%">
                                            <option ng-value="st" ng-repeat="st in AllEmployeeColl">{{st.Name}}-{{st.EmployeeCode}}</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="col-12">
                                    <div class="form-group d-flex">
                                        <label class="mr-2 mt-1" style="min-width:90px;">Subject <span class="text-danger">*</span></label>
                                        <input type="text" class="form-control form-control-sm" ng-model="CurEmailSend.Subject" />
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="form-group mb-0">
                                        <label for="" class="mr-2">Message <span class="text-danger">*</span> </label>
                                        <textarea class="textarea" placeholder="Place some text here" style="
                                                                          width: 100%;
                                                                          height: 130px!important;
                                                                          font-size: 14px;
                                                                          line-height: 18px;
                                                                          border: 1px solid #dddddd;
                                                                          padding: 10px;
                                                                        " ng-model="CurEmailSend.Description" html-editor></textarea>
                                    </div>
                                </div>

                                <div class="col-12">
                                    <div class="card-body mt-0" id="add-file-area" style="background-color: #eff0ef;">
                                        <label><b>Attach File</b></label>
                                        <div id="actions" class="row">
                                            <div class="col-md-1 mt-4">
                                                <div class="btn-group">
                                                    <span class="btn-add-file col fileinput-button">
                                                        <span><i class="fas fa-plus-circle"></i></span>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="col-md-11">
                                                <div class="d-flex p-2" id="previews" style="overflow-x: auto;">
                                                    <div id="template">
                                                        <a data-dz-remove class="cancel float-right">
                                                            <i class="fas fa-times-circle" style="  color:#28a745;font-size: 14px;"></i>
                                                        </a>
                                                        <span class="preview p-2 ml-2"><img style="height:150px;width:auto" src="data:," alt="" data-dz-thumbnail /></span>
                                                        <div class="align-items-center" style="width: 100%;">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-12 mt-2">
                                    <div class="form-group">
                                        <label>Total numbers of selected student: {{CurEmailSend.DataColl.length}}</label>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-sm  btn-default" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-sm btn-success" ng-click="SendEmail()">Send</button>
                        </div>
                    </div>
                </div>
            </div>

            <!--Modal for sms Starts-->
            <div class="modal fade" id="Smsmodal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-header py-2">
                            <h6 class="modal-title mb-0" id="exampleModalLabel">Send SMS</h6>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body py-2">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="from-group">
                                        <label>Communication Type</label>
                                        <select class="form-control form-control-sm" ng-model="newSMS.CommunicationType">
                                            <option value="">Email</option>
                                            <option value="">SMS</option>
                                            <option value="others">Others</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-8">
                                    <div class="from-group">
                                        <label>Send To Employee</label>
                                        <select class="form-control form-control-sm select2" multiple>
                                            <option value="">Employee1</option>
                                            <option value="">Employee2</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group mt-2">
                                        <label>Point of Contact</label>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="custom-control custom-checkbox">
                                                    <input class="custom-control-input" type="checkbox" id="primaryemail">
                                                    <label for="primaryemail" class="custom-control-label">
                                                        Primary Number
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="custom-control custom-checkbox">
                                                    <input class="custom-control-input" type="checkbox" id="fatheremail">
                                                    <label for="fatheremail" class="custom-control-label">
                                                        Father Number
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="custom-control custom-checkbox">
                                                    <input class="custom-control-input" type="checkbox" id="motheremail">
                                                    <label for="motheremail" class="custom-control-label">
                                                        Mother Number
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="custom-control custom-checkbox">
                                                    <input class="custom-control-input" type="checkbox" id="guardianemail">
                                                    <label for="guardianemail" class="custom-control-label">
                                                        Guardian Number
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
