        color: #ffffff;
        background-color: #025a58;
    }

    .nav-pills .examnav {
        color: #6c757d;
        background-color: #76be4e;
        color: #fff;
    }

    .main-content-cards .card a:hover {
        color: #2d3333 !important;
    }

