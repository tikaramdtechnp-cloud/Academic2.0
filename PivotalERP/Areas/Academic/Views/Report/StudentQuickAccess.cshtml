        color: #ffffff;
        background-color: #035b59;
    }

    .nav-pills .nav-link:not(.active):hover {
        color: #025a58;
    }

    .triangle-up {
        width: 0;
        height: 0;
        border-left: 8px solid transparent;
        border-right: 8px solid transparent;
        border-bottom: 15px solid #76be4e;
    }

    .table-scroll-x-y table thead th {
        border: 1px solid #c1dacf !important;
    }

    .due_det .active {
        background-color: #E9F2EE;
        clip-path: polygon(0% 0%, 91% 0, 100% 50%, 91% 100%, 0% 100%);
    }

    .due_det div {
        padding: 0.8rem 0.75rem;
    }

    .absent {
        color: #FF6347; /* Optional: To make the text color contrast well with the red background */
    }
