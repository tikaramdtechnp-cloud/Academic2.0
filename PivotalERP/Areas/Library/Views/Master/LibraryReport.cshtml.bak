                                                    " ng-model="newStudent.SelectEmployee">

                                                    <option class="dropdown-item" ng-repeat="ss in EmployeeSearchOptions" ng-value="ss.value" ng-selected="ss.value==newStudent.SelectEmployee">
                                                        {{ss.text}}
                                                    </option>

                                                </select>

                                                <select class="form-control select2 form-control-sm" all-employee ng-model="newStudent.EmployeeId" employee-detail="newStudent.EmployeeDetails" side-bar-data="newStudent.EmpSideBarData" search-by="newStudent.SelectEmployee" style="width: 100%;" confirm-action="GetEmpBookRegister()">
                                                </select>

                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-lg-6 col-md-6 mt-4" ng-show="newStudent.IssueTo==1" id="selectStu">

                                            <label class="sr-only"
                                                   for="inlineFormInputGroup">Select Student</label>
