                                <div id="student-book-section">
                                    <div class="row">

                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <label>Issue To</label>
                                                <select class="form-control form-control-sm" ng-model="newStudent.IssueTo">
                                                    <option ng-value="it.id" ng-repeat="it in IssueToList" ng-selected="it.id==newStudent.IssueTo">{{it.text}}</option>
                                                </select>
                                            </div>
                                        </div>


                                        <div class="col-sm-6 col-lg-6 col-md-6 mt-4" ng-show="newStudent.IssueTo==2" id="selectEmp">
