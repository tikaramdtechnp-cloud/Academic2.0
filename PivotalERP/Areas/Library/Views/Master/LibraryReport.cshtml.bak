                            <div class="tab-pane fade show active main-section" id="home" role="tabpanel"
                                 aria-labelledby="home-tab">
                                <div id="booktitle-section">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <span for="">Material Type</span>
                                                <select class="form-control form-control-sm select2" ng-model="bookList.MaterialTypeId">
                                                    <option ng-value="0" selected>**All**</option>
                                                    <option ng-value="mt.MaterialTypeId" ng-repeat="mt in MaterialTypeList">{{mt.Name}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-8 mt-4">
                                            <!-- <div class="select-button"> -->
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input"
                                                       type="radio"
                                                       name="inlineRadioOptions"
                                                       id="inlineRadio1"
                                                       ng-value="1" ng-model="bookList.ForType" />
                                                <label class="form-check-label" for="inlineRadio1">Available</label>
                                            </div>
                                            <div class="form-check form-check-inline">
                                                <input class="form-check-input"
                                                       type="radio"
                                                       name="inlineRadioOptions"
                                                       id="inlineRadio3"
                                                       ng-value="2" ng-model="bookList.ForType" />
