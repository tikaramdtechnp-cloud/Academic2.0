                                                    <option ng-repeat="ss in BookSearchOptions" ng-value="ss.value" ng-selected="ss.value==bookRegister.SelectBook">
                                                        {{ss.text}}
                                                    </option>

                                                </select>

                                                <select class="form-control select2 form-control-sm" all-book ng-model="bookRegister.BookEntryId" book-detail="bookRegister.BookDetails" side-bar-data="bookRegister.SideBarData" search-by="bookRegister.SelectBook" for-report="true" confirm-action="GetBookRegister()" showdetails="true">
                                                </select>
                                            </div>
